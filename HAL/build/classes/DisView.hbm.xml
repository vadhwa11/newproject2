<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="DisView" table="dis_view">
		<meta attribute="sync-DAO">false</meta>

		<property name="DepartmentName" column="department_name"
			type="string" not-null="false" length="30" />
		<property name="Count(*)" column="count(*)" type="java.lang.Long"
			not-null="false" length="19" />
		<property name="ServiceTypeName" column="service_type_name"
			type="string" not-null="false" length="30" />
		<property name="DateOfDischarge" column="date_of_discharge"
			type="date" not-null="false" />


	</class>
</hibernate-mapping>