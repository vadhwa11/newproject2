<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="BedStatus" table="bed_status">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="SR_NO">
			<generator class="native" />
		</id>

		<property name="Remd" column="REMD" type="integer" not-null="true"
			length="10" />
		<property name="Adm" column="ADM" type="integer" not-null="true"
			length="10" />
		<property name="Dis" column="DIS" type="integer" not-null="false"
			length="10" />
		<property name="Death" column="DEATH" type="integer"
			not-null="false" length="10" />
		<property name="OfficerAf" column="OFFICER_AF" type="integer"
			not-null="false" length="10" />
		<property name="OfficerArmy" column="OFFICER_ARMY" type="integer"
			not-null="false" length="10" />
		<property name="OfficerNavy" column="OFFICER_NAVY" type="integer"
			not-null="false" length="10" />
		<property name="OfffamilyAf" column="OFFFAMILY_AF" type="integer"
			not-null="false" length="10" />
		<property name="OfffamilyArmy" column="OFFFAMILY_ARMY" type="integer"
			not-null="false" length="10" />
		<property name="OfffamilyNavy" column="OFFFAMILY_NAVY" type="integer"
			not-null="false" length="10" />
		<property name="OrsAf" column="ORS_AF" type="integer"
			not-null="false" length="10" />
		<property name="OrsArmy" column="ORS_ARMY" type="integer"
			not-null="false" length="10" />
		<property name="OrsNavy" column="ORS_NAVY" type="integer"
			not-null="false" length="10" />
		<property name="OrsfAf" column="ORSF_AF" type="integer"
			not-null="false" length="10" />
		<property name="OrsfArmy" column="ORSF_ARMY" type="integer"
			not-null="false" length="10" />
		<property name="OrsfNavy" column="ORSF_NAVY" type="integer"
			not-null="false" length="10" />


	</class>
</hibernate-mapping>