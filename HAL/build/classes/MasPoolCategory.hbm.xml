<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasPoolCategory" table="mas_pool_category">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="pool_category_id">
			<generator class="native" />
		</id>

		<property name="PoolCategoryCode" column="pool_category_code"
			type="string" not-null="false" length="8" />
		<property name="PoolCategoryName" column="pool_category_name"
			type="string" not-null="false" length="30" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />

		<set name="MasPools" inverse="true">
			<key column="pool_category_id" />
			<one-to-many class="MasPool" />
		</set>
		<set name="MasSmqs" inverse="true">
			<key column="pool_category_id" />
			<one-to-many class="MasSmq" />
		</set>
	</class>
</hibernate-mapping>