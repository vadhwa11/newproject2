<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MisConfirmedH1n1"
		table="MIS_CONFIRMED_H1N1"
	>
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="H1N1_ID">
			<generator class="increment" />
		</id>
		<property
			name="ServiceNo"
			column="SERVICE_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="MedFacilityFirst"
			column="MED_FACILITY_FIRST"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="MedUnitFirstAdmitted"
			column="MED_UNIT_FIRST_ADMITTED"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DateOfOnset"
			column="DATE_OF_ONSET"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="DateOfAdmission"
			column="DATE_OF_ADMISSION"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="ContactWithCase"
			column="CONTACT_WITH_CASE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Travell"
			column="TRAVELL"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ClinicalFeatures"
			column="CLINICAL_FEATURES"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Respiratory"
			column="RESPIRATORY"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DateSampleSent"
			column="DATE_SAMPLE_SENT"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="TestingLaboratory"
			column="TESTING_LABORATORY"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DateOfReport"
			column="DATE_OF_REPORT"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="Result"
			column="RESULT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Treatment"
			column="TREATMENT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="PresentCondition"
			column="PRESENT_CONDITION"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgBy"
			column="LAST_CHG_BY"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="LastChgDate"
			column="LAST_CHG_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LastChgTime"
			column="LAST_CHG_TIME"
			type="string"
			not-null="false"
			length="10"
		/>

<many-to-one name="Hospital" column="HOSPITAL_ID" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hin" column="HIN_ID" class="Patient"
			not-null="false">
		</many-to-one>
	</class>	
</hibernate-mapping>