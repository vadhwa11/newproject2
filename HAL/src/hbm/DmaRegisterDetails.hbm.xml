<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="DmaRegisterDetails"
		table="DMA_REGISTER_DETAILS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="DMA_REGISTER_DETAILS_ID"
		>
			<generator class="native"></generator></id>
		
		<property
			name="Dosage"
			column="DOSAGE"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="IssueQty"
			column="ISSUE_QTY"
			type="integer"
			not-null="false"
		/>
		<property
			name="Remarks"
			column="REMARKS"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="Route"
			column="route"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="NoOfDays"
			column="no_of_days"
			type="integer"
			not-null="false"
		/>
		<many-to-one
			name="Item"
			column="ITEM_ID"
			class="MasStoreItem"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="DmaRegisterHeader"
			column="DMA_REGISTER_HEADER_ID"
			class="DmaRegisterHeader"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Stock"
			column="STOCK_ID"
			class="StoreItemBatchStock"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Frequency"
			column="FREQUENCY_ID"
			class="MasFrequency"
			not-null="false"
		>
		</many-to-one>
	</class>	
</hibernate-mapping>