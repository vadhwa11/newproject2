<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="PhysioVisitEntryHeader" table="PHYSIO_VISIT_ENTRY_HEADER">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="VISIT_ENTRY_HEADER_ID">
	       <generator class="native"></generator>
		</id>

		<property
			name="LastChgTime"
			column="LAST_CHG_TIME"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="LastChgDate"
			column="LAST_CHG_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="PhysioVisitDate"
			column="physio_visit_date"
			type="date"
			not-null="false"
			length="7"
		/>
		
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="PhyStatus"
			column="phy_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Diagnosis"
			column="diagnosis"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="VisitStatus"
			column="visit_status"
			type="string"
			not-null="false"
			length="2"
		/>
		
		<property
			name="PatientType"
			column="PATIENT_TYPE"
			type="string"
			not-null="false"
			length="3"
		/>
		
		<many-to-one name="Hin" column="hinId" class="Patient"
			not-null="false">
		</many-to-one>
		<many-to-one name="LastChgBy" column="last_chg_by"
			class="Users" not-null="false">
		</many-to-one>
		<many-to-one name="Hospital" column="hospital_id"
			class="MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Inpatient" column="inpatient_id"
			class="Inpatient" not-null="false">
		</many-to-one>
		
		<many-to-one name="MedicalOfficer" column="medical_officer_id"
			class="MasEmployee" not-null="false">
		</many-to-one>
		<many-to-one name="Visit" column="visitId"
			class="Visit" not-null="false">
		</many-to-one>
		<many-to-one name="PhysioRequisition" column="PHYSIO_REQUISITION_ID"
			class="PhysioRequisitionHeader" not-null="false">
		</many-to-one>
		<set inverse="true" name="PhysioVisitEntryDetails">
			<key column="physio_requisition_header_id" />
			<one-to-many class="PhysioVisitEntryDetail" />
		</set>

	</class>	
</hibernate-mapping>