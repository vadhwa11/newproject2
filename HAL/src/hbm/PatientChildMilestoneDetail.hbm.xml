<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PatientChildMilestoneDetail"
		table="PATIENT_CHILD_MILESTONE_DETAIL"
	>
		<meta attribute="sync-DAO">false</meta>
		
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="CHILD_MILESTONE_ID">
			<generator class="native"></generator>
			
		</id>
		
		
		<property
			name="NormalAgeRange"
			column="NORMAL_AGE_RANGE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Remarks"
			column="REMARKS"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="Result"
			column="result"
			type="string"
			not-null="false"
			length="10"
		/>
		
		<property
			name="LastChgDate"
			column="LAST_CHG_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LastChgTime"
			column="LAST_CHG_TIME"
			type="string"
			not-null="false"
			length="5"
		/>
		
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		
		<many-to-one
			name="Hin"
			column="HIN_ID"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="HOSPITAL_ID"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="LAST_CHG_BY"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			 name="Milestone"
		 	column="MILESTONE_ID" 
		 	class="MasChildMilestone"
			not-null="false">
		</many-to-one>
	<many-to-one
			 name="Visit"
		 	column="visit_id" 
		 	class="Visit"
			not-null="false">
		</many-to-one>
		<many-to-one
		 name="OpdPatientDetail"
		 column="OPD_PATIENT_DETAIL_ID"
	     class="OpdPatientDetails"
		 not-null="false">
		</many-to-one>

	</class>	
</hibernate-mapping>