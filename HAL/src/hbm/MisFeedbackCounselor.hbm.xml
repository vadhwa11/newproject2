<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MisFeedbackCounselor"
		table="MIS_FEEDBACK_COUNSELOR"
	>
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="ID">
			<!-- <generator class="native" /> -->
			<generator class="native"></generator>
		</id>
		<property
			name="CounselorName"
			column="COUNSELOR_NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Qualification"
			column="QUALIFICATION"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="EmployedDate"
			column="EMPLOYED_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="Honorarium"
			column="HONORARIUM"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="VisitsPerWeek"
			column="VISITS_PER_WEEK"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="WorkHours"
			column="WORK_HOURS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="CounselingPlace"
			column="COUNSELING_PLACE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="CounseledPersons"
			column="COUNSELED_PERSONS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="CounseledCasesType"
			column="COUNSELED_CASES_TYPE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="NoOfLecture"
			column="NO_OF_LECTURE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="CounAfterWorkingHrs"
			column="COUN_AFTER_WORKING_HRS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="AlcoholDependence"
			column="ALCOHOL_DEPENDENCE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="RecordCases"
			column="RECORD_CASES"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="AdministrativeAction"
			column="ADMINISTRATIVE_ACTION"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="EmploymentOfCounselor"
			column="EMPLOYMENT_OF_COUNSELOR"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ProblemsFaced"
			column="PROBLEMS_FACED"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="AnalysisofPsyCases"
			column="ANALYSISOF_PSY_CASES"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgBy"
			column="LAST_CHG_BY"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="LastChgDate"
			column="LAST_CHG_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LastChgTime"
			column="LAST_CHG_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
	<many-to-one name="Hospital" column="HOSPITAL_ID" class="MasHospital"
			not-null="false">
		</many-to-one>

	</class>	
</hibernate-mapping>