<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MdBillMovement" table="md_bill_movement">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="native"></generator>
		</id>

		<property name="FwtDate" column="fwt_date" type="date"
			not-null="false" />
		<property name="DocumnetNo" column="documnet_no" type="string"
			not-null="false" length="50" />
		<property name="BillStatus" column="bill_status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="ContingentHd" column="contingent_hd_id"
			class="MdCardicContingentBillHd" not-null="false">
		</many-to-one>
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
		<many-to-one name="FwtTo" column="fwt_to" class="MdMasAuthority"
			not-null="false">
		</many-to-one>
		<many-to-one name="Inpatient" column="inpatient_id" class="Inpatient"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hin" column="hin_id" class="Patient"
			not-null="false">
		</many-to-one>
		<many-to-one name="ContingentDt" column="contingent_dt_id"
			class="MdCardicContingentBillDt" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>