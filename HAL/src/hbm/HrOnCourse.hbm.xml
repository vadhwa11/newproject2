<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="HrOnCourse" table="hr_on_course">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="native"></generator>
		</id>

		<property name="CourseName" column="course_name" type="string"
			not-null="false" length="45" />
		<property name="FromDate" column="from_date" type="date"
			not-null="false" />
		<property name="ToDate" column="to_date" type="date"
			not-null="false" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="30" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="15" />
		<many-to-one name="Unit" column="unit_id" class="MasUnit"
			not-null="false">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id" class="MasEmployee"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>