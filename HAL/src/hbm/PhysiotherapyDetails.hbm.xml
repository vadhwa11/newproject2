<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="PhysiotherapyDetails" table="PHYSIOTHERAPY_DETAILS" >
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="PHYSIOTHERAPY_DETAILS_ID" >
			<generator class="native"></generator>
		</id>

		<property
			name="Diagnosis"
			column="DIAGNOSIS"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="SittingTime"
			column="SITTING_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Treatment"
			column="TREATMENT"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Remarks"
			column="REMARKS"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="LastChgDate"
			column="LAST_CHG_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LastChgTime"
			column="LAST_CHG_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		
		<property
			name="Duration"
			column="duration"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="NoOfDays"
			column="no_of_days"
			type="integer"
			not-null="false"
		/>
		<property
			name="PhyVisitDate"
			column="PHY_VISIT_DATE"
			type="date"
			not-null="false"
		/>
		<property
			name="PhyVisitNo"
			column="phy_visit_no"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TimeBegin"
			column="time_begin"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="TimeComplete"
			column="time_complete"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="NextAppointmentDate"
			column="next_appointment_date"
			type="date"
			not-null="false"
		/>
		<property
			name="NextAppointmentTime"
			column="next_appointment_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="Visit"
			column="VISIT_ID"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="HIN_ID"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="HOSPITAL_ID"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="MedicalOfficer"
			column="MEDICAL_OFFICER"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="TherapyType"
			column="THERAPY_TYPE_ID"
			class="MasTherapyType"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="LAST_CHG_BY"
			class="Users"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>