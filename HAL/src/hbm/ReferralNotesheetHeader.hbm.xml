<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="ReferralNotesheetHeader"
		table="referral_notesheet_header"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="NoteSheetNo"
			column="note_sheet_no"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="15"
		/>
		<many-to-one
			name="ReferralHeader"
			column="referral_header_id"
			class="ReferralPatientHeader"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>