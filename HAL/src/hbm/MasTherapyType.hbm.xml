<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasTherapyType"
		table="MAS_THERAPY_TYPE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="THERAPY_TYPE_ID"
		>
			<generator class="native"></generator>
		</id>

		<property
			name="TherapyTypeCode"
			column="THERAPY_TYPE_CODE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="TherapyTypeName"
			column="THERAPY_TYPE_NAME"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="LAST_CHG_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LastChgTime"
			column="LAST_CHG_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one name="LastChgBy" column="last_chg_by"
			class="Users" not-null="false">
		</many-to-one>

	</class>	
</hibernate-mapping>