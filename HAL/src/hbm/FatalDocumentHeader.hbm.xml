<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="FatalDocumentHeader"
		table="FATAL_DOCUMENT_HEADER"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			column="ID"
			type="integer"
			
		>
		<generator class="native"></generator></id>
		<property name="ServiceNo" column="service_no" type="string"
			not-null="false" length="20" />
		<property
			name="AdNo"
			column="AD_NO"
			type="string"
			not-null="false"
			length="40"
		/>
		
		<property
			name="SPersonName"
			column="S_PERSON_NAME"
			type="string"
			not-null="false"
			length="100"
		/>
		
		
		<property
			name="DateOfBirth"
			column="DATE_OF_BIRTH"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="Age"
			column="AGE"
			type="string"
			not-null="false"
			length="50"
		/>
		
		<property
			name="DateOfComm"
			column="DATE_OF_COMM"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="TotalService"
			column="TOTAL_SERVICE"
			type="string"
			not-null="false"
			length="40"
		/>
		
		<property
			name="PrevMedHistory"
			column="PREV_MED_HISTORY"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="Diagnosis"
			column="DIAGNOSIS"
			type="string"
			not-null="false"
			length="400"
		/>
		<property
			name="HospitalName"
			column="HOSPITAL_NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DateOfAdmission"
			column="DATE_OF_ADMISSION"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="TimeOfAdmission"
			column="TIME_OF_ADMISSION"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="TypeOdDeath"
			column="TYPE_OD_DEATH"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DateOfDeath"
			column="DATE_OF_DEATH"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="TimeOfDeath"
			column="TIME_OF_DEATH"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="LocationOfDeath"
			column="LOCATION_OF_DEATH"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="ConditionToDeath"
			column="condition_to_death"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="DueConsequence"
			column="due_consequence"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="OtherCondition"
			column="other_condition"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="MlcNo"
			column="MLC_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		
<many-to-one name="HinId" column="HIN_ID"
			class="Patient" not-null="false">
		</many-to-one>
		<many-to-one name="RankId" column="RANK_ID"
			class="MasRank" not-null="false">
		</many-to-one>
		<many-to-one name="UnitId" column="UNIT_ID"
			class="MasUnit" not-null="false">
		</many-to-one>
		<many-to-one name="TradeId" column="TRADE_ID"
			class="MasTrade" not-null="false">
		</many-to-one>
		
		<many-to-one name="SexId" column="SEX_ID"
			class="MasAdministrativeSex" not-null="false">
		</many-to-one>
		<many-to-one name="MedicalCategoryId" column="MEDICAL_CATEGORY_ID"
			class="MasMedicalCategory" not-null="false">
		</many-to-one>

	</class>	
</hibernate-mapping>