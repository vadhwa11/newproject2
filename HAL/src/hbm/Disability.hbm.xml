<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="Disability"
		table="DISABILITY"
	>

        <id name="Disabilityid" type="integer" column="DISABILITYID">
			 <generator class="native"></generator></id>
		
		<property
			name="ServiceNumber"
			column="SERVICE_NUMBER"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Date"
			column="DATE_"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="Disability"
			column="DISABILITY"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Category"
			column="CATEGORY"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="PermTemp"
			column="PERM__TEMP"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Periods"
			column="PERIODS"
			type="integer"
			not-null="false"
			length="22"
		/>
		


	</class>	
</hibernate-mapping>