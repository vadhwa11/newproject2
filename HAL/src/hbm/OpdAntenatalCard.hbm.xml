		<property name="Smoking" column="smoking" type="string"
			not-null="false" length="3" />
		<property name="Build" column="build" type="string" not-null="false"
			length="10" />
		<property name="Nutrition" column="nutrition" type="string"
			not-null="false" length="10" />
		<property name="Height" column="height" type="string"
			not-null="false" length="10" />
		<property name="Weight" column="weight" type="string"
			not-null="false" length="6" />
		<property name="Breast" column="breast" type="string"
			not-null="false" length="5" />
		<property name="Nipple" column="nipple" type="string"
