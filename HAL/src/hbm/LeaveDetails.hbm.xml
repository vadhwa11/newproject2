<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="HrLeaveDetails"
		table="hr_leave_details"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="FromDate"
			column="from_date"
			type="date"
			not-null="false"
		/>
		<property
			name="ToDate"
			column="to_date"
			type="date"
			not-null="false"
		/>
		<property
			name="NoOfWorkingDays"
			column="no_of_working_days"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="ApprovedOn"
			column="approved_on"
			type="date"
			not-null="false"
		/>
		<property
			name="JoiningDate"
			column="joining_date"
			type="date"
			not-null="false"
		/>
		<property
			name="Reason"
			column="reason"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="ContactAddress"
			column="contact_address"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ContactPhone"
			column="contact_phone"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="AppliedOn"
			column="applied_on"
			type="date"
			not-null="false"
		/>
		<property
			name="Suggestion"
			column="suggestion"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="ModifiedBy"
			column="modified_by"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ModifiedOn"
			column="modified_on"
			type="date"
			not-null="false"
		/>
		<property
			name="DisapproveReason"
			column="disapprove_Reason"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="AlternateEmailId"
			column="alternate_email_Id"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="HalfDay"
			column="half_day"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="ShortLeaveHalfDay"
			column="short_leave_half_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="DeleteReason"
			column="delete_reason"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="LastChgBy"
			column="last_chg_by"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
		/>
		<property
			name="RecStatus"
			column="rec_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="leaveApprovedBy"
			column="approved_by"
			class="jkt.hms.masters.business.MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="leaveType"
			column="leave_type"
			class="HrMasLeaveTypeMediator"
			not-null="false"
			cascade="all"
		>
		</many-to-one>
		<many-to-one
			name="empIdBal"
			column="balance_id"
			class="HrEmployeeBalanceNew"
			not-null="false"
			lazy="false"
		>
		</many-to-one>
		<many-to-one
			name="leaveStatus"
			column="status"
			class="HrMasLeaveStatus"
			not-null="false"
			lazy="false"
			
		>
		</many-to-one>
		<many-to-one
			name="EmpId"
			column="empid"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Company"
			column="company_id"
			class="MasHospital"
			not-null="false"
		> 
		</many-to-one>
		
		<many-to-one
			name="HrMasLeaveTypeNew"
			column="hr_mas_leave_type_new_id"
			class="HrMasLeaveTypeNew"
			not-null="false"
		>
		</many-to-one>
		
	


	</class>	
</hibernate-mapping>