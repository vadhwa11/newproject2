<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="BlOpBillHeader"
		table="bl_op_bill_header"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="op_bill_header_id"
		>
			<generator class="native"/>
		</id>

		<property
			name="PatientName"
			column="patient_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Age"
			column="age"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="HinNo"
			column="hin_no"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ConsultantName"
			column="consultant_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="BillNo"
			column="bill_no"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="BillAmt"
			column="bill_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="DiscountAmt"
			column="discount_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="Outstanding"
			column="outstanding"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="RoundOff"
			column="round_off"
			type="big_decimal"
			not-null="false"
			length="9"
		/>
		<property
			name="DiscountOnBill"
			column="discount_on_bill"
			type="big_decimal"
			not-null="false"
			length="9"
		/>
		<property
			name="NetAmt"
			column="net_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="BillDate"
			column="bill_date"
			type="date"
			not-null="true"
			length="10"
		/>
		<property
			name="BillTime"
			column="bill_time"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="PatientStatus"
			column="patient_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="AdvanceAdjustment"
			column="advance_adjustment"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="PayableAmt"
			column="payable_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="VisitId"
			column="visit_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="PackageBillId"
			column="package_bill_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ActualCollectedAmt"
			column="actual_collected_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="Discount"
			column="discount"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="Charity"
			column="charity"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="TokenNo"
			column="token_no"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RelativeName"
			column="relative_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="RegType"
			column="reg_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="SchemeId"
			column="scheme_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="PayStatus"
			column="pay_status"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="CharityRcvd"
			column="charity_rcvd"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="100"
		/>
		<many-to-one
			name="Sex"
			column="sex_id"
			class="MasAdministrativeSex"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Authorizer"
			column="authorizer_id"
			class="MasAuthorizer"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="PatientType"
			column="patient_type_id"
			class="MasPatientType"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Company"
			column="company_id"
			class="MasCompany"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="ChangedBy"
			column="changed_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Consultant"
			column="consultant_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="SchemeAuthorizer"
			column="scheme_authorizer_id"
			class="MasAuthorizer"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="true"
		>
		</many-to-one>
	

		<set name="BlOpBillDetails" inverse="true">
			<key column="op_bill_header_id"/>
			<one-to-many class="BlOpBillDetails"/>
		</set>


	</class>	
</hibernate-mapping>