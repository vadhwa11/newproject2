<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasInstruction"
		table="mas_instruction"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="OpInsCode"
			column="op_ins_code"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="OpInsName"
			column="op_ins_name"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
			<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
	

		<set name="MasOpInstructions" inverse="true">
			<key column="id"/>
			<one-to-many class="MasOpInstruction"/>
		</set>


	</class>	
</hibernate-mapping>