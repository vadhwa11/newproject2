	<queryString><![CDATA[SELECT
     concat(mas_title.`title_name`," ",mas_employee.`first_name`," ",mas_employee.`middle_name`," ",mas_employee.`last_name`) AS employee_name,
     mas_employee.`service_no` AS service_no,
     posted_out_entry.`sors` AS sors,
     posted_out_entry.`date` AS postedDate,
     posted_out_entry.`authority` AS authority,	
     posted_out_entry.`por_SlNo` AS porslno,	
      posted_out_entry.`appraisal_report` as appraisal ,
     mas_rank.`rank_name` AS rank_name,
     mas_department.`department_name` AS department_name,
     mas_employee.`vide` AS vide,
     mas_unit.`unit_name` AS unit_name,
     mas_trade.`trade_name` as trade_name	
FROM
    `mas_employee` mas_employee Left join `posted_out_entry` posted_out_entry on  mas_employee.`employee_id`=posted_out_entry.`employee_id`
     LEFT JOIN `mas_rank` mas_rank  ON mas_rank.`rank_id` = mas_employee.`rank_id`
     Left JOIN `mas_title` mas_title ON mas_employee.`title_id` = mas_title.`title_id`
     Left JOIN `mas_department` mas_department ON mas_employee.`department_id` = mas_department.`department_id`
     Left JOIN `mas_unit` mas_unit ON posted_out_entry.`unit_posted_to` = mas_unit.`unit_id`
     Left join `mas_trade` mas_trade on mas_employee.`trade_id`=mas_trade.`trade_id`	
WHERE
    posted_out_entry.employee_id = $P{employee_id}]]></queryString>

