	<queryString><![CDATA[select

(case
when extract(month from date_of_accident)=1 then 'Forth Quater'
when extract(month from date_of_accident)=2 then 'Forth Quater'
when extract(month from date_of_accident)=3 then 'Forth Quater'

when extract(month from date_of_accident)=4 then 'First Quater'
when extract(month from date_of_accident)=5 then 'First Quater'
when extract(month from date_of_accident)=6 then 'First Quater'

when extract(month from date_of_accident)=7 then 'Second Quater'
when extract(month from date_of_accident)=8 then 'Second Quater'
when extract(month from date_of_accident)=9 then 'Second Quater'

when extract(month from date_of_accident)=10 then 'Third Quater'
when extract(month from date_of_accident)=11 then 'Third Quater'
when extract(month from date_of_accident)=12 then 'Third Quater'

end) as quater,

date_of_accident, type_of_accident, type_of_vehical, cause_of_accident, place_of_accident, 
time_of_accident, approx_time_of_accident, head_inj_driver, head_inj_pillion_rider,
fractures_driver, fractures_pillion_rider, minor_inj_driver, minor_inj_pillion_rider,
crash_helmet_driver, crash_helmet_pillion_rider, chin_strap_driver, chin_strap_pillion_rider,
crash_helmet_driver,
(pd.p_first_name|| ' ' ||pd.p_last_name) as name, r.rank_name, pd.service_no, t.trade_name,
pd.age, pd.service_years, u.unit_name,
(pr.p_first_name|| ' ' ||pr.p_last_name) as RiderName, rr.rank_name as riderRank, pr.service_no as riderService, tt.trade_name as riderTrade,
pr.age as riderAge, pr.service_years as riderSerYear, uu.unit_name as riderUnit, a.CRASH_HELMET,
a.other_cause_of_accident

from accidental_details a
left outer join patient pd on a.driver_hin_id=pd.hin_id
left outer join patient pr on a.PILLOR_RIDDER_HIN_ID=pr.hin_id
left outer join mas_rank r on pd.rank_id=r.rank_id
left outer join mas_trade t on pd.trade_id=t.trade_id
left outer join mas_unit u on pd.unit_id=u.unit_id
left outer join mas_rank rr on pr.rank_id=r.rank_id
left outer join mas_trade tt on pr.trade_id=t.trade_id
left outer join mas_unit uu on pr.unit_id=u.unit_id

where accident_id=$P{accidentId}]]></queryString>

