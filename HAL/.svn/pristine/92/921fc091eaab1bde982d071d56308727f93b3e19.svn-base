	<parameter name="fromDate" isForPrompting="false" class="java.util.Date"/>
	<parameter name="toDate" isForPrompting="false" class="java.util.Date"/>
	<queryString><![CDATA[SELECT
     app_patient_appointments.`appointment_no` AS app_patient_appointments_appointment_no,
     app_patient_appointments.`appointment_date` AS app_patient_appointments_appointment_date,
     app_patient_appointments.`service_no` AS app_patient_appointments_service_no,
     app_patient_appointments.`mobile_no` as app_patient_appointments_mobileNo,
     CONCAT(app_patient_appointments.`from_time_slot`," - ",app_patient_appointments.`to_time_slot`)as time_slot,
     CONCAT(mas_employee.`first_name`," ",mas_employee.`middle_name`," ",mas_employee.`last_name`)AS doctor,
     mas_department.`department_name` AS mas_department_department_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     patient.`hin_no` AS patient_hin_no,
     app_patient_appointments.`sex` AS app_patient_appointments_sex,
     app_patient_appointments.`age` AS app_patient_appointments_age,
     CONCAT(app_patient_appointments.`age`,"/",app_patient_appointments.`sex`) AS ageSex,
     app_patient_appointments.`patient_name` AS app_patient_appointments_patient_name,
     CONCAT(app_patient_appointments.`service_person_name`) AS app_patient_appointments_service_person_name,
     case when mas_rank.`rank_name` is null 
             then app_patient_appointments.`rank`
               else mas_rank.`rank_name` end AS rank_name,
     mas_relation.`relation_name` AS relation_name
FROM
     `mas_employee` mas_employee RIGHT OUTER JOIN `app_patient_appointments` app_patient_appointments ON mas_employee.`employee_id` = app_patient_appointments.`employee_id`
     LEFT OUTER JOIN `mas_department` mas_department ON app_patient_appointments.`department_id` = mas_department.`department_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON app_patient_appointments.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `patient` patient ON app_patient_appointments.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_relation` mas_relation ON app_patient_appointments.`relation_id` = mas_relation.`relation_id`
$P!{QUERY}]]></queryString>

