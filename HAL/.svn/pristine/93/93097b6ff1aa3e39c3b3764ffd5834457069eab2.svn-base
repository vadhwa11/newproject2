	<queryString><![CDATA[SELECT
     birthdeathreg.`Name` AS birthdeathreg_Name,
     birthdeathreg.`fname` AS birthdeathreg_fname,
     birthdeathreg.`mname` AS birthdeathreg_mname,
     birthdeathreg.`DOB` AS birthdeathreg_DOB,
     birthdeathreg.`DOR` AS birthdeathreg_DOR,
     birthdeathreg.`Regno` AS birthdeathreg_Regno,
     mas_administrative_sex.`administrative_sex_name` AS mas_administrative_sex_administrative_sex_name,
     birthdeathreg.`DOD` AS birthdeathreg_DOD,
     birthdeathreg.`remarks` AS birthdeathreg_remarks,
     birthdeathreg.`date_of_issue` AS birthdeathreg_date_of_issue,
     birthdeathreg.`address_death` AS address_death,
     birthdeathreg.`address_permanent` AS address_permanent,
    patient.`hin_no` AS hin_no,
     concat(mas_employee.`first_name`,mas_employee.`middle_name`,mas_employee.`last_name`) AS emp_name
FROM
     `mas_administrative_sex` mas_administrative_sex
 LEFT OUTER JOIN `birthdeathreg` birthdeathreg ON mas_administrative_sex.`administrative_sex_id` = birthdeathreg.`administrative_sex_id`
 LEFT OUTER JOIN `mas_employee` mas_employee ON birthdeathreg.`emp_id` = mas_employee.`employee_id`
 LEFT OUTER JOIN `inpatient` inpatient ON birthdeathreg.`inpatient_id`= inpatient.`inpatient_id` 
 LEFT OUTER JOIN `patient` patient ON inpatient.`hin_id` = patient.`hin_id`
where
	 birthdeathreg.inpatient_id=$P{inpatientId} and  birthdeathreg.bdType='b']]></queryString>

