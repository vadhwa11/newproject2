	<parameter name="QUERY" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     patient.`service_no` AS patient_service_no,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_trade.`trade_name` AS mas_trade_trade_name,
     patient.`date_of_birth` AS patient_date_of_birth,
     CONCAT(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`," (",mas_relation.`relation_name`,") ")as patientName,
     CONCAT(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)as serviceName,
     md_cardic_claim_advance.`identification_mark` AS md_cardic_claim_advance_identification_mark,
     mas_relation.`relation_name` AS mas_relation_relation_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name ,
     mas_unit.`unit_name` as unit_name
FROM
     `mas_rank` mas_rank RIGHT OUTER JOIN `patient` patient ON mas_rank.`rank_id` = patient.`rank_id`
     RIGHT OUTER JOIN `md_cardic_claim_advance` md_cardic_claim_advance ON patient.`hin_id` = md_cardic_claim_advance.`hin_id`
     LEFT OUTER JOIN `mas_trade` mas_trade ON patient.`trade_id` = mas_trade.`trade_id`
     Left outer join `mas_unit` mas_unit on patient.`unit_id`=mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`
     INNER JOIN `mas_hospital` mas_hospital ON patient.`hospital_id` = mas_hospital.`hospital_id`
     AND mas_hospital.`hospital_id` = md_cardic_claim_advance.`hospital_id`
$P!{QUERY}]]></queryString>

