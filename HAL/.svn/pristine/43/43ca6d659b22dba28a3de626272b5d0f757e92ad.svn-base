	</parameter>
	<queryString><![CDATA[SELECT
    mas_charge_code.charge_code_name,mas_main_chargecode.main_chargecode_name,mas_sub_chargecode.sub_chargecode_name,
	case  when dg_mas_investigation.appear_in_discharge_summary='n' then 'No' else 'Yes' end as appear,
	case  when dg_mas_investigation.confidential='n' then 'No' else 'Yes' end as confidential,
	case  when dg_mas_investigation.investigation_type='t' then 'Template' 
	when dg_mas_investigation.investigation_type='s' then 'Single Parameter'
	when dg_mas_investigation.investigation_type='m' then 'Multiple Parameter'
	when dg_mas_investigation.investigation_type='v' then 'Sensitive'
	else '' end as investigation_type

,instructions,
	case  when dg_mas_investigation.status='y' then 'Active' else 'Inactive' end as status
FROM
		dg_mas_investigation dg_mas_investigation
   	 left outer join    mas_main_chargecode mas_main_chargecode  ON mas_main_chargecode.main_chargecode_id = dg_mas_investigation.main_chargecode_id
     left outer join mas_sub_chargecode mas_sub_chargecode on  mas_sub_chargecode.sub_chargecode_id = dg_mas_investigation.sub_chargecode_id
     left outer join mas_charge_code mas_charge_code on mas_charge_code.charge_code_id=dg_mas_investigation.charge_code_id
     left outer join mas_department  mas_department on mas_department.department_id=mas_main_chargecode.department_id
where mas_department.department_id=$P{deptId}
$P!{qry}
ORDER BY
     mas_charge_code.charge_code_name ASC]]></queryString>

