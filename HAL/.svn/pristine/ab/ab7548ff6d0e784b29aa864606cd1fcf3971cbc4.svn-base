	/* Only for test purpose */
.Block {
	background:url(../images/formBg.gif) repeat-x #EFF3F8;
	border: 1px solid #CCCFD2;
	padding: 3px 0px 3px 0px;
	float:left;
	width:992px;
	}
.Block .leftPlace{
	float:left;
	width:400px;
	}
.Block a {
	color: #F27824;
	font: bold 14px/ 25px Arial, Helvetica, sans-serif;
	text-decoration: underline;
	padding-left: 10px;
}

.Block a:hover {
	color: #F27824;
	font: bold 14px/ 25px Arial, Helvetica, sans-serif;
	text-decoration: none;
}

.Block img {
	vertical-align: middle;
	border: none;
	line-height: normal;
	padding: 0px;
	margin-right: 14px;
	display: inline;
	float: left;
}

.Block #calendar {
	padding: 0px;
	margin: 5px 14px 0px 0px;
	vertical-align: middle;
	float: left;
}

.Block .msg {
	color: #F27824;
	font: 14px arial;
}

.Block label {
	font: bold 12px/ 20px arial;
	color: #01497B;
	width: 147px;
	float: left;
	text-align: left;
	padding-left: 5px;
	margin: 2px 0px 0px 0px;
}
.Block p{
	margin:2px 1px 0px 5px;
	padding:0px;
	width:auto;
	color:#0350BB;
	font: bold 12px/ 22px arial;
	text-decoration:underline;
	float: left;
	text-align: right;
	height:22px;
}
.Block label.highlight{
	color: #F15917;
	}

.Block label.medium {
	width: 77px;
}
.Block label.medium2 {
	width: 93px;
}
.Block label.subHeading {
