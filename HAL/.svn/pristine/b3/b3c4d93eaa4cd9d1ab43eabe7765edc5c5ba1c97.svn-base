	</parameter>
	<queryString><![CDATA[select i.AD_NO as adNO,p.SERVICE_NO as serNo,md.DEPARTMENT_NAME as ward,
(p.P_FIRST_NAME||' '||p.P_LAST_NAME ) as pName,mr.RELATION_NAME as relation,mra.RANK_NAME,
(p.S_FIRST_NAME||' '||p.S_LAST_NAME) as pname1,mt.TRADE_NAME as trade,mu.UNIT_NAME as unit,
p.AGE as age,mas.ADMINISTRATIVE_SEX_NAME as gender,mi.ICD_NAME as diagnosis,
(mr1.RANK_NAME||' '||me.FIRST_NAME||' '||me.LAST_NAME) as admittingMo,p.DRUG_ALLERGIES as allergy,cat.CATEGORIES as medCat,
i.DATE_OF_ADDMISSION as doa,
mh.hospital_name as hospName,mh.address as addr,sds.REMARKS as remarks,
sds.CONDITION_STATUS as conditionStatus,sds.NOK as nok,(mr2.RANK_NAME||' '||me1.FIRST_NAME||' '||me1.LAST_NAME) as placedBy,
sds.LAST_CHG_DATE,sds.LAST_CHG_TIME,sds.SIL_DIL_REMARKS as sildilremarks,sds.remarks as condition,
p.Nok2_address as nokAddress
from sil_dil_status sds 
left outer join inpatient i on sds.INPATIENT_ID=i.INPATIENT_ID
left outer join patient p on i.HIN_ID=p.HIN_ID
left outer join mas_department md on i.DEPARTMENT_ID=md.DEPARTMENT_ID
left outer join mas_relation mr on p.RELATION_ID=mr.RELATION_ID
left outer join mas_rank mra on p.RANK_ID=mra.RANK_ID
left outer join mas_unit mu on p.UNIT_ID=mu.UNIT_ID
left outer join mas_trade mt on p.TRADE_ID=mt.TRADE_ID
left outer join mas_administrative_sex mas on p.SEX_ID=mas.ADMINISTRATIVE_SEX_ID
left outer join discharge_icd_code dic on i.INPATIENT_ID=dic.DISCHARGE_ICD_CODE_ID
left outer join mas_icd mi on dic.ICD_ID=mi.ICD_ID
left outer join mas_employee me on i.DOCTOR_ID=me.EMPLOYEE_ID
left outer join category cat on p.CATEGORY_ID=cat.CATEGORYID
left outer join mas_hospital mh on p.hospital_id=mh.hospital_id
left outer join mas_employee me1 on sds.PLACED_BY=me1.EMPLOYEE_ID
left outer join mas_rank mr1 on me.RANK_ID=mr1.RANK_ID
left outer join mas_rank mr2 on me1.RANK_ID=mr2.RANK_ID
where p.SERVICE_NO=$P{serviceNo} and p.hin_no=$P{hinNo} and i.AD_NO=$P{adNo}
and p.hospital_id=$P{hospitalId}
order by sds.LAST_CHG_DATE,sds.LAST_CHG_TIME]]></queryString>

