	<parameter name="date" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select osh.`requisition_date` as requisition_date ,
concat( ifnull(mr.`relation_name`,'-'),' ',ifnull(p.`service_no`,'-'),' ',ifnull(mra.`rank_name`,'-')
,' ',concat(ifnull(p.`s_first_name`,''),' ',ifnull(p.`s_middle_name`,''),' ',ifnull(p.`s_last_name`,''))) as perticular,
concat(ifnull(me.`first_name`,''),' ',ifnull(me.`middle_name`,''),' ',ifnull(me.`last_name`,'')) as approved_by,
opad.`asa_grade_acceptance` as asa_grade_acceptance,
opad.`asa_grade` as asa_grade,
osh.`visit_id` as visit_id,
osh.`inpatient_id` as inpatient,
osh.`pac_status` as pas_status 
from opd_surgery_header osh left outer join ot_pre_anesthesia_details opad on opad.visit_id = osh.visit_id or opad.inpatient_id = osh.inpatient_id
left outer join patient p on osh.hin_id = p.hin_id
left outer join mas_relation mr on mr.relation_id = p.relation_id
left outer join mas_rank mra on mra.rank_id=p.rank_id
left outer join mas_employee me on me.employee_id = osh.employee_id
where osh.`requisition_date` =$P{date}
and osh.`booking_status` ='pending']]></queryString>

