	<queryString><![CDATA[SELECT
     ipd_output.ipd_output_date AS ipd_output_date,
    sum(ipd_output.drain),
    sum(ipd_output.girth),
    sum(ipd_output.blood_sugar),
    sum(ipd_output.insulin)
   
FROM
     ipd_intake_output_chart ipd_intake_output_chart
     LEFT OUTER JOIN ipd_output ipd_output ON ipd_intake_output_chart.intakeoutput_id = ipd_output.intakeoutput_id
WHERE
     ipd_intake_output_chart.ad_no = $P{adNo}
 and ipd_intake_output_chart.intake_date >= $P{fromDate}
     and ipd_intake_output_chart.intake_date <=  $P{toDate}
group by  ipd_output.ipd_output_date]]></queryString>

