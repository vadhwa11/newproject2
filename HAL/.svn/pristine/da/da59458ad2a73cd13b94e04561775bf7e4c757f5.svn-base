	<queryString><![CDATA[select service_no,
(p_first_name||' '||nvl(p_middle_name,'')||' '||nvl(p_last_name,'')) as pname,
injection_date 
,(s_first_name||' '||nvl(s_middle_name,'')||' '||nvl(s_last_name,'')) as sname,
rel.relation_name,mr.rank_name,mu.UNIT_NAME,age,ms.ADMINISTRATIVE_SEX_NAME,
msi.nomenclature,opd.initial_diagnosis,
h.hospital_name as hospital_name,
injd.allergic_testing as allergic_testing,
injd.adverse_reaction as adverse_reaction,
(emprank.rank_name || ' '||me.first_name||' '||nvl(me.middle_name,'')||' '||nvl(me.last_name,'')) as mo,
injd."DOSE" AS dose,
injd."ROUTE" AS route,
injd."NO_OF_DAYS" AS no_of_days,
     mf."FREQUENCY_NAME" AS MAS_FREQUENCY_FREQUENCY_NAME
from injection_register_details injd left join injection_register injh on injd.injection_register_id=injh.injection_register_id
left join mas_store_item msi on injd.item_id= msi.item_id
left join visit v on injh.visit_id= v.visit_id
left join opd_patient_details opd on v.visit_id=opd.visit_id
left join patient p on injh.hin_id=p.hin_id 
left join mas_relation rel on p.relation_id=rel.relation_id
left join mas_rank mr on p.rank_id=mr.rank_id
left join mas_unit mu on p.unit_id=mu.unit_id
left join mas_administrative_sex ms on p.sex_id=ms.ADMINISTRATIVE_SEX_ID
left join mas_hospital h on injh.hospital_id=h.hospital_id
left join mas_employee me on injh.medical_officer_id=me.employee_id
left join mas_rank emprank on me.rank_id=emprank.rank_id
left join mas_frequency mf on injd.frequency_id=mf.frequency_id
 where injection_date between $P{fromDate} and $P{toDate} and injh.hospital_id=$P{hospitalId} $P!{qry}]]></queryString>

