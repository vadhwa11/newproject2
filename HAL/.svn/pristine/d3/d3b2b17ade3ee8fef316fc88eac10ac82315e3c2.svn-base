left outer join store_item_batch_stock sibs on idc.BATCH_STOCK_ID=sibs.STOCK_ID
left outer join mas_store_item msi on sibs.ITEM_ID=msi.ITEM_ID
left outer join mas_hospital mh on idc.HOSPITAL_ID=mh.HOSPITAL_ID 
where idc.hospital_id=$P{hospitalId} 
$P!{query}
group by msi.NOMENCLATURE,sibs.BATCH_NO,mh.HOSPITAL_NAME,mh.ADDRESS]]></queryString>

