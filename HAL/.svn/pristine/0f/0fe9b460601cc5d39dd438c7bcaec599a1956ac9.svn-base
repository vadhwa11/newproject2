		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     patient.hin_no AS patient_hin_no,
     patient.service_no AS patient_service_no,
     (patient.s_first_name|| ' ' ||patient.s_last_name)AS patient_name,
     mas_department.department_name AS mas_department_department_name,
     mas_diagnosis_conclusion.diagnosis_conclusion_name AS diagnosis_conclusion_name,
     (visit.age|| ' ' ||'/'|| ' ' ||mas_administrative_sex.administrative_sex_name)AS age_sex,
     mas_rank_category.rank_category_name AS rank_category_name,
     visit.visit_date AS visit_visit_date,
     mas_rank.rank_name AS mas_rank_rank_name,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     mas_relation.relation_name AS mas_relation_relation_name,
     NVL(mas_icd.icd_name,'')AS mas_icd_icd_name,
     visit.working_diagnosis AS sd,
    --- NVL(visit.working_diagnosis ,'')AS visit_working_diagnosis,
     mas_unit.unit_name AS mas_unit_unit_name,
     mas_unit.unit_address AS mas_unit_unit_address
FROM
     patient patient RIGHT OUTER JOIN visit visit ON patient.hin_id = visit.hin_id
     LEFT OUTER JOIN mas_department mas_department ON visit.department_id = mas_department.department_id
     LEFT OUTER JOIN mas_diagnosis_conclusion mas_diagnosis_conclusion ON visit.diagnosis_id = mas_diagnosis_conclusion.diagnosis_conclusion_id
     LEFT OUTER JOIN discharge_icd_code discharge_icd_code ON visit.visit_id = discharge_icd_code.visit_id
     AND patient.hin_id = discharge_icd_code.hin_id
     LEFT OUTER JOIN mas_icd mas_icd ON discharge_icd_code.icd_id = mas_icd.icd_id
     LEFT OUTER JOIN mas_administrative_sex mas_administrative_sex ON patient.sex_id = mas_administrative_sex.administrative_sex_id
     LEFT OUTER JOIN mas_rank mas_rank ON patient.rank_id = mas_rank.rank_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON patient.hospital_id = mas_hospital.hospital_id
     RIGHT OUTER JOIN mas_relation mas_relation ON patient.relation_id = mas_relation.relation_id
     INNER JOIN mas_unit mas_unit ON patient.unit_id = mas_unit.unit_id
     LEFT OUTER JOIN mas_rank_category mas_rank_category ON mas_rank.rank_category_id = mas_rank_category.rank_category_id
WHERE
     visit.visit_date >= $P{FROM_DATE}
 AND visit.visit_date <= $P{TO_DATE}
 AND visit.department_id = $P{DEPARTMENT_ID}
 AND mas_rank.rank_category_id = $P{RANK_ID}
 And mas_hospital.hospital_id=$P{hospitalId}]]></queryString>

