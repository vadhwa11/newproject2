	<queryString><![CDATA[SELECT
     MAS_DEPARTMENT.DEPARTMENT_NAME AS MAS_DEPARTMENT_DEPARTMENT_NAME,
     INPATIENT.AD_NO AS INPATIENT_AD_NO,
     INPATIENT.DATE_OF_ADDMISSION AS INPATIENT_DATE_OF_ADDMISSION,
      INPATIENT.TIME_OF_ADDMISSION AS INPATIENT_TIME_OF_ADDMISSION,
     INPATIENT.HIN_ID AS INPATIENT_HIN_ID,
     (PATIENT.P_FIRST_NAME + ' ' + ISNULL(PATIENT.P_MIDDLE_NAME,'') + ' ' + ISNULL(PATIENT.P_LAST_NAME,'')) AS PATIENT_NAME,
     ISNULL(PATIENT.SERVICE_NO, ' ') AS PATIENT_SERVICE_NO,
     PATIENT.RANK_ID AS PATIENT_RANK_ID,
     MAS_RANK.RANK_NAME AS MAS_RANK_RANK_NAME,
     (PATIENT.S_FIRST_NAME + ' ' + ISNULL(PATIENT.S_MIDDLE_NAME,'') + ' ' + ISNULL(PATIENT.S_LAST_NAME,'')) AS SERVICE_PERSON_NAME,
     PATIENT.TRADE_ID AS PATIENT_TRADE_ID,
     PATIENT.RECORD_OFFICE_ADDRESS_ID AS PATIENT_RECORD_OFFICE_ADDRESS,
     CONVERT(VARCHAR, ISNULL(MAS_RECORD_OFFICE_ADDRESS.ADDRESS,'') ) AS MAS_RECORD_OFFICE_ADDRESS_ADD,
     PATIENT.CDA_ACCOUNT_NO AS PATIENT_CDA_ACCOUNT_NO,
     INPATIENT.DIET_ID AS INPATIENT_DIET_ID,
     MAS_DIET.DIET_NAME AS MAS_DIET_DIET_NAME,
     INPATIENT.ADMISSION_TYPE_ID AS INPATIENT_ADMISSION_TYPE_ID,
     INPATIENT.DIAGNOSIS_ID AS INPATIENT_DIAGNOSIS_ID,
     INPATIENT.DOCTOR_ID AS INPATIENT_DOCTOR_ID,
    (MAS_EMPLOYEE.FIRST_NAME +' '+ISNULL(MAS_EMPLOYEE.MIDDLE_NAME,' ') +' '+ISNULL(MAS_EMPLOYEE.LAST_NAME, ' '))AS DOCTOR_NAME,
     PATIENT.RELIGION_ID AS PATIENT_RELIGION_ID,
     PATIENT.DISTRICT_ID AS PATIENT_DISTRICT_ID,
     MAS_DEPARTMENT.DEPARTMENT_ID AS MAS_DEPARTMENT_DEPARTMENT_ID,
    PATIENT.HIN_NO AS PATIENT_HIN_NO,
     PATIENT.SERVICE_TYPE_ID AS PATIENT_SERVICE_TYPE_ID,
    
     INPATIENT.AGE AS INPATIENT_AGE,
     PATIENT.AGE AS PATIENT_AGE,
    
     PATIENT.SERVICE_YEARS AS PATIENT_SERVICE_YEARS,
     MAS_ADMISSION_TYPE.ADMISSION_TYPE_NAME AS ADMISSION_TYPE_NAME,
     MAS_ICD.ICD_NAME AS MAS_ICD_ICD_NAME,
     MAS_RELIGION.RELIGION_NAME AS MAS_RELIGION_RELIGION_NAME, 
     MAS_DISTRICT.DISTRICT_NAME AS MAS_DISTRICT_DISTRICT_NAME,
     PATIENT.RELATION_ID AS PATIENT_RELATION_ID,
     MAS_RELATION.RELATION_NAME AS MAS_RELATION_RELATION_NAME,
     PATIENT.NEXT_OF_KIN_RELATION_ID AS NEXT_OF_KIN_RELATION_ID,
     PATIENT.TOTAL_SERVICE_PERIOD AS PATIENT_TOTAL_SERVICE_PERIOD,     
    CONVERT(VARCHAR, ISNULL(REPLACE(REPLACE(PATIENT.NEXT_OF_KIN_ADDRESS,CHAR(10),' '),CHAR(13) ,' '),' ') + '   ' + ISNULL(PATIENT.NEXT_OF_KIN_PHONE_NUMBER,'')) AS PATIENT_NEXT_OF_KIN_ADDRESS,
     MAS_RELATION_B.RELATION_NAME AS RELATION_NAME,
     PATIENT.SUFFIX AS PATIENT_SUFFIX,
   
     INPATIENT.REMARKS AS INPATIENT_REMARKS,
     INPATIENT.DIET_TYPE AS MAS_DIET_TYPE_NAME,
     INPATIENT.DIET_TYPE AS INPATIENT_DIET_TYPE,
     INPATIENT.INIT_DIAGNOSIS AS INPATIENT_INIT_DIAGNOSIS,
     INPATIENT.HSR_RECEIPT_NO AS INPATIENT_HSR_RECEIPT_NO,
     INPATIENT.HSR_AMOUNT AS INPATIENT_HSR_AMOUNT,
     INPATIENT.TRANS_FROM AS INPATIENT_TRANS_FROM,
     INPATIENT.PREVIOUS_AD_NO AS INPATIENT_PREVIOUS_AD_NO,
     MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_NAME AS ADMINISTRATIVE_SEX_NAME,
     PATIENT.SEX_ID AS PATIENT_SEX_ID,
     INPATIENT.FRW_SL_NO AS INPATIENT_FRW_SL_NO,
     INPATIENT.FRW_ISSUED AS INPATIENT_FRW_ISSUED,
     MAS_DISTRICT.DISTRICT_NAME AS PT_DISTRICT,
     (CASE  WHEN (INPATIENT.ATTACHED_UNIT IS NULL) THEN ' ' ELSE '(ATTACHED UNIT:' + INPATIENT.ATTACHED_UNIT +')' END)
    AS ATTACH_UNIT,

    CASE WHEN (PATIENT.ECHS_NO IS NULL OR LEN(PATIENT.ECHS_NO) = 0)
       THEN  ' ' ELSE 'ECHS NO: ' + PATIENT.ECHS_NO END AS ECHS_NO,
    HOSPITAL.HOSPITAL_NAME AS HOSPITAL_NAME,
MAS_MARITAL_STATUS.MARITAL_STATUS_NAME AS MARITAL_STATUS,
PATIENT.STATION AS STATION,
ISNULL(PATIENT.NEXT_OF_KIN_NAME,'') AS NEXT_OF_KIN_NAME,
CASE WHEN PATIENT.AB_64_AVAILABLE='Y' THEN 'YES' ELSE 'NO' END AS AB64,
INPATIENT.CONDITION_STATUS AS CONDITION
FROM
     MAS_DEPARTMENT MAS_DEPARTMENT LEFT OUTER JOIN INPATIENT INPATIENT ON MAS_DEPARTMENT.DEPARTMENT_ID = INPATIENT.AD_WARD_ID
     LEFT OUTER JOIN PATIENT PATIENT ON INPATIENT.HIN_ID = PATIENT.HIN_ID
     LEFT OUTER JOIN MAS_RECORD_OFFICE_ADDRESS MAS_RECORD_OFFICE_ADDRESS ON PATIENT.RECORD_OFFICE_ADDRESS_ID = MAS_RECORD_OFFICE_ADDRESS.RECORD_OFFICE_ADDRESS_ID
     LEFT OUTER JOIN MAS_DIET MAS_DIET ON INPATIENT.DIET_ID = MAS_DIET.DIET_ID
     LEFT OUTER JOIN MAS_ADMISSION_TYPE MAS_ADMISSION_TYPE ON INPATIENT.ADMISSION_TYPE_ID = MAS_ADMISSION_TYPE.ADMISSION_TYPE_ID
     LEFT OUTER JOIN VISIT VISIT ON inpatient.VISIT_ID = VISIT.VISIT_ID
left outer join discharge_icd_code dic on dic.VISIT_ID= VISIT.VISIT_ID
     LEFT OUTER JOIN MAS_ICD MAS_ICD ON dic.icd_id = MAS_ICD.ICD_ID
     LEFT OUTER JOIN MAS_EMPLOYEE MAS_EMPLOYEE ON INPATIENT.DOCTOR_ID = MAS_EMPLOYEE.EMPLOYEE_ID         
     LEFT OUTER JOIN MAS_RANK MAS_RANK ON PATIENT.RANK_ID = MAS_RANK.RANK_ID         
     LEFT OUTER JOIN MAS_RELATION MAS_RELATION ON PATIENT.RELATION_ID = MAS_RELATION.RELATION_ID
     LEFT OUTER JOIN MAS_RELIGION MAS_RELIGION ON PATIENT.RELIGION_ID = MAS_RELIGION.RELIGION_ID
     LEFT OUTER JOIN MAS_DISTRICT MAS_DISTRICT ON PATIENT.DISTRICT_ID = MAS_DISTRICT.DISTRICT_ID
     LEFT OUTER JOIN MAS_RELATION MAS_RELATION_B ON PATIENT.NEXT_OF_KIN_RELATION_ID = MAS_RELATION_B.RELATION_ID
     LEFT OUTER JOIN MAS_ADMINISTRATIVE_SEX MAS_ADMINISTRATIVE_SEX ON PATIENT.SEX_ID = MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_ID
     LEFT OUTER JOIN MAS_RANK MAS_RANKE ON MAS_EMPLOYEE.RANK_ID = MAS_RANKE.RANK_ID
     LEFT JOIN MAS_HOSPITAL HOSPITAL ON PATIENT.HOSPITAL_ID=HOSPITAL.HOSPITAL_ID
     LEFT OUTER JOIN MAS_MARITAL_STATUS MAS_MARITAL_STATUS ON PATIENT.MARITAL_STATUS_ID = MAS_MARITAL_STATUS.MARITAL_STATUS_ID    
WHERE
     INPATIENT.AD_NO = $P{adNo} AND INPATIENT.HOSPITAL_ID = $P{hospitalId}]]></queryString>

