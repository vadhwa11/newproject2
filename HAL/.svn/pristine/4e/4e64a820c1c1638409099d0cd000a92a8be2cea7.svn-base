	<queryString><![CDATA[SELECT
     msi.pvms_no AS item_pvms_no,
     msi.nomenclature AS item_nomenclature,
     msu.unit_name AS convrsn_itm_unit_name,
     mss.section_name AS section_name,
     mss.section_id
from
mas_store_item msi 
LEFT OUTER JOIN mas_store_item_conversion msic ON msi.item_conversion_id = msic.item_conversion_id
LEFT OUTER JOIN mas_store_unit msu ON msu.unit_id = msic.ISSUE_UNIT_ID
LEFT OUTER JOIN mas_store_section mss ON mss.section_id = msi.section_id

WHERE  msi.section_id = $P{SECTION}
AND    msi.item_type_id = $P{ITEM_TYPE}
AND    msi.department_id  = $P{DEPART}
And   msi.hospital_id = $P{hospital_Id}
order by msi.pvms_no]]></queryString>

