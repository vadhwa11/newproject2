	<queryString><![CDATA[SELECT
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     md_contigent_medical_bill_hd.`entry_date` AS md_contigent_medical_bill_hd_entry_date,
     mas_relation.`relation_name` AS mas_relation_relation_name,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     patient.`service_no` AS patient_service_no,
CONCAT(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)as serviceName,
     mas_charge_code.`charge_code_name` AS mas_charge_code_charge_code_name,
     md_mas_authority.`authority_name` AS md_mas_authority_authority_name,
     md_contigent_medical_bill_hd.`amount` AS md_contigent_medical_bill_hd_amount
FROM
     `md_contigent_medical_bill_hd` md_contigent_medical_bill_hd RIGHT OUTER JOIN `md_contigent_medical_bill_dt` md_contigent_medical_bill_dt ON md_contigent_medical_bill_hd.`id` = md_contigent_medical_bill_dt.`bill_header_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON md_contigent_medical_bill_hd.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `patient` patient ON md_contigent_medical_bill_hd.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `md_mas_authority` md_mas_authority ON md_contigent_medical_bill_hd.`fwt_to` = md_mas_authority.`id`
     LEFT OUTER JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_charge_code` mas_charge_code ON md_contigent_medical_bill_dt.`charge_id` = mas_charge_code.`charge_code_id`

$P!{QUERY}]]></queryString>

