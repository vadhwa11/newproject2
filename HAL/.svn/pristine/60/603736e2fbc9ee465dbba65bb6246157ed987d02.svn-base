	<queryString><![CDATA[SELECT
     store_op_patient_issue_m.issue_no AS store_m_issue_no,
     patient.hin_no AS store_m_hin_no,
     patient.service_no as service_no,
     (patient.p_first_name||' '||patient.p_middle_name||' '||patient.p_last_name) as patient_name,
     store_op_patient_issue_m.issue_date AS store_issue_date,
     store_op_patient_issue_m.prescription_no AS store_m_prescription_no,
     (mas_employee.first_name||' ' || mas_employee.last_name) as doctor_name,
     mas_store_item.pvms_no AS mas_pvms_no,
     mas_store_item.nomenclature AS mas_nomenclature,
     store_op_patient_issue_t.qty_issued AS store_t_qty_issued,
     store_op_patient_issue_t.batch_no AS store_t_batch_no,
     store_op_patient_issue_t.expiry_date AS store_t_expiry_date
FROM patient  left JOIN 
     store_op_patient_issue_m store_op_patient_issue_m on patient.hin_id=store_op_patient_issue_m.hin_id
     left JOIN store_op_patient_issue_t store_op_patient_issue_t ON store_op_patient_issue_m.op_issue_id = store_op_patient_issue_t.op_issue_id
     left JOIN mas_employee mas_employee ON store_op_patient_issue_m.emp_id = mas_employee.employee_id
     left JOIN mas_store_item mas_store_item ON store_op_patient_issue_t.item_id_issue = mas_store_item.item_id
WHERE store_op_patient_issue_m.issue_no = $P{issueNo}
AND   store_op_patient_issue_m.department_id =$P{deptId}
AND   store_op_patient_issue_m.type_of_issue = 'P']]></queryString>

