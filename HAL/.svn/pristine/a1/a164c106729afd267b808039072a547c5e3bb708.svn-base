	<queryString><![CDATA[SELECT  
  mas_employee.first_name,
  mas_rank.rank_name,
  hr_specialist_master.specialist_name,
  mas_employee.service_no,
  mas_employee.join_date,
  mas_employee.status,
  mas_department.department_name,
  hr_specialist_master.specialist_name,
  mas_department.department_name
FROM mas_employee
  LEFT JOIN
  mas_rank ON mas_employee.rank_id = mas_rank.rank_id
  LEFT JOIN
  hr_specialist_master ON mas_employee.speciality_id = hr_specialist_master.id
  LEFT JOIN
  mas_trade on mas_employee.trade_id=mas_trade.trade_id
  LEFT JOIN
  mas_department on mas_employee.department_id=mas_department.department_id 
  WHERE
  mas_trade.trade_name="Med Asst" and mas_employee.speciality_id=$P{speciality}]]></queryString>

