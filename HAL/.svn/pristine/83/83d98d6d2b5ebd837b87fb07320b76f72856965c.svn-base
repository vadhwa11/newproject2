	<queryString><![CDATA[select
     discharge.injury_report_initiated_on as injury_report_initiated_on,
 discharge.injury_report_init_at as injury_report_init_at,
     to_char(discharge.discharge_in_medical_category) as discharge_in_med_category,
     discharge.board_held_on as discharge_board_held_on,
     discharge.care_type_id as discharge_care_type_id,
     to_char(discharge.care_summary) as discharge_care_summary,
     to_char(discharge.instructions_to_patient) as instructions_to_patient,
     (to_date(discharge.date_of_discharge, 'dd-mon-yyyy')||' '||discharge.time_of_discharge) as date_of_discharge,
     inpatient.age as inpatient_age,
     inpatient.date_of_addmission as date_of_addmission,
     (case (discharge.other_hospital) when '1' then mas_disposed_to.disposed_to_name else discharge.other_hospital end) disposed_to_name,
     mas_diagnosis_conclusion.diagnosis_conclusion_name as diagnosis_conclusion_name,
     mas_disposal.disposal_name as mas_disposal_disposal_name,
     mas_disposed_to.disposed_to_name as disposed_to_name,
     (patient.p_first_name||' '||patient.p_middle_name||' '||patient.p_last_name)as patientname,
     (patient.s_first_name||' '||patient.s_middle_name||' '||patient.s_last_name)as servicepersonname,
     patient.service_no as patient_service_no,
     patient.station as patient_station,
     patient.formation as patient_formation,
     patient.service_years as patient_service_years,
     mas_rank.rank_name as mas_rank_rank_name,
     to_char(mas_unit.unit_name||','||mas_unit.unit_address)as mas_unit_unit_name,
     mas_trade.trade_name as mas_trade_trade_name,
     mas_relation.relation_name as relation_name,
     to_char(mas_record_office_address.address) as record_office_address,
     mas_administrative_sex.administrative_sex_name as administrative_sex_name,
     mas_service_type.service_type_name as service_type_name,
     mas_marital_status.marital_status_name as marital_status_name,
     mas_religion.religion_name as religion_name,
     patient.hin_no as patient_hin_no,
     inpatient.ad_status as inpatient_ad_status,
     inpatient.inpatient_id as inpatient_inpatient_id,
     inpatient.ad_no as inpatient_ad_no,
     inpatient.time_of_addmission as time_of_addmission,
     discharge.time_of_discharge as time_of_discharge,
     patient.total_service_period as total_service_period,
     discharge.document_initiated as documentinitiated,
    (case inpatient.attached_unit when null then ' ' else ('(attached unit:'|| inpatient.attached_unit||')') end)
    as attach_unit,
    (case length(discharge.working_diagnosis) when  0 then ' ' else ('('|| discharge.working_diagnosis || ')') end) workingdiagnosis,
    mas_department.department_name as department_name,
mas_hospital.hospital_name as hospital_name
from
     patient patient right outer join discharge discharge on patient.hin_id = discharge.hin_id
     left outer join inpatient inpatient on patient.hin_id = inpatient.hin_id
     left outer join mas_rank mas_rank on patient.rank_id = mas_rank.rank_id
     left outer join mas_unit mas_unit on patient.unit_id = mas_unit.unit_id
     left outer join mas_trade mas_trade on patient.trade_id = mas_trade.trade_id
     left outer join mas_relation mas_relation on patient.relation_id = mas_relation.relation_id
     left outer join mas_record_office_address mas_record_office_address on patient.record_office_address_id = mas_record_office_address.record_office_address_id
     left outer join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
     left outer join mas_service_type mas_service_type on patient.service_type_id = mas_service_type.service_type_id
     left outer join mas_marital_status mas_marital_status on patient.marital_status_id = mas_marital_status.marital_status_id
     left outer join mas_religion mas_religion on patient.religion_id = mas_religion.religion_id
     left outer join mas_document mas_document on inpatient.document_id = mas_document.document_id
     left outer join mas_diagnosis_conclusion mas_diagnosis_conclusion on inpatient.diagnosis_id = mas_diagnosis_conclusion.diagnosis_conclusion_id
     left outer join mas_disposal mas_disposal on discharge.disposal_id = mas_disposal.disposal_id
     left outer join mas_disposed_to mas_disposed_to on discharge.disposed_to_id = mas_disposed_to.disposed_to_id
     left outer join mas_department mas_department on inpatient.department_id = mas_department.department_id
     left outer join mas_hospital mas_hospital on discharge.hospital_id=mas_hospital.hospital_id
where
    inpatient.ad_no=discharge.ad_no and inpatient.inpatient_id = $P{inpatientId} and inpatient.ad_status in ('d','r','s')]]></queryString>

