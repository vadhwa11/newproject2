	<parameter name="QUERY" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     md_cardic_contingent_bill_hd.`entry_date` AS md_cardic_contingent_bill_hd_entry_date,
     patient.`service_no` AS patient_service_no,
     mas_relation.`relation_name` AS mas_relation_relation_name,
     CONCAT(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)as serviceName,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_hospital_A.`hospital_name` AS mas_hospital_A_hospital_name,
     md_cardic_claim_advance.`dgms_dispatch_date` AS md_cardic_claim_advance_dgms_dispatch_date,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     md_cardic_claim_advance.`treatment_detail` AS md_cardic_claim_advance_treatment_detail,
     md_cardic_claim_advance.`qualifying_amount` AS md_cardic_claim_advance_qualifying_amount,
     md_cardic_claim_advance.`ex_post` AS md_cardic_claim_advance_ex_post
FROM
     `md_cardic_contingent_bill_hd` md_cardic_contingent_bill_hd RIGHT OUTER JOIN `md_cardic_contingent_bill_dt` md_cardic_contingent_bill_dt ON md_cardic_contingent_bill_hd.`id` = md_cardic_contingent_bill_dt.`cardic_hd_id`
     LEFT OUTER JOIN `patient` patient ON md_cardic_contingent_bill_hd.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `md_cardic_claim_advance` md_cardic_claim_advance ON md_cardic_contingent_bill_hd.`cardic_advance_id` = md_cardic_claim_advance.`id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON md_cardic_contingent_bill_hd.`hospital_id` = mas_hospital.`hospital_id`
     AND patient.`hin_id` = md_cardic_claim_advance.`hin_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital_A ON md_cardic_claim_advance.`payable_to` = mas_hospital_A.`hospital_id`
     LEFT OUTER JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
$P!{QUERY}]]></queryString>

