	<queryString><![CDATA[SELECT
     DMO_CALL_REGISTER."PATIENT_REPORTED_DATE" AS REPORTED_DATE,
     DMO_CALL_REGISTER."CALL_SENT_DATE" AS CALL_SENT_DATE,
     DMO_CALL_REGISTER."DMO_ATTENDED_DATE" AS DMO_ATTENDED_DATE,
     DMO_CALL_REGISTER."DISPOSAL" AS DISPOSAL,
     (PATIENT."P_FIRST_NAME"||' '||PATIENT."P_MIDDLE_NAME"||' '||PATIENT."P_LAST_NAME")AS PATIENT_NAME,
     MAS_HOSPITAL."HOSPITAL_NAME" AS HOSPITAL_NAME,
     (MAS_EMPLOYEE."FIRST_NAME"||' '||MAS_EMPLOYEE."MIDDLE_NAME"||' '||MAS_EMPLOYEE."LAST_NAME")AS EMPLOYEE_NAME,
     PATIENT."SERVICE_NO" AS PATIENT_SERVICE_NO,
     PATIENT."HIN_NO" AS PATIENT_HIN_NO
FROM
     "HMS"."PATIENT" PATIENT RIGHT OUTER JOIN "HMS"."DMO_CALL_REGISTER" DMO_CALL_REGISTER ON PATIENT."HIN_ID" = DMO_CALL_REGISTER."HIN_ID"
     LEFT OUTER JOIN "HMS"."MAS_HOSPITAL" MAS_HOSPITAL ON DMO_CALL_REGISTER."HOSPITAL_ID" = MAS_HOSPITAL."HOSPITAL_ID"
     LEFT OUTER JOIN "HMS"."MAS_EMPLOYEE" MAS_EMPLOYEE ON DMO_CALL_REGISTER."MEDICAL_OFFICER_ID" = MAS_EMPLOYEE."EMPLOYEE_ID"
where trunc(DMO_CALL_REGISTER."DMO_ATTENDED_DATE") between $P{fromDate} and $P{toDate}]]></queryString>

