	<queryString><![CDATA[SELECT
     store_internal_return_t.`return_qty` AS store_internal_return_t_return_qty,
     store_internal_return_m.`return_date` AS store_internal_return_m_return_date,
     store_grn_t.`received_qty` AS store_grn_t_received_qty,
     store_grn_m.`grn_date` AS store_grn_m_grn_date,
     store_issue_m.`issue_date` AS store_issue_m_issue_date,
     store_issue_t.`qty_issued` AS store_issue_t_qty_issued,
     mas_store_item.`pvms_no` AS mas_store_item_pvms_no,
     store_item_batch_stock.`opening_balance_date` AS store_item_batch_stock_opening_balance_date,
     store_item_batch_stock.`opening_balance_qty` AS store_item_batch_stock_opening_balance_qty
	
FROM
     `mas_store_item` mas_store_item INNER JOIN `store_issue_t` store_issue_t ON mas_store_item.`item_id` = store_issue_t.`item_id`
     INNER JOIN `store_issue_m` store_issue_m ON store_issue_t.`issue_m_id` = store_issue_m.`id`
     INNER JOIN `store_grn_t` store_grn_t ON mas_store_item.`item_id` = store_grn_t.`item_id`
     INNER JOIN `store_internal_return_t` store_internal_return_t ON mas_store_item.`item_id` = store_internal_return_t.`item_id`
     INNER JOIN `store_item_batch_stock` store_item_batch_stock ON mas_store_item.`item_id` = store_item_batch_stock.`item_id`
     INNER JOIN `store_internal_return_m` store_internal_return_m ON store_internal_return_t.`return_main_id` = store_internal_return_m.`id`
     INNER JOIN `store_grn_m` store_grn_m ON store_grn_t.`grn_master_id` = store_grn_m.`grn_master_id`
WHERE
      store_internal_return_m.`return_date` between $P{fromDate} and $P{toDate} or 
	 store_grn_m.`grn_date` between $P{fromDate} and $P{toDate} or
	 store_issue_m.`issue_date`  between $P{fromDate} and $P{toDate} 
	and mas_store_item.`pvms_no`=$P{pvmsNo}
]]></queryString>

