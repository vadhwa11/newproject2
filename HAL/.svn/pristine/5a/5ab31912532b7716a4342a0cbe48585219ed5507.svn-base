	<parameter name="QUERY" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     CONCAT(mas_employee.`first_name`," ",mas_employee.`middle_name`," ",mas_employee.`last_name`)as collectedBy,
     CONCAT(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`)as patientname,
     dg_sample_collection_details.`sample_no` AS dg_sample_collection_details_sample_no,
     dg_sample_collection_header.`sample_collection_time` AS dg_sample_collection_header_sample_collection_time,
     dg_sample_collection_header.`sample_collection_date` AS dg_sample_collection_header_sample_collection_date,
     dg_orderhd.`order_date` AS dg_orderhd_order_date,
     dg_orderhd.`order_no` AS dg_orderhd_order_no,
     dg_orderhd.`hin_id` AS dg_orderhd_hin_id,
     patient.`age` AS patient_age,
     mas_administrative_sex.`administrative_sex_name` AS mas_administrative_sex_administrative_sex_name,
     mas_hospital_A.`hospital_name` AS mas_hospital_A_hospital_name,
     dg_sample_collection_header.`sample_collection_date` AS dg_sample_collection_header_sample_date,
     mas_main_chargecode.`main_chargecode_name` AS mas_main_chargecode_main_chargecode_name,
     mas_sub_chargecode.`sub_chargecode_name` AS mas_sub_chargecode_sub_chargecode_name
FROM
     `dg_orderhd` dg_orderhd RIGHT OUTER JOIN `dg_sample_collection_header` dg_sample_collection_header ON dg_orderhd.`orderhd_id` = dg_sample_collection_header.`order_id`
     RIGHT OUTER JOIN `dg_sample_collection_details` dg_sample_collection_details ON dg_sample_collection_header.`sample_collection_header_id` = dg_sample_collection_details.`sample_collection_header_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital_A ON dg_sample_collection_header.`hospital_id` = mas_hospital_A.`hospital_id`
     LEFT OUTER JOIN `mas_employee` mas_employee ON dg_sample_collection_details.`collected_by` = mas_employee.`employee_id`
     LEFT OUTER JOIN `mas_main_chargecode` mas_main_chargecode ON dg_sample_collection_details.`maincharge` = mas_main_chargecode.`main_chargecode_id`
     LEFT OUTER JOIN `mas_sub_chargecode` mas_sub_chargecode ON dg_sample_collection_details.`subcharge` = mas_sub_chargecode.`sub_chargecode_id`
     LEFT OUTER JOIN `patient` patient ON dg_orderhd.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `mas_administrative_sex` mas_administrative_sex ON patient.`sex_id` = mas_administrative_sex.`administrative_sex_id`

$P!{QUERY}]]></queryString>

