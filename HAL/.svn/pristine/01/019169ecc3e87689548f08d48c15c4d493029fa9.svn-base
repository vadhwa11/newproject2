<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="WorkloadMonthly"
		table="WORKLOAD_MONTHLY"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="WORKLOAD_ID"
		>
			<generator class="native"></generator>
		</id>

		<property
			name="TotalPopulation"
			column="TOTAL_POPULATION"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="OffrsAircrew"
			column="OFFRS_AIRCREW"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="OffrsGd"
			column="OFFRS_GD"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="AirmenCrew"
			column="AIRMEN_CREW"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="AirmenOther"
			column="AIRMEN_OTHER"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Ncs"
			column="NCS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Dsc"
			column="DSC"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="TotalSickReport"
			column="TOTAL_SICK_REPORT"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="AdmissionRatePopulation"
			column="ADMISSION_RATE_POPULATION"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="AdmServicePers"
			column="ADM_SERVICE_PERS"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="AdmissionTotal"
			column="ADMISSION_TOTAL"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="ReferralsAll"
			column="REFERRALS_ALL"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="ReferralsRatePopulation"
			column="REFERRALS_RATE_POPULATION"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="MiscMedBoards"
			column="MISC_MED_BOARDS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="MiscMedExams"
			column="MISC_MED_EXAMS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<many-to-one name="HospitalId"	column="HOSPITAL_ID" class="MasHospital"
			not-null="false">
			</many-to-one>
		<many-to-one name="UnitId"	column="UNIT_ID" class="MasUnit"  not-null="false" >
		</many-to-one>
	</class>	
</hibernate-mapping>