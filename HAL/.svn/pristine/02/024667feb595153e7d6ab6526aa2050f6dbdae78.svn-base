     pension_calculation_sheet.`net_qualifying_service_days` AS pension_calculation_sheet_net_qualifying_service_days,
     pension_calculation_sheet.`net_qualifying_service_months` AS pension_calculation_sheet_net_qualifying_service_months,
     pension_calculation_sheet.`net_qualifying_service_years` AS pension_calculation_sheet_net_qualifying_service_years,
     pension_data_sheet.`nationality_code` AS pension_data_sheet_nationality_code,
     mas_personnel_details.`date_of_birth` AS mas_personnel_details_date_of_birth,
     mas_personnel_details.`appointment_date` AS mas_personnel_details_appointment_date,
     mas_personnel_details.`retirement_date` AS mas_personnel_details_retirement_date,
     pension_data_sheet.`date_of_receipt` AS pension_data_sheet_date_of_receipt,
     pension_data_sheet.`fraction_comm` AS pension_data_sheet_fraction_comm,
     pension_data_sheet.`amount_comm` AS pension_data_sheet_amount_comm,
     pension_data_sheet.`age_load` AS pension_data_sheet_age_load,
     mas_designation.`designation_id` AS mas_designation_designation_id,
     mas_personnel_details.`personnel_id` AS mas_personnel_details_personnel_id,
     mas_personnel_details.`designation_id` AS mas_personnel_details_designation_id,
     pension_calculation_sheet.`personnel_id` AS pension_calculation_sheet_personnel_id,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `mas_designation` mas_designation RIGHT OUTER JOIN `mas_personnel_details` mas_personnel_details ON mas_designation.`designation_id` = mas_personnel_details.`designation_id`
     LEFT OUTER JOIN `pension_calculation_sheet` pension_calculation_sheet ON mas_personnel_details.`personnel_id` = pension_calculation_sheet.`personnel_id`
