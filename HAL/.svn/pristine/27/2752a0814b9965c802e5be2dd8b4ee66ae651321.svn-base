<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PatientGyneDetails"
		table="PATIENT_GYNE_DETAILS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="GYNE_DETAILS_ID">
			 <generator class="native"></generator>
		</id>
		
		<property
			name="MenstrualHistoryUnknown"
			column="MENSTRUAL_HISTORY_UNKNOWN"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="MenstrualStatus"
			column="MENSTRUAL_STATUS"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Lmp"
			column="LMP"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LmpUnknown"
			column="LMP_UNKNOWN"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="MenarcheAge"
			column="MENARCHE_AGE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="MenopauseAge"
			column="MENOPAUSE_AGE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="MenstrualCycle"
			column="MENSTRUAL_CYCLE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="MenstrualFlow"
			column="MENSTRUAL_FLOW"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Dysmenorrhea"
			column="DYSMENORRHEA"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="MenstrualAbnormalities"
			column="MENSTRUAL_ABNORMALITIES"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ObstetricHistoryUnknown"
			column="OBSTETRIC_HISTORY_UNKNOWN"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="ObstetricNotApplicable"
			column="OBSTETRIC_NOT_APPLICABLE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Gravida"
			column="GRAVIDA"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Para"
			column="PARA"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Abortions"
			column="ABORTIONS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Children"
			column="CHILDREN"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Pregnent"
			column="PREGNENT"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Lactating"
			column="LACTATING"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="FirstDeliveryAge"
			column="FIRST_DELIVERY_AGE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="LastDeliveryAge"
			column="LAST_DELIVERY_AGE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="BreastFeeding"
			column="BREAST_FEEDING"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="BreastFeedingDuration"
			column="BREAST_FEEDING_DURATION"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="MolarPregnency"
			column="MOLAR_PREGNENCY"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="HinNo"
			column="HIN_NO"
			type="string"
			not-null="false"
			length="30"
		/>
		<many-to-one name="Hin" column="hin_id" class="Patient"
			not-null="false">
		</many-to-one>

	</class>	
</hibernate-mapping>