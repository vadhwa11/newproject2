	<queryString><![CDATA[SELECT
     inpatient.`ad_no` AS ad_no,
     patient.`service_no` AS service_no,
     patient.`hin_no` AS hin_no,
     concat(patient.`p_first_name`," ",
     patient.`p_middle_name`," ",
     patient.`p_last_name`) AS patientName,
     patient.`age` AS patient_age,
     mas_administrative_sex.`administrative_sex_name` AS mas_administrative_sex_administrative_sex_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mis_fatal_tracking.`latest_status` AS status,
     mis_fatal_tracking.`latest_status_date` AS status_date,
     mis_fatal_tracking.`latest_status_remarks` AS status_remarks
FROM
     `inpatient` inpatient INNER JOIN `mis_fatal_tracking` mis_fatal_tracking ON inpatient.`inpatient_id` = mis_fatal_tracking.`inpatient_id`
     LEFT JOIN `patient` patient ON inpatient.`hin_id` = patient.`hin_id`
     LEFT JOIN `mas_administrative_sex` mas_administrative_sex ON patient.`sex_id` = mas_administrative_sex.`administrative_sex_id`
     LEFT JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
where mis_fatal_tracking.dateof_death between $P{fromDate} and $P{toDate}]]></queryString>

