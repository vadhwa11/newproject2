<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="DmaRegisterHeader"
		table="DMA_REGISTER_HEADER"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="DMA_REGISTER_HEADER_ID"
		>
			<generator class="increment"></generator>
		</id>

		<property
			name="Complaints"
			column="COMPLAINTS"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="LastChgDate"
			column="LAST_CHG_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LastChgTime"
			column="LAST_CHG_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="DmaRegisterDate"
			column="DMA_REGISTER_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="WorkingDiagnosis"
			column="WORKING_DIAGNOSIS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DmoCalled"
			column="DMO_CALLED"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="CallTime"
			column="CALL_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="TimeAttended"
			column="TIME_ATTENDED"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Remarks"
			column="REMARKS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ReportedTime"
			column="REPORTED_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="DmoTransAmbu"
			column="dmo_trans_ambu"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="TptAmbSentTime"
			column="tpt_amb_sent_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="DmoArrivedTime"
			column="dmo_arrived_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="Hin"
			column="HIN_ID"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="DMA"
			column="DMA_ID"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="DMO"
			column="DMO_ID"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Disposal"
			column="DISPOSAL_ID"
			class="MasDisposal"
			not-null="false"
		>
		</many-to-one>
	</class>	
</hibernate-mapping>