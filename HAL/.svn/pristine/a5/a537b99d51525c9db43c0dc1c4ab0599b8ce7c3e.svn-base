	<queryString><![CDATA[SELECT
    max(num),(sum(num)/(To_days($P{toDate}) - To_days($P{fromDate})))as offavg
from (SELECT
    count(*)as num
FROM
    `patient` patient INNER JOIN `inpatient` inpatient ON patient.`hin_id` = inpatient.`hin_id`
     INNER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     INNER JOIN `mas_rank_category` mas_rank_category ON mas_rank.`rank_category_id` = mas_rank_category.`rank_category_id`
WHERE
     inpatient.date_of_addmission between $P{fromDate} and $P{toDate}
and
     mas_rank_category.`rank_category_name` = "Officer"
GROUP BY
     inpatient.date_of_addmission)as a]]></queryString>

