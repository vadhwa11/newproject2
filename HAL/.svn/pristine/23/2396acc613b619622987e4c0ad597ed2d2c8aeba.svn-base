	<queryString><![CDATA[SELECT concat(mr.rank_name,' ',me1.first_name,' ',me1.middle_name,' ',me1.last_name) as emp_name,md.department_name as department_name, concat(me2.first_name,' ',me2.middle_name,' ',me2.last_name) as chairperson_name FROM ap_meeting_schedule ams
left outer join ap_meeting_employee ae on  ams.ap_meeting_schedule_id = ae.meeting_id
left outer join mas_employee me1 on ae.emp_id = me1.employee_id
left outer join mas_employee me2 on ams.actual_chaired_by = me2.employee_id
left outer join mas_department md on me1.department_id = md.department_id
left outer join mas_rank mr on mr.rank_id = me1.rank_id
where ae.meeting_id = 1
group by chairperson_name,department_name,emp_name]]></queryString>

