    <bean id="simpleTrigger" class="org.springframework.scheduling.quartz.SimpleTriggerBean">

        <property name="jobDetail">
            <ref bean="batchProcess" />
        </property>

        <property name="startDelay" value="0" />

        <property name="repeatInterval" value="0" />
    </bean>

    <bean id="cronTrigger" class="org.springframework.scheduling.quartz.CronTriggerBean">

        <property name="jobDetail">
            <ref bean="batchProcess" />
        </property>
        <property name="cronExpression" value="00 00 02 * * ?" />
    </bean>

    <bean name="batchProcess" class="org.springframework.scheduling.quartz.JobDetailBean">
        <property name="jobClass">
            <value>jkt.hms.diet.controller.DietSchedulerController</value>
        </property>
        <property name="jobDataAsMap">
            <map>
                <entry key="dietDataService" value-ref="dietDataService" />
            </map>
        </property>
    </bean>

    <bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
        <property name="triggers">
            <list>
                <ref local="cronTrigger" />

            </list>
        </property>
    </bean>
