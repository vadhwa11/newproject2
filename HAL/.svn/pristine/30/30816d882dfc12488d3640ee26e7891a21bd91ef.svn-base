     <bean id="cronTriggerForGRN"
class="org.springframework.scheduling.quartz.CronTriggerBean">
                <property name="jobDetail">
                        <ref bean="batchProcessForGRN" />
                </property>
                <property name="cronExpression" value="59 59 23 31 03 ?" />
        </bean>

        <bean name="batchProcessForGRN"
class="org.springframework.scheduling.quartz.JobDetailBean">
                <property name="jobClass">
                        <value>jkt.hms.stores.controller.GRNSchedulerController</value>
                </property>
                <property name="jobDataAsMap">
                        <map>
                                <entry key="storesDataService" value-ref="storesDataService" />
                        </map>
                </property>
        </bean>
        <bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
                <property name="triggers">
                        <list>
                                <ref local="cronTriggerForGRN" />

                        </list>
                </property>
        </bean>

