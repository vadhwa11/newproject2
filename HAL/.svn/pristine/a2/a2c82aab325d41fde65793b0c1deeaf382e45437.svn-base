hrorderly_leave_application.`addl_leave_to_date` as addl_leave_to_date ,
hrorderly_leave_application.`full_name` as full_name ,
hrorderly_leave_application.`house_name` as house_name ,
hrorderly_leave_application.`village` as village ,
hrorderly_leave_application.`po` as po ,
hrorderly_leave_application.`telegraph_office` as telegraph_office ,
hrorderly_leave_application.`police_station` as police_station ,
hrorderly_leave_application.`pin` as pin ,
hrorderly_leave_application.`telephone_no` as telephone_no ,
hrorderly_leave_application.`nrs` as nrs ,
mas_state.`state_name` as state_name ,
mas_district2.`district_name` as RS_upto ,
hrorderly_leave_application.`other_trains_upto` as other_trains_upto ,
case when hrorderly_leave_application.`drawing_transport_allowance`='y' then "YES"
     when hrorderly_leave_application.`drawing_transport_allowance`='n' then "NO"
     else "YES/NO" end  as drawing_transport ,
case when hrorderly_leave_application.`availing_ltc`='y' then "YES"
     when hrorderly_leave_application.`availing_ltc`='n' then "NO"
     else "YES/NO" end  as availing_ltc ,

mas_district1.`district_name` as city_to ,
mas_district.`district_name` as district_name ,
case when hrorderly_leave_application.`type`="f" then "FRW"
     when hrorderly_leave_application.`type`="c" then "CV"
     else "FRW/CV" end as application_type ,

case when mas_employee.`livingInOut`=0 then "Living IN"
            when mas_employee.`livingInOut`=1 then "Living OUT"
            else "" end as livingInOut ,
concat(ifnull(mas_title.`title_name`,"")," ",ifnull(mas_employee.`first_name`,"")," ",ifnull(mas_employee.`middle_name`,"")," ",ifnull(mas_employee.`last_name`,"")) as employee_name ,
ifnull( (select hla.`leave_availed` from hrorderly_leave_account hla left outer join hr_leave_type_master hltm on hla.`leave_type_id`=hltm.`id`
 where hla.`employee_id`=mas_employee.`employee_id` and hltm.`leave_type`='AL')," ") as al_leave ,
ifnull( (select hla.`leave_availed` from hrorderly_leave_account hla left outer join hr_leave_type_master hltm on hla.`leave_type_id`=hltm.`id`
 where hla.`employee_id`=mas_employee.`employee_id` and hltm.`leave_type`='SL')," ") as sl_leave ,
ifnull( (select hla.`leave_availed` from hrorderly_leave_account hla left outer join hr_leave_type_master hltm on hla.`leave_type_id`=hltm.`id`
 where hla.`employee_id`=mas_employee.`employee_id` and hltm.`leave_type`='CL')," ") as cl_leave ,
ifnull((select count(*) from family_details fd where fd.`leave_application_id`=hrorderly_leave_application.`leave_application_id`
and fd.`select_family`='s'),"") as self_family ,
ifnull((select count(*) from family_details fd where fd.`leave_application_id`=hrorderly_leave_application.`leave_application_id`
and fd.`select_family`='w'),"") as wife_family ,
ifnull((select count(*) from family_details fd where fd.`leave_application_id`=hrorderly_leave_application.`leave_application_id`
