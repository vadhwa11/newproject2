	<queryString><![CDATA[select 
Surname, First_Name, Service_No, Crew_Duty, Duration_Concussion, Duration_Retrograde,
R.rank_name,
(case
when fatal='n' then 'Non Fatal'
when fatal='f' then 'Fatal'
end) as fatal,


(case
when burns='n' then 'Nil'
when burns='s' then 'Slight'
when burns='v' then 'Severe'
when burns='i' then 'Incineration'
when burns='a' then 'Ante Mortem'
when burns='p' then 'Post Mortem'
when burns='b' then 'Both Periods'
when burns='k' then 'Period Not Known'
end) as burns,

(case
when Haemorrhage='n' then 'Nil'
when Haemorrhage='e' then 'Negligible'
when Haemorrhage='m' then 'Moderate'
when Haemorrhage='s' then 'Severe'
when Haemorrhage='a' then 'Not Applicable'
end) as Haemorrhage,

(case
when Shock='n' then'Nil'
when Shock='m' then'Mild'
when Shock='o' then'Moderate'
when Shock='s' then'Severe'
when Shock='a' then'Not Applicable'
end) as Shock,

(case
when Shock='n' then'Nil'
when Shock='m' then'Mild'
when Shock='o' then'Moderate'
when Shock='s' then'Severe'
when Shock='a' then'Not Applicable'
end) as Concussion


from av_accident A
left outer join mas_rank R on A.rank_id=R.rank_id
where  
A.ID=$P{AV_ACCIDENT_ID}]]></queryString>

