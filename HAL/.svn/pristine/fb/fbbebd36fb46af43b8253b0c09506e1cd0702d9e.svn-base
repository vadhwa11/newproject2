	<queryString><![CDATA[SELECT
     store_issue_t.`qty_issued` AS store_issue_t_qty_issued,
     store_issue_m.`issue_no` AS store_issue_m_issue_no,
     store_issue_m.`issue_date` AS store_issue_m_issue_date,
     mas_store_item_conversion.`item_unit_name` AS mas_store_item_conversion_item_unit_name,
     mas_store_item.`pvms_no` AS mas_store_item_pvms_no,
     mas_store_item.`nomenclature` AS mas_store_item_nomenclature,
     mas_store_item.`item_id` AS mas_store_item_item_id,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     mas_unit.`unit_name` AS mas_unit_unit_name
FROM
     `store_issue_m` store_issue_m LEFT OUTER JOIN `store_issue_t` store_issue_t ON store_issue_m.`id` = store_issue_t.`issue_m_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON store_issue_m.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON store_issue_m.`otafu` = mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_store_item` mas_store_item ON store_issue_t.`item_id` = mas_store_item.`item_id`
     LEFT OUTER JOIN `mas_store_item_conversion` mas_store_item_conversion ON mas_store_item.`item_conversion_id` = mas_store_item_conversion.`item_conversion_id`
WHERE
     store_issue_m.`issue_type` = 'x'
 AND store_issue_m.`department_id` = $P{DEPART}
 AND store_issue_m.`issue_date` >= $P{FROM_DATE}
 AND store_issue_m.`issue_date` <= $P{TO_DATE}
 AND store_issue_m.`otafu` = $P{UNIT}]]></queryString>

