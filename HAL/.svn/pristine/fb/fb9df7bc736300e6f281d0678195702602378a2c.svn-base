	<queryString><![CDATA[select 
 r.round_type, r.SANITARY_ROUND_DATE, r.CLASSIFICATION, r.SANITARY_AREA, r.SANITARY_PLACE,
 r.OBSERVATION, r.SUGGESTION, d.DEPARTMENT_NAME, p.service_no, 
 (p.P_FIRST_NAME ||' '|| p.P_MIDDLE_NAME || ' ' ||p.P_LAST_NAME) as name, t.trade_name, h.hospital_name
from SANITARY_ROUND r
left outer join mas_HOSPITAL H ON h.hospital_id = r.hospital_id
left outer join mas_department d ON d.department_id = r.department_id
left outer join patient p on p.hin_id=r.hin_id
left outer join mas_trade t on p.TRADE_ID = t.TRADE_ID
where r.id=$P{sanitaryId}]]></queryString>

