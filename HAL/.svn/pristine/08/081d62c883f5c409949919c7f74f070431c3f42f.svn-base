unit.unit_name,loaninm.supplier_id
 FROM store_loanin_t loanint
left join store_loanin_m loaninm on loanint.loanin_master_id=loaninm.loanin_master_id
left join mas_store_supplier supplier on loaninm.supplier_id = supplier.supplier_id
left join mas_store_item item on loanint.item_id=item.item_id
left join mas_store_item_conversion cnvr on item.item_conversion_id=cnvr.item_conversion_id
left join mas_store_unit unit on unit.unit_id = cnvr.purchase_unit_id
left join store_po_header po on loaninm.po_id=po.po_id WHERE
 loaninm.loanin_date >= $P{fromDate} and loaninm.loanin_date <= $P{toDate} $P!{qryStr}]]></queryString>

