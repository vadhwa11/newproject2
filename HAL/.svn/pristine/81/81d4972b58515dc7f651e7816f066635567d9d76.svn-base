	<queryString><![CDATA[Select
rcrh.request_no, rcrh.request_date, msi.pvms_no,msi.nomenclature,rcrd.req_qty,
msic.item_unit_name as AU, md.department_name AS from_dept, mic.classification ,
md1.department_name AS to_dept, rcrd.from_departments

FROM
rc_request_header rcrh
Left Outer join rc_request_details rcrd ON rcrh.id=rcrd.header_id
Left outer join mas_store_item msi ON msi.item_id=rcrd.item_id
Left outer join mas_store_item_conversion msic ON msic.item_conversion_id=msi.item_conversion_id
LefT outer join mas_department md ON rcrh.department_id = md.department_id
LefT outer join mas_department md1 ON rcrh.to_department = md1.department_id
Left outer join mas_item_classification mic ON mic.id= msi.item_classification_id

Where rcrd.header_id=$P{header_id}]]></queryString>

