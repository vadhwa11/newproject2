	<queryString><![CDATA[select 
service_no, full_name,age, pfd.sortie_day, visual_inspect,
r.rank_name, s.administrative_sex_name, u.unit_name, h.hospital_name,pfh.flight_date,pfh.flight_time
from av_pre_flight_dt pfd

left outer join mas_rank r on pfd.rank_id=r.rank_id
left outer join mas_administrative_sex s on pfd.sex_id=s.administrative_sex_id
left outer join mas_unit u on pfd.unit_id=u.unit_id
left outer join av_pre_flight pfh on pfh.pre_flight_id=pfd.flight_hd_id
left outer join mas_hospital h on pfh.hospital_id=h.hospital_id
where pfh.pre_flight_id = $P{avAccidentId}

]]></queryString>

