	<queryString><![CDATA[SELECT  MAS_HOSPITAL.HOSPITAL_NAME AS HOSPITAL_NAME,
     MAS_HOSPITAL.ADDRESS AS ADDRESS,
     accom_registration.`registration_date` AS accom_registration_registration_date,
     accom_registration.`registration_time` AS accom_registration_registration_time,
     accom_registration.`service_person_name` AS accom_registration_service_person_name,
     mas_rank.`rank_name` AS accom_registration_rank_id,
     mas_trade.`trade_name` AS accom_registration_trade_id,
     accom_registration.`reg_type` AS accom_registration_reg_type,
     mas_unit.`unit_name` AS accom_registration_unit_id,
     accom_registration.`remarks` AS accom_registration_remarks
FROM
  `accom_registration` accom_registration 
   LEFT OUTER JOIN `mas_hospital` mas_hospital ON   accom_registration.HOSPITAL_ID = mas_hospital.hospital_id
   left outer join `mas_rank` mas_rank on mas_rank.`rank_id` = accom_registration.`rank_id`
   left outer join `mas_trade` mas_trade on mas_trade.`trade_id` = accom_registration.`trade_id`
   left outer join `mas_unit` mas_unit on mas_unit.`unit_id` = accom_registration.`unit_id` 
WHERE
    accom_registration.`registration_date` between  $P{fromdate} and $P{todate}
     and accom_registration.`department_id` = $P{departId} and accom_registration.`reg_type` ='o']]></queryString>

