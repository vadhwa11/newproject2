	<queryString><![CDATA[select present_advice, disposal, Mh_Run, Systemic_Disorder, Special_Advise_Followup, opd.disposal_days,
opd.date_of_next_review
from opd_patient_details opd
left outer join opd_patient_history his on opd.id=his.opd_patient_details_id
left outer join visit v on opd.visit_id=v.visit_id
left outer join patient p on v.hin_id=p.hin_id
left outer join mas_disposal d on v.disposal_id=d.disposal_id

WHERE
    v.visit_id = $P{visitId} and p.service_no=$P{serviceNo}]]></queryString>

