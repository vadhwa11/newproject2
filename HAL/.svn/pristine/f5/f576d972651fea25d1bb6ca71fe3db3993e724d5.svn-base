	<queryString><![CDATA[SELECT
     mas_complaint_register.`complaint_no` AS mas_complaint_register_complaint_no,
     mas_complaints_type.`complaint_type_name` AS mas_complaints_type_complaint_type_name,
     mas_complaint_register.`complaint_date` AS mas_complaint_register_complaint_date,
     mas_complaint_register.`completion_date` AS mas_complaint_register_completion_date,
     mas_complaint_register.`allocated_to` AS mas_complaint_register_allocated_to,
     mas_complaint_register.`docket_no` AS mas_complaint_register_docket_no
FROM
     `mas_complaint_register` mas_complaint_register LEFT OUTER JOIN `mas_complaints_type` mas_complaints_type ON mas_complaint_register.`complaint_type` = mas_complaints_type.`complaint_type_id`
WHERE
     mas_complaint_register.`complaint_date` between $P{fromDate} and $P{toDate}
     and mas_complaint_register.status = 'c'
     and mas_complaint_register.completion_date <= mas_complaint_register.complaint_date + 2
     $P!{stringvariable}
     $P!{QUERY}]]></queryString>

