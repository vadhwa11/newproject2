	<queryString><![CDATA[SELECT (select hospital.hospital_name from hospital where hospital_id = accom_registration.hospital_id )as hospital_name,
     accom_allotment.`allotment_no` AS allotment_no,
     mas_rank.`rank_name` AS rank_name,
     mas_unit.`unit_name` AS unit_name,
     mas_unit.`unit_address` AS unit_address,
     accom_registration.`registration_date` AS registration_date,
     accom_registration.`reg_status` AS status,
     smq_vacation.`vacation_date` AS smq_vacation_vacation_date,
     accom_allotment.`allot_type` AS accom_allotment_allot_type
FROM
     `accom_registration` accom_registration RIGHT OUTER JOIN `accom_allotment` accom_allotment ON accom_registration.`accom_id` = accom_allotment.`accom_id`
     RIGHT OUTER JOIN `mas_rank` mas_rank ON accom_allotment.`rank_id` = mas_rank.`rank_id`
     AND mas_rank.`rank_id` = accom_registration.`rank_id`
     RIGHT OUTER JOIN `mas_unit` mas_unit ON accom_registration.`unit_id` = mas_unit.`unit_id`
     AND mas_unit.`unit_id` = accom_registration.`unit`
   left outer JOIN `smq_vacation` smq_vacation ON accom_allotment.`allotment_id` = smq_vacation.`allotment_id`]]></queryString>

