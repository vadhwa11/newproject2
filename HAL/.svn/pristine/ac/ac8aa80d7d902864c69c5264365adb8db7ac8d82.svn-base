	<parameter name="number" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="path" isForPrompting="false" class="java.lang.String"/>
	<parameter name="hinId" isForPrompting="false" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select 
 p.service_no AS patient_service_no,
(isnull(p.s_first_name, '') +isnull(p.s_middle_name,'') +isnull(p.s_last_name,''))AS employee_name,
(isnull(p.p_first_name, '') +isnull(p.p_middle_name,'') +isnull(p.p_last_name,''))AS patient_name,
isnull(empp.department_no,'')+'/'+isnull(p.service_no,'') AS department_number,
rel.relation_name AS relation_name,
hos.hospital_name AS hospital_name,
nis_no,
(emp.first_name+ ' ' +emp.middle_name+ ' ' +emp.last_name)AS doctor ,
(item.nomenclature) as Particulars,
item.pvms_no,
ppd.total as Qty, 
isnull(ppd.qty_issued,0) as issued,

spi.issue_date,
(iemp.first_name+ ' ' +iemp.middle_name+ ' ' +iemp.last_name) as pharmacist,
pph.prescription_no as slip_no,
pph.issued_date
from patient_prescription_details ppd
inner join patient_prescription_header pph on ppd.prescription_id = pph.prescription_id
LEFT OUTER JOIN patient p	on pph.hin_id = p.hin_id
LEFT OUTER JOIN visit v	on pph.visit_id = v.visit_id
LEFT OUTER JOIN mas_department dep ON v.department_id = dep.department_id
left outer join mas_store_item item on item.item_id= ppd.item_id
left outer join mas_item_classification c on item.item_classification_id= c.id
LEFT OUTER JOIN mas_employee emp ON pph.emp_id = emp.employee_id   
LEFT OUTER JOIN mas_employee empp ON p.employee_id = empp.employee_id   
LEFT OUTER JOIN users ui ON pph.issued_by = ui.user_id 
LEFT OUTER JOIN mas_employee iemp ON ui.employee_id = iemp.employee_id   
LEFT OUTER JOIN mas_relation rel ON p.relation_id = rel.relation_id
LEFT OUTER JOIN mas_hospital hos ON v.hospital_id = hos.hospital_id  
LEFT OUTER JOIN store_op_patient_issue_m spi ON spi.prescription_id = ppd.prescription_id   
where  pph.dispensary_issue_no = $P{number} and nis_no is not null  and ppd.total>0 and c.id=1 and p.hin_id = $P{hinId}
group by p.service_no,
(isnull(p.s_first_name, '') +isnull(p.s_middle_name,'') +isnull(p.s_last_name,'')),
(isnull(p.p_first_name, '') +isnull(p.p_middle_name,'') +isnull(p.p_last_name,'')),
empp.department_no,
rel.relation_name,
hos.hospital_name,
(emp.first_name+ ' ' +emp.middle_name+ ' ' +emp.last_name),
item.nomenclature,
item.pvms_no,
ppd.total,
ppd.qty_issued,
spi.issue_date,
spi.last_chg_by,
pph.prescription_no,
pph.issued_date,
(iemp.first_name+ ' ' +iemp.middle_name+ ' ' +iemp.last_name),nis_no]]></queryString>

