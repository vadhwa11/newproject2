	<parameter name="toStore" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select a.sr_no,b.pvms_no,b.nomenclature,b.item_id,d.item_unit_name,
IFNULL((select st.qty_request from store_internal_indent_t st where st.internal_id = $P{internalIndentId} and st.item_id = a.item_id),0)
 as qty_requested, a.batch_no,
 (SELECT sum(s.qty_issued) FROM store_issue_t s, store_issue_m m where s.item_order = '2'
and  s.item_id = a.item_id  and s.issue_m_id = m.id and m.issue_type = 'i' and m.status = 'o'
and m.to_store= $P{toStore}
group by s.item_id) as loan_out_qty,
((IFNULL((select st.qty_request from store_internal_indent_t st where st.internal_id = $P{internalIndentId} and st.item_id = a.item_id),0))-
(IFNULL((SELECT sum(s.qty_issued) FROM store_issue_t s, store_issue_m m where s.item_order = '2'
and  s.item_id = a.item_id  and s.issue_m_id = m.id and m.issue_type = 'i' and m.status = 'o'
and m.to_store= $P{toStore}
group by s.item_id),0))) as balance,



 a.remarks,concat('O I/C ',d1.department_name,'\n CHAF BANGALORE') as from_department,
  concat('O I/C ',d2.department_name,'\n CHAF BANGALORE') as to_department



from store_issue_t a, mas_store_item b, mas_store_item c, mas_store_item_conversion d,store_internal_indent_m e,store_internal_indent_t f,
store_issue_m g


LEFT OUTER JOIN mas_department d1 ON g.department_id = d1.department_id

LEFT OUTER JOIN mas_department d2 ON g.to_store = d2.department_id

where item_order = '2'
and f.internal_id =  $P{internalIndentId}
and g.to_store =  $P{toStore}
and a.item_issued = b.item_id
and b.item_conversion_id = d.item_conversion_id
and a.item_id = c.item_id
and a.issue_m_id=g.id
and e.id = f.internal_id

group by a.item_id
order by item_order ;]]></queryString>

