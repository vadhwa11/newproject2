	<parameter name="opdvid" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     visit.visit_no AS visit_visit_no,
     visit.visit_date AS visit_visit_date,
     opd_oncology.stage_n AS opd_oncology_stage_n,
     opd_oncology.stage_t AS opd_oncology_stage_t,
     opd_oncology.stage_m AS opd_oncology_stage_m,
  ----   (case opd_oncology.stage_nor when 1 then 'I' when 2 then 'II' when 3 then 'III' when 4 then 'IV' else opd_oncology.stage_nor end)AS opd_oncology_stage_nor,
     opd_oncology.stage_a AS opd_oncology_stage_a,
     opd_oncology.stage_b AS opd_oncology_stage_b,
     opd_oncology.surgery_onco AS opd_oncology_surgery_onco,
     opd_oncology.medical_onco AS opd_oncology_medical_onco,
     opd_oncology.rt AS opd_oncology_rt,
     opd_oncology.remarks AS opd_oncology_remarks,
     (patient.p_first_name||patient.p_middle_name||patient.p_last_name)AS patient_name,
     patient.date_of_birth AS patient_date_of_birth,
     patient.service_no AS patient_service_no,
     patient.hin_no AS patient_hin_no,
     mas_department.department_name AS department_name,
     mas_hospital.hospital_name AS hospital_name,
     patient.age AS patient_age,
     opd_patient_details.plan AS details_plan,
     mas_icd.icd_name AS mas_icd_icd_name,
     patient_investigation_details.quantity AS details_quantity,
     patient_prescription_details.dosage AS prescription_details_dosage,
     patient_prescription_details.no_of_days AS prescrip_no_of_days,
    patient_prescription_details.total AS prescription_details_total,
    patient_prescription_details.type AS prescription_details_type,
     patient_prescription_header.remarks AS prescription_header_remarks,
    patient_investigation_header.clinical_notes AS header_clinical_notes,
    mas_store_item.nomenclature AS item_nomenclature,
    mas_charge_code.charge_code_name AS charge_code_name,
   mas_frequency.frequency_name AS frequency_name
FROM
     visit visit RIGHT OUTER JOIN opd_oncology opd_oncology ON visit.visit_id = opd_oncology.visit_id
     LEFT OUTER JOIN patient patient ON opd_oncology.hin_id = patient.hin_id
     LEFT OUTER JOIN mas_department mas_department ON opd_oncology.department_id = mas_department.department_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON opd_oncology.hospital_id = mas_hospital.hospital_id
     LEFT OUTER JOIN patient_prescription_header patient_prescription_header ON patient.hin_id = patient_prescription_header.hin_id
     LEFT OUTER JOIN patient_investigation_header patient_investigation_header ON patient.hin_id = patient_investigation_header.hin_id
     LEFT OUTER JOIN discharge_icd_code discharge_icd_code ON patient.hin_id = discharge_icd_code.hin_id
     AND visit.visit_id = discharge_icd_code.visit_id
     LEFT OUTER JOIN mas_icd mas_icd ON discharge_icd_code.icd_id = mas_icd.icd_id
     AND visit.visit_id = patient_investigation_header.visit_id
     AND visit.visit_id = patient_prescription_header.visit_id
     LEFT OUTER JOIN opd_patient_details opd_patient_details ON visit.visit_id = opd_patient_details.visit_id
     LEFT OUTER JOIN patient_prescription_details patient_prescription_details ON patient_prescription_header.prescription_id = patient_prescription_details.prescription_id
     LEFT OUTER JOIN mas_store_item mas_store_item ON patient_prescription_details.item_id = mas_store_item.item_id
     LEFT OUTER JOIN mas_frequency mas_frequency ON patient_prescription_details.frequency_id = mas_frequency.frequency_id
     LEFT OUTER JOIN patient_investigation_details patient_investigation_details ON patient_investigation_header.investigation_header_id = patient_investigation_details.investigation_header_id
     LEFT OUTER JOIN mas_charge_code mas_charge_code ON patient_investigation_details.charge_code_id = mas_charge_code.charge_code_id
WHERE
     opd_oncology.visit_id = $P{opdvid}
]]></queryString>

