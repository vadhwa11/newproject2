	<queryString><![CDATA[SELECT
     store_amc_m.`serial_no` AS store_amc_m_serial_no,
     store_amc_t.`adv_bill_no` AS store_amc_t_adv_bill_no,
     store_amc_t.`adv_bill_amount` AS store_amc_t_adv_bill_amount,
     store_amc_t.`balance_bill_no` AS store_amc_t_balance_bill_no,
     store_amc_t.`balance_bill_date` AS store_amc_t_balance_bill_date,
     store_amc_t.`balance_bill_amount` AS store_amc_t_balance_bill_amount,
     store_amc_t.`remarks` AS store_amc_t_remarks,
     store_amc_t.`adv_bill_date` AS store_amc_t_adv_bill_date,
     mas_department.`department_name` AS mas_department_department_name,
     mas_store_item.`pvms_no` AS mas_store_item_pvms_no,
     mas_store_item.`nomenclature` AS mas_store_item_nomenclature,
     store_amc_m.`tot_received_qty` AS store_amc_m_tot_received_qty,
     mas_store_item_conversion.`item_unit_name` AS mas_store_item_conversion_item_unit_name,
     mas_company.`company_name` AS mas_company_company_name,
     DATEDIFF(store_amc_t.`amc_end_date`,store_amc_t.`amc_start_date`)AS AMC_Period,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `store_amc_t` store_amc_t RIGHT OUTER JOIN `store_amc_m` store_amc_m ON store_amc_t.`amc_m_id` = store_amc_m.`id`
     LEFT OUTER JOIN `mas_department` mas_department ON store_amc_m.`department_id` = mas_department.`department_id`
     LEFT OUTER JOIN `mas_store_item` mas_store_item ON store_amc_m.`item_id` = mas_store_item.`item_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON store_amc_m.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `mas_store_item_conversion` mas_store_item_conversion ON mas_store_item.`item_conversion_id` = mas_store_item_conversion.`item_conversion_id`
     LEFT OUTER JOIN `mas_company` mas_company ON store_amc_t.`amc_company_id` = mas_company.`company_id`

WHERE store_amc_m.`department_id` = $P{DEPART}
AND   $P{PVMS_NO} = 0]]></queryString>

