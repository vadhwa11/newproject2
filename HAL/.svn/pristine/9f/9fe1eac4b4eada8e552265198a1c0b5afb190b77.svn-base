	<queryString><![CDATA[select 

mmer.serviceno, mmer.name_in_full, r.rank_name, t.trade_name,
(me.first_name||' '||me.last_name) as moName, ra.rank_name as rank1,
me.service_no as serMo,
h.hospital_name

from mas_medical_examination_report mmer

left outer join mas_rank r on mmer.rank_id=r.rank_id
left outer join mas_trade t on mmer.branch_id=t.trade_id
left outer join visit visit on mmer.visit_id=visit.visit_id
left outer join mas_employee me on mmer.forward_mo=me.employee_id
left outer join mas_rank ra on me.rank_id=ra.rank_id
left outer join mas_hospital h on mmer.hospital_id=h.hospital_id

where mmer.visit_id = $P{visit_id}]]></queryString>

