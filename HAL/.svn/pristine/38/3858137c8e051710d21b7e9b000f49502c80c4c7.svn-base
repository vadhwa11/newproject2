	<queryString><![CDATA[select mss.SUPPLIER_NAME
from  store_proforma_header sph
left outer join store_proforma_detail spd on sph.PROFORMA_ID= spd.PROFORMA_ID 
LEFT OUTER JOIN STORE_LOANIN_T SLT ON SPD.CRV_ID=SLT.LOANIN_TRANS_ID
LEFT OUTER JOIN STORE_LOANIN_M SLM ON SLT.LOANIN_MASTER_ID=SLM.LOANIN_MASTER_ID
LEFT OUTER JOIN MAS_STORE_SUPPLIER MSS ON SLM.SUPPLIER_ID=MSS.SUPPLIER_ID
where  sph.PROFORMA_no=$P{proNo} and sph.HOSPITAL_ID=$P{hospitalId}
