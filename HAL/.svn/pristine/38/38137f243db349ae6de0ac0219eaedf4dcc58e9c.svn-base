	<queryString><![CDATA[select c.pvms_no,c.nomenclature,concat('[',c.`pvms_no`,'] ',
c.`nomenclature`) AS mas_store_item_nomenclature,
b.mmf_in_qty as 'req MMF',a.mmf_for_the_year,a.mmf_store_type,d.department_name,
b.curr_year_mmf as 'appr MMF',e.item_unit_name,b.Curr_year_mmf as current,
b.prev_year_mmf as previous,
sum(if(a.mmf_for_the_year=$P{mmfForTheYear}-1,b.curr_year_mmf,0)) as 'prev year',
sum(if(a.mmf_for_the_year=$P{mmfForTheYear},b.curr_year_mmf,0)) as Appr1,f.unit_name ,r.remark
from
store_mmf_department_m a,  store_mmf_department_t b, mas_store_item c left join 
store_mmf_item_remarks r on r.item_id=c.item_id ,mas_department d,
mas_store_item_conversion e,mas_store_unit f,mas_item_type i
where a.id = b.store_mmf_department_m_id and e.item_conversion_id=c.item_conversion_id and
f.unit_id = e.purchase_unit_id and
a.store_ward_dept=d.department_id 
and b.item_id = c.item_id
and c.item_type_id = i.item_type_id
and a.mmf_store_type=$P{storeType}
and (r.mmf_store_type=$P{storeType} or r.mmf_store_type is null)
and c.status = 'y' and c.department_id = '24'
and i.item_type_id = $P{item_type}
group by b.item_id
having Appr1 >0
order by c.pvms_no]]></queryString>

