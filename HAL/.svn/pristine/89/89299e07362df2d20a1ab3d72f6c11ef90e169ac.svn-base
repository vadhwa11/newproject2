	<parameter name="INDENT_NO" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<parameter name="DEPART" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     store_indent_t.SERIAL_NO as srNo,	
     store_indent_m.indent_no AS store_indent_m_indent_no,
     store_indent_m.indent_date AS store_indent_m_indent_date,
     store_indent_t.qty_in_mmf AS store_indent_t_qty_in_mmf,
     store_indent_t.stock_in AS store_indent_t_stock_in,
     store_indent_t.qty_in_demand AS store_indent_t_qty_in_demand,
     store_indent_t.unit_rate AS unit_rate,
     store_indent_t.remarks AS remarks,
     store_indent_t.total_cost AS store_indent_t_total_cost,
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
     msu.unit_name AS item_conversion_item_unit_name,
     store_indent_m.required_form AS store_indent_m_required_form,
     store_indent_m.nrs AS store_indent_m_nrs,
     store_indent_m.patient_details AS store_indent_m_patient_details,
     store_indent_m.authority AS store_indent_m_authority,
     store_indent_m.section AS store_indent_m_section,
     store_indent_m.SELF_LIFE as self_life,
     store_indent_m.CODE_HEAD as code_head,
     store_indent_m.INDENT_OPTION,
     CASE 
          WHEN  store_indent_m.INDENT_OPTION='1' then 'Emergent'
         WHEN store_indent_m.INDENT_OPTION='2' then '3 Months'
         
           ELSE '6 Months'
     END AS "Inndet_type" ,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     mas_hospital.ADDRESS as hos_Add,
     msafd.AIR_FORCE_DEPOT_NAME as supplied_by
FROM
     store_indent_m store_indent_m RIGHT OUTER JOIN store_indent_t store_indent_t ON store_indent_m.indent_id = store_indent_t.indent_id
     LEFT OUTER JOIN mas_store_item mas_store_item ON store_indent_t.item_id = mas_store_item.item_id
     LEFT OUTER JOIN mas_store_item_conversion mas_store_item_conversion ON mas_store_item.item_conversion_id = mas_store_item_conversion.item_conversion_id
left outer join mas_store_unit msu on msu.unit_id=mas_store_item_conversion.purchase_unit_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON store_indent_m.hospital_id = mas_hospital.hospital_id
     left outer join mas_store_air_force_depot msafd on store_indent_m.SUPPLIED_BY= msafd.AIR_FORCE_DEPOT_ID

WHERE
     store_indent_m.indent_no = $P{INDENT_NO}
 AND store_indent_m.indent_type = 'd'
 AND store_indent_m.department_id = $P{DEPART}
and store_indent_m.hospital_id = $P{hospitalId}
order by store_indent_t.SERIAL_NO]]></queryString>

