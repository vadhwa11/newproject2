	<parameter name="entryNo" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     mas_department_A.`department_name` As dispatched_by_department_name,
     concat(mas_employee.`first_name`," ",
		mas_employee.`middle_name`," ",
		mas_employee.`last_name`)as specimenReceivedBy,
     ot_human_body_disposal.`entry_no` AS ot_human_body_disposal_entry_no,
     ot_human_body_disposal.`clinical_notes` AS ot_human_body_disposal_clinical_notes,
     ot_human_body_disposal.`time_of_dispatch` AS ot_human_body_disposal_time_of_dispatch,
     concat(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`)as patientName,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     ot_human_body_disposal.`tissue_organ` AS ot_human_body_disposal_tissue_organ,
     concat(patient.`s_first_name` ," ",
     patient.`s_middle_name` ," ",
     patient.`s_last_name`)as servicePersonName,
     patient.`hin_no` AS patient_hin_no,
     patient.`service_no` AS patient_service_no,
    ot_human_body_disposal.`dispatch_date` AS ot_human_body_disposal_dispatch_date
	
FROM
    `patient` patient RIGHT JOIN `ot_human_body_disposal` ot_human_body_disposal ON patient.`hin_id` = ot_human_body_disposal.`hin_id`
     LEFT JOIN `mas_employee` mas_employee ON ot_human_body_disposal.`specimen_recieved_by` = mas_employee.`employee_id`
     LEFT JOIN `mas_department` mas_department_A ON ot_human_body_disposal.`specimen_dispatched_by` = mas_department_A.`department_id`
     LEFT JOIN `mas_hospital` mas_hospital ON ot_human_body_disposal.`hospital_id` = mas_hospital.`hospital_id`
where
	ot_human_body_disposal.`entry_no` = $P{entryNo}]]></queryString>

