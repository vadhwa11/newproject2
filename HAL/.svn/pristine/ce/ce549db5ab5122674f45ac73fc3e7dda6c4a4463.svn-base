	<queryString><![CDATA[SELECT
     inpatient.init_diagnosis AS inpatient_init_diagnosis,
     inpatient.ad_no AS inpatient_ad_no,
     inpatient.inpatient_id AS inpatient_id,
     inpatient.date_of_addmission AS inpatient_date_of_addmission,
     inpatient.time_of_addmission AS inpatient_time_of_addmission,
     inpatient.list_date AS list_date,
     inpatient.list_time AS list_time,
     patient.service_no AS patient_service_no,
     (patient.s_first_name ||' '||patient.s_middle_name||' '||patient.s_last_name)AS patient_s_name,
     mas_department.department_name AS mas_department_department_name,
     mas_relation.relation_name AS mas_relation_relation_name,
     mas_unit.unit_name AS mas_unit_unit_name,
     mas_rank.rank_name AS mas_rank_rank_name,
     mas_service_type.service_type_name AS service_type_name,
     patient.age AS patient_age,
     patient.sex_id AS patient_sex_id,
     mas_administrative_sex.administrative_sex_name AS administrative_sex_name,
     sil_dil_status.last_chg_date AS sil_dil_status_last_chg_date,
     sil_dil_status.last_chg_time AS sil_dil_status_last_chg_time,
     hospital.hospital_name as hospital_name
FROM
     mas_rank mas_rank LEFT OUTER JOIN patient patient ON mas_rank.rank_id = patient.rank_id
     LEFT OUTER JOIN inpatient inpatient ON patient.hin_id = inpatient.hin_id
     LEFT OUTER JOIN mas_unit mas_unit ON patient.unit_id = mas_unit.unit_id
     LEFT OUTER JOIN mas_relation mas_relation ON patient.relation_id = mas_relation.relation_id
     LEFT OUTER JOIN mas_service_type mas_service_type ON patient.service_type_id = mas_service_type.service_type_id
     LEFT OUTER JOIN mas_administrative_sex mas_administrative_sex ON patient.sex_id = mas_administrative_sex.administrative_sex_id
     LEFT OUTER JOIN mas_department mas_department ON inpatient.department_id = mas_department.department_id
     LEFT OUTER JOIN sil_dil_status sil_dil_status ON inpatient.inpatient_id = sil_dil_status.inpatient_id
     left outer join mas_hospital hospital on patient.hospital_id=hospital.hospital_id
WHERE
     inpatient.condition_status = $P{Sil_Or_Dil}
     and inpatient.ad_status = 'A'
     and sil_dil_status.last_chg_time = inpatient.list_time
     and sil_dil_status.last_chg_date = inpatient.list_date
    group by   inpatient.ad_no,inpatient.inpatient_id,patient.service_no,inpatient.date_of_addmission, inpatient.time_of_addmission,inpatient.list_date,inpatient.list_time,
    (patient.s_first_name ||' '||patient.s_middle_name||' '||patient.s_last_name),mas_department.department_name,inpatient.init_diagnosis, mas_relation.relation_name ,
     mas_unit.unit_name ,
     mas_rank.rank_name,
     mas_service_type.service_type_name,
     patient.age ,
     patient.sex_id ,
     mas_administrative_sex.administrative_sex_name,
     sil_dil_status.last_chg_date ,
     sil_dil_status.last_chg_time,  hospital.hospital_name
    
ORDER BY
     inpatient.date_of_addmission ASC]]></queryString>

