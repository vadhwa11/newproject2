	<queryString><![CDATA[select 
case when disabilitybefore='y' then 'Yes' else 'No' end as disabilitybefore,
case when med_exam_carry_time ='y' then 'Yes' else 'No' end as med_exam_carry_time,
case when disable_aggrav_persist ='y' then 'Yes' else 'No' end as disable_aggrav_persist,
case when aggrav_material_period  ='y' then 'Yes' else 'No' end as aggrav_material_period,
case when individual_misconduct ='y' then 'Yes' else 'No' end as individual_misconduct,
case when nvl(in_way,'') is null then '' else ('(a) '||nvl(in_way,'')) end as in_way,
case when nvl(in_way2,'') is null then '' else ('(a) '||nvl(in_way2,'')) end as in_way2 ,
case when aggravated_misconduct ='y' then 'Yes' else 'No' end as aggravated_misconduct,
case when nvl(percentage_disable ,'') is null then '' else ('(b) '||nvl(percentage_disable ,'')) end as percentage_disable,
case when refuse_operation_treat  ='y' then 'Yes' else 'No' end as refuse_operation_treat,
individual_reason,
case when reduction_disable_pension ='y' then 'Yes' else 'No' end as reduction_disable_pension ,
reduc_disable_pension_entry,
case when operation_treat_cured  ='a' then 'Affirmative' else 'Negative' end as operation_treat_cured, 
case when nvl(percentage_disable_treatment,'') is null then '' else (nvl(percentage_disable_treatment,'')||' %') end as percentage_disable_treatment,
case when individual_refusal_tratment='y' then 'Yes' else 'No' end as individual_refusal_tratment ,
opera_treatment_recommend,
case when individual_further_tratment ='y' then 'Yes' else 'No' end as individual_further_tratment  ,
individual_nature,
case when nvl(treatment_year ,'') is null then '' else (nvl(treatment_year ,'')||'Years') end as treatment_year,
case when nvl(treatment_month,'') is null then '' else (nvl(treatment_month,'')||'Months') end as treatment_month,
case when individual_attendent ='y' then 'Yes' else 'No' end as individual_attendent
from mas_medical_examination_report p 
where medical_examination_id=$P{medExamId}]]></queryString>

