	<queryString><![CDATA[SELECT
     inpatient.`ad_no` AS adNo,
     inpatient.`inpatient_id` AS inpatient_id,
     mas_unit.`unit_name` AS unitName,
     mas_relation.`relation_name` AS relationName,
     mas_trade.`trade_name` AS tradeName,
     concat(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)AS sName,
     patient.`service_years` AS serviceYears,
  patient.`total_service_period` AS period,
     mas_admission_type.`admission_type_name` AS admissionTypeName,
     (case inpatient.`ad_status` when "D" then DATE_FORMAT(inpatient.`discharge_date`, '%d-%b-%Y') else 'still in hospital' end) as dateOfDischarge,
     mas_rank.`rank_name` AS rankName,
     inpatient.`date_of_addmission` AS dateOfAddmission,
	inpatient.`previous_ad_no` AS previous_ad_no,
     inpatient.`age` AS age,
     patient.`address` AS address,
     patient.`service_no` AS serviceNo,
     mas_service_type.`service_type_name` AS serviceTypeName,
     mas_diagnosis_conclusion.`diagnosis_conclusion_name` AS mas_diagnosis_conclusion_diagnosis_conclusion_name,
     mas_record_office_address.`address` AS mas_record_office_address_address,
     concat("Pt Dt :",mas_district.`district_name`) AS district_name,
     concat("Discharged to ",mas_disposed_to.`disposed_to_name`) AS disposed_to
FROM
`inpatient` inpatient
     LEFT JOIN `patient` patient ON inpatient.`hin_id` = patient.`hin_id`
     LEFT JOIN `mas_trade` mas_trade ON   patient.`trade_id`=mas_trade.`trade_id`
     LEFT JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
     LEFT JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`

     LEFT JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT JOIN `mas_service_type` mas_service_type ON patient.`service_type_id` = mas_service_type.`service_type_id`
     LEFT JOIN `mas_diagnosis_conclusion` mas_diagnosis_conclusion ON inpatient.`diagnosis_id` = mas_diagnosis_conclusion.`diagnosis_conclusion_id`
     LEFT JOIN `mas_admission_type` mas_admission_type ON inpatient.`admission_type_id` = mas_admission_type.`admission_type_id`
    LEFT OUTER JOIN `mas_record_office_address` mas_record_office_address ON inpatient.`record_office_address_id` = mas_record_office_address.`record_office_address_id`
 LEFT  JOIN `mas_district` mas_district ON patient.`district_id` = mas_district.`district_id`
LEFT  JOIN `discharge` discharge ON inpatient.`ad_no` = discharge.`ad_no`
LEFT  JOIN `mas_disposed_to` mas_disposed_to ON mas_disposed_to.`disposed_to_id` = discharge.`disposed_to_id`

where  inpatient.`date_of_addmission` between $P{fromDate} and $P{toDate} $P!{serviceType}
order by concat(inpatient.`date_of_addmission`,inpatient.`time_of_addmission`)]]></queryString>

