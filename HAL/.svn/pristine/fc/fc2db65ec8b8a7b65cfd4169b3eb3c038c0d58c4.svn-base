	</parameter>
	<queryString><![CDATA[select
     ipd_intake.intakeoutput_id AS intakeoutput_id,
     ipd_intake.ip_intake_date AS ipd_intake_date,
     ipd_intake.time AS ipd_intake_time,
     ipd_intake.oral AS ipd_intake_oral,
     case when (ipd_intake.intake is null) then '0' else ipd_intake.intake end AS ipd_intake_intake,
 (select
    sum(cast(ipd_intake.intake as int))
    from ipd_intake_output_chart ipd_intake_output_chart right join ipd_intake ipd_intake on ipd_intake_output_chart.intakeoutput_id = ipd_intake.intakeoutput_id
left outer join inpatient i on i.inpatient_id = ipd_intake_output_chart.inpatient_id
WHERE
     i.inpatient_id = $P{adNo}
    group by ipd_intake_output_chart.ad_no) as Oral_Total,
ipd_intake.iv iv,
     (ipd_intake.iv_count) AS IV_Count,
(select
    sum(cast(ipd_intake.iv_count as int))
    from ipd_intake_output_chart ipd_intake_output_chart right join ipd_intake ipd_intake on ipd_intake_output_chart.intakeoutput_id = ipd_intake.intakeoutput_id
left outer join inpatient i on i.inpatient_id = ipd_intake_output_chart.inpatient_id
WHERE
     i.inpatient_id = $P{adNo}
    group by ipd_intake_output_chart.ad_no) as iv_total,
     ipd_intake.remarks as ipd_intake_remarks,intake_type,intake_total
from
     ipd_intake_output_chart ipd_intake_output_chart 
left outer join ipd_intake ipd_intake on ipd_intake_output_chart.intakeoutput_id = ipd_intake.intakeoutput_id
left outer join inpatient i on i.inpatient_id = ipd_intake_output_chart.inpatient_id
WHERE
     i.inpatient_id = $P{adNo}]]></queryString>

