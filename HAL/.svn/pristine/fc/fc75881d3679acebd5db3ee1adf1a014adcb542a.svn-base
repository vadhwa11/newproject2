	<queryString><![CDATA[SELECT
     mas_personnel_details.`identification_mark` AS mas_personnel_details_identification_mark,
     mas_personnel_details.`height` AS mas_personnel_details_height,
     mas_personnel_details.`date_of_birth` AS mas_personnel_details_date_of_birth,
     mas_personnel_details.`pass_no` AS mas_personnel_details_pass_no,
     mas_personnel_details.`personnel_name` AS mas_personnel_details_personnel_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     mas_hospital.`hospital_id` AS mas_hospital_hospital_id,
     mas_personnel_details.`hospital_id` AS mas_personnel_details_hospital_id,
     mas_designation.`designation_name` AS mas_designation_designation_name
FROM
     `mas_unit` mas_unit RIGHT OUTER JOIN `mas_personnel_details` mas_personnel_details ON mas_unit.`unit_id` = mas_personnel_details.`unit_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON mas_personnel_details.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `mas_designation` mas_designation ON mas_personnel_details.`designation_id` = mas_designation.`designation_id`
WHERE
     mas_personnel_details.`pass_no` = $P{Passno}
     or mas_personnel_details.`personnel_name` = $P{Name}]]></queryString>

