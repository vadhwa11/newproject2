	<queryString><![CDATA[select     
h.hospital_name,
opd.NAME_OF_CONSTRAC,
opd.location,
opd.remarks,
(mas_employee.first_name || ' ' || mas_employee.middle_name || ' '|| mas_employee.last_name) as employee_name,
opd.quantity
from   opd_patient_details opd 
left join visit v on opd.visit_id = v.visit_id
left join opd_patient_history ph on opd.id = ph.opd_patient_details_id
left join mas_employee mas_employee on opd.issue_to = mas_employee.employee_id
left join mas_hospital h on opd.hospital_id = h.hospital_id
left join patient p on ph.hin_id=p.hin_id
where trunc(opd.opd_date) between $P{fromDate} and $P{toDate} 
and opd.hospital_id=$P{hospitalId}
and v.Reporting_For='FamilyWC'
$P!{qry}]]></queryString>

