<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="BloodDiscardEntry" table="blood_discard_entry">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="native" />
		</id>

		<property name="DiscardNo" column="discard_no" type="string"
			not-null="false" length="5" />
		<property name="DiscardDate" column="discard_date" type="date"
			not-null="false" />
		<property name="Remarks" column="remarks" type="string"
			not-null="false" length="100" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="ApprovedBy" column="approved_by" class="MasEmployee"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
		<many-to-one name="StockDetail" column="stock_detail_id"
			class="BloodStockDetail" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>