	<queryString><![CDATA[SELECT
     visit.visit_no as visit_visit_no,
     patient.service_no as patient_service_no,
     patient.hin_no as patient_hin_no,
     mas_store_item.nomenclature as mas_store_item_nomenclature,
     mas_store_item.pvms_no as mas_store_item_pvms_no,
     patient_prescription_details.route as patient_prescription_details_r,
     patient_prescription_details.total as patient_prescription_details_t,
     patient_prescription_details.no_of_days as no_of_days,
     patient_prescription_details.dosage as dosage,
     patient_prescription_details.frequency_id as patient_prescription_details_f,
     mas_frequency.frequency_name as frequency_name
FROM
 opd_patient_details opd_patient_details 
 left outer join visit visit on opd_patient_details.visit_id = visit.visit_id
 left outer join patient patient on visit.hin_id=patient.hin_id
 left outer join opd_patient_history opd_patient_history on opd_patient_details.id = opd_patient_history.opd_patient_details_id
 left outer join patient_prescription_header patient_prescription_header on visit.visit_id = patient_prescription_header.visit_id
 left outer join patient_prescription_details patient_prescription_details on patient_prescription_header.prescription_id = patient_prescription_details.prescription_id
 left outer join mas_store_item mas_store_item on patient_prescription_details.item_id = mas_store_item.item_id
 left outer join mas_frequency mas_frequency on patient_prescription_details.frequency_id = mas_frequency.frequency_id
WHERE
    visit.visit_id=$P{visitId}]]></queryString>

