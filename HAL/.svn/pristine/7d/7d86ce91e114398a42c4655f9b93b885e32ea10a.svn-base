	<queryString><![CDATA[select firstsq.*,sndsql.* from
(SELECT count(livingInOut) as cnt,join_date  from mas_employee
 where livingInOut = 1 and mess_id =3 and
month(join_date) = $P{month} and year(join_date) = $P{year}
group by join_date ) as firstsq
left outer join
( SELECT count(livingInOut) as cnt1,join_date as jdate
from mas_employee where livingInOut = 1 and rank_id >= 14 and
month(join_date) = $P{month} and year(join_date) = $P{year}
group by join_date ) as sndsql

on
 firstsq.join_date = sndsql.jdate
union
select firstsq.*,sndsql.* from
(SELECT count(livingInOut) as cnt,join_date  from mas_employee
 where livingInOut = 1 and mess_id =3 and
month(join_date) = $P{month} and year(join_date) = $P{year}
group by join_date ) as firstsq
right outer join
( SELECT count(livingInOut) as cnt1,join_date as jdate
from mas_employee where livingInOut = 1 and rank_id >= 14 and
month(join_date) = $P{month} and year(join_date) = $P{year}
group by join_date ) as sndsql

on
 firstsq.join_date = sndsql.jdate;]]></queryString>

