	<queryString><![CDATA[SELECT
     concat(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)AS patientName,
     inpatient.`discharge_date` AS inpatient_discharge_date,
     inpatient.`date_of_addmission` AS inpatient_date_of_addmission,
     mas_icd.`icd_code` AS mas_icd_icd_code,
     patient.`service_no` AS patient_service_no,
     mas_relation.`relation_name` as relation,
     mas_rank.`rank_name` as rank,
     mas_unit.`unit_name` as unit,
     discharge.`injury_report_initiated_on` as injury_initiated_date,
     inpatient.`inpatient_id` as inpId

FROM
     `patient` patient INNER JOIN `inpatient` inpatient ON patient.`hin_id` = inpatient.`hin_id`
     LEFT JOIN `discharge` discharge ON patient.`hin_id` = discharge.`hin_id`
     LEFT JOIN `discharge_icd_code` discharge_icd_code ON patient.`hin_id` = discharge_icd_code.`hin_id`
     LEFT JOIN `mas_service_type` mas_service_type ON patient.`service_type_id` = mas_service_type.`service_type_id`
     LEFT JOIN `mas_icd` mas_icd ON discharge_icd_code.`icd_id` = mas_icd.`icd_id`
     LEFT JOIN `mas_disposal` mas_disposal ON discharge.`disposal_id` = mas_disposal.`disposal_id`
     LEFT JOIN `mas_diagnosis_conclusion` mas_diagnosis_conclusion ON discharge.`diagnosis_id` = mas_diagnosis_conclusion.`diagnosis_conclusion_id`
     LEFT JOIN `mas_relation` mas_relation ON mas_relation.`relation_id` = patient.`relation_id`
     LEFT JOIN `mas_rank` mas_rank ON mas_rank.`rank_id` = patient.`rank_id`
     LEFT JOIN `mas_unit` mas_unit ON mas_unit.`unit_id` = patient.`unit_id`
LEFT OUTER JOIN `mas_disposed_to` mas_disposed_to ON discharge.`disposed_to_id` = mas_disposed_to.`disposed_to_id`
WHERE inpatient.`ad_status`='D' and  discharge.`injury_report_initiated_on` between $P{fromDate} and $P{toDate} and discharge.`injury_report_initiated_on` is not null
  Group By inpatient.ad_no
order by discharge.`injury_report_initiated_on`]]></queryString>

