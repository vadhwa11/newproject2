	<queryString><![CDATA[SELECT
     mas_personnel_details.`personnel_name` AS mas_personnel_details_personnel_name,
     mas_personnel_details.`pass_no` AS mas_personnel_details_pass_no,
     mas_personnel_details.`father_husband_name` AS mas_personnel_details_father_husband_name,
     mas_personnel_details.`date_of_birth` AS mas_personnel_details_date_of_birth,
     mas_religion.`religion_name` AS mas_religion_religion_name,
     mas_personnel_details.`present_address` AS mas_personnel_details_present_address,
     mas_personnel_details.`appointment_date` AS mas_personnel_details_appointment_date,
     mas_personnel_details.`retirement_date` AS mas_personnel_details_retirement_date,
     pension_form7_details.`gratuity_recieved_military` AS pension_form7_details_gratuity_recieved_military,
     pension_form7_details.`gratuity_recieved_civil_service` AS pension_form7_details_gratuity_recieved_civil_service,
     pension_form7_details.`gratuity_period` AS pension_form7_details_gratuity_period,
     mas_personnel_details.`total_service_without_eol_days` AS mas_personnel_details_total_service_without_eol_days,
     mas_personnel_details.`total_service_without_eol_months` AS mas_personnel_details_total_service_without_eol_months,
     mas_personnel_details.`total_service_without_eol_years` AS mas_personnel_details_total_service_without_eol_years,
     pension_calculation_sheet.`pension_class` AS pension_calculation_sheet_pension_class,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `mas_religion` mas_religion RIGHT OUTER JOIN `mas_personnel_details` mas_personnel_details ON mas_religion.`religion_id` = mas_personnel_details.`religion_id`
     LEFT OUTER JOIN `pension_form7_details` pension_form7_details ON mas_personnel_details.`personnel_id` = pension_form7_details.`personnel_id`
     LEFT OUTER JOIN `pension_calculation_sheet` pension_calculation_sheet ON mas_personnel_details.`personnel_id` = pension_calculation_sheet.`personnel_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON mas_personnel_details.`hospital_id` = mas_hospital.`hospital_id`
WHERE
     mas_personnel_details.`personnel_name` = $P{Name}
     or mas_personnel_details.`pass_no` = $P{Passno}]]></queryString>

