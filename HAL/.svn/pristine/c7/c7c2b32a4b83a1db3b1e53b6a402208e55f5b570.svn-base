	<queryString><![CDATA[select cause.icd_cause_id,cause.icd_cause_name , a.no_of_days from mas_icdcausegrp cause left join
(select m.icd_name, mic.icd_cause_name,
(ip.discharge_date-ip.date_of_addmission) as no_of_days,
dis.z03 from discharge_icd_code dis
left join mas_icd m on dis.icd_id=m.icd_id
left join inpatient ip on dis.inpatient_id=ip.inpatient_id
left join mas_icdcausegrp mic on  m.icd_cause_id=mic.icd_cause_id
where dis.z03='n' and ip.ad_status='D' and mic.icd_cause_name <> ''
and ip.discharge_date between $P{from_date} and $P{to_date}
group by mic.icd_cause_name)as a on cause.icd_cause_name=a.icd_cause_name;]]></queryString>

