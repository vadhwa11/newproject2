	<queryString><![CDATA[SELECT
    
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
f.financial_year,
dep.department_name,
pr_name,m.project,mpr_date,m.mpr_no,t.qty_required,t.estimated_values,t.delivery_date,qty_in_stock,
c.currency_name,a.item_unit_name,po_header.po_date,po_header.po_number,s.supplier_name,t.mpr_qty,t.unit_rate,
pmpr.proposed_m_p_r_name, hash_table_value,hash_table_qty,t.tax,m.delivery_schedule,t.item_range_capacity
FROM
     store_material_purchase_req_m m 
     LEFT OUTER JOIN store_material_purchase_req_t t ON m.id = t.header_id
     LEFT OUTER JOIN mas_store_item mas_store_item ON t.item_id = mas_store_item.item_id
     LEFT OUTER JOIN mas_store_item_conversion a ON a.item_conversion_id = mas_store_item.item_conversion_id
     LEFT OUTER JOIN mpr_priority mpr ON mpr.id = m.mpr_priority_id
     LEFT OUTER JOIN store_po_detail po_det ON po_det.po_detail_id = t.po_detail_id
     LEFT OUTER JOIN store_po_header po_header ON po_header.po_id = po_det.po_id
     LEFT OUTER JOIN mas_department dep ON dep.department_id = m.department_id
     LEFT OUTER JOIN mas_store_financial f ON f.financial_id = m.financial_id
     LEFT OUTER JOIN mas_currency c ON c.currency_id = t.currency_id
     LEFT OUTER JOIN mas_store_supplier s ON s.supplier_id = po_header.supplier_id
	left outer join mas_proposed_m_p_r pmpr on pmpr.proposed_m_p_r_id=m.probase_id
where   m.id  = $P{mrpId}]]></queryString>

