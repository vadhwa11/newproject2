	<queryString><![CDATA[SELECT
     mas_department.department_code AS mas_department_department_code,
     mas_department.department_name AS mas_department_department_name,
    case when mas_department.status ='Y' or mas_department.status ='y' then 'Active' else 'Inactive' end AS mas_department_status,
     mas_department_type.department_type_name AS mas_department_type_department_type_name,
mas_division.division_name,department_no
FROM
mas_department mas_department 
    left outer   JOIN  mas_department_type mas_department_type  ON mas_department_type.department_type_id = mas_department.department_type_id
     LEFT OUTER JOIN mas_division mas_division ON mas_department.division_id = mas_division.division_id
where    mas_department.department_id!=0
$P!{qry}
ORDER BY
     mas_department.department_name ASC]]></queryString>

