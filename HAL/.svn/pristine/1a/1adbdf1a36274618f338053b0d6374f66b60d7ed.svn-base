	<queryString><![CDATA[Select  main.dietdate , main.dietCombinationId,main.diet_combination_name ,main.DTYPE,Sum(main.TOTALVAL) as SCount , mmh.Diet_id  From
( SELECT 'DESS' as DTYPE ,count(*) as TOTALVAL , dt.diet_combination_name ,dt.diet_combination_id as dietCombinationId,concat("",dd.diet_date) as dietdate
FROM hms.diet_details dd
left join hms.mas_diet_combination dt on dd.diet_combination_id=dt.diet_combination_id
where dd.diet_date between '2008-12-03'and '2008-12-04'  group by dd.diet_date,dd.diet_combination_id

Union All

Select  'DEM' as DTYPE , Dem.strength as TOTALVAL, Dem.diet_combination_name ,Dem.diet_combination_id as dietCombinationId ,  DESS.Diet_Date as dietdate From
(SELECT dt.diet_combination_id,dd.diet_date
FROM hms.diet_details dd
left join hms.mas_diet_combination dt on dd.diet_combination_id=dt.diet_combination_id
where dd.diet_date between '2008-12-03'and '2008-12-04'  group by dd.diet_date,dd.diet_combination_id ) as DESS ,
(SELECT dd.strength,dt.diet_combination_name ,dt.diet_combination_id
FROM hms.diet_demand_ration_strength dd ,hms.mas_diet_combination dt,hms.diet_demand_ration_header drh
Where dd.diet_combination_id=dt.diet_combination_id
And dd.diet_demand_ration_header_id=drh.diet_demand_ration_header_id
And (drh.Demand_From_Date between '2008-12-03' and '2008-12-05' ) And (drh.Demand_to_Date between  '2008-12-03'and '2008-12-11')
) as Dem

Where Dem.diet_combination_id = DESS.diet_combination_id ) as main , hms.mas_diet_combination mmh
Where main.dietCombinationID = mmh.diet_Combination_ID
Group By main.dietdate , main.dietCombinationId ,main.DTYPE]]></queryString>

