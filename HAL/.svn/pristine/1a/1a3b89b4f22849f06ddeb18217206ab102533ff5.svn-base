<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="Diagnosispro"
		table="DIAGNOSISPRO"
	>
		<id name="Sno" type="string" column="SNO">
			<generator class="assigned"></generator>
		</id>


		<property
			name="Name"
			column="NAME"
			type="string"
			not-null="false"
		/>
		<property
			name="Signssymptoms"
			column="SIGNSSYMPTOMS"
			type="string"
			not-null="false"
		/>
		<property
			name="Diseaseprogression"
			column="DISEASEPROGRESSION"
			type="string"
			not-null="false"
		/>
		<property
			name="Diagnostictestresults"
			column="DIAGNOSTICTESTRESULTS"
			type="string"
			not-null="false"
		/>
		<property
			name="Associateddiseasesruleouts"
			column="ASSOCIATEDDISEASESRULEOUTS"
			type="string"
			not-null="false"
		/>
		<property
			name="Diseasemechanismclassification"
			column="DISEASEMECHANISMCLASSIFICATION"
			type="string"
			not-null="false"
		/>
		<property
			name="Treatment"
			column="TREATMENT"
			type="string"
			not-null="false"
		/>


	</class>	
</hibernate-mapping>