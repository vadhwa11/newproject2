	<queryString><![CDATA[SELECT
     concat(mas_employee.`first_name`,mas_employee.`middle_name`,mas_employee.`last_name`)AS employee_name,
     mas_employee.`first_name` AS mas_employee_first_name,
     mas_employee.`last_name` AS mas_employee_last_name,
     mas_employee.`middle_name` AS mas_employee_middle_name,
     posted_out_entry.`por_SlNo` AS posted_out_entry_por_SlNo,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     posted_out_entry.`authority` AS posted_out_entry_authority,
     posted_out_entry.`date` AS posted_out_entry_date,
     posted_out_entry.`sors` AS posted_out_entry_sors,
     posted_out_entry.`appraisal_report` AS posted_out_entry_appraisal_report,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_service_type.`service_type_name` AS mas_service_type_service_type_name,
     mas_department.`department_name` AS mas_department_department_name
FROM
     `mas_employee` mas_employee RIGHT OUTER JOIN `posted_out_entry` posted_out_entry ON mas_employee.`employee_id` = posted_out_entry.`employee_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON posted_out_entry.`unit_posted_to` = mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON mas_employee.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_service_type` mas_service_type ON mas_employee.`service_type_id` = mas_service_type.`service_type_id`
     LEFT OUTER JOIN `mas_department` mas_department ON mas_employee.`department_id` = mas_department.`department_id`
WHERE
     mas_employee.`employee_id` = $P{empId} and
mas_employee.`rank_id` = $P{rankId}]]></queryString>

