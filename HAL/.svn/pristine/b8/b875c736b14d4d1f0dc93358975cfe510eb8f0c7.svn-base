<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="AntiMalariaEntry"
		table="ANTI_MALARIA_ENTRY"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="ID"
		>
			<generator class="native"></generator>
		</id>

		<property
			name="NoOfRoundsSpray"
			column="NO_OF_ROUNDS_SPRAY"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="AdpPerProtective"
			column="ADP_PER_PROTECTIVE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="AntiLarvalMeasure"
			column="ANTI_LARVAL_MEASURE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="NoOfAntiMalaria"
			column="NO_OF_ANTI_MALARIA"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="SuppressiveTreatment"
			column="SUPPRESSIVE_TREATMENT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="TypesOfCases"
			column="TYPES_OF_CASES"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="SpecimenOfPlasmodium"
			column="SPECIMEN_OF_PLASMODIUM"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="WeatherInvestigated"
			column="WEATHER_INVESTIGATED"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="RemedicalMeasure"
			column="REMEDICAL_MEASURE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="FlyProofingKitchen"
			column="FLY_PROOFING_KITCHEN"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DisposalRefuseGarbage"
			column="DISPOSAL_REFUSE_GARBAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="FrequencyOfInsecticide"
			column="FREQUENCY_OF_INSECTICIDE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="BloodSlidesExamined"
			column="BLOOD_SLIDES_EXAMINED"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DetailsOfTreatment"
			column="DETAILS_OF_TREATMENT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="AntiFilarialMeasures"
			column="ANTI_FILARIAL_MEASURES"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
		name="DateOfAntiMalaria"
		column="ANTI_MALARIA_ENTRY_DATE"
		type="date"
		not-null="false"
		length="50"
		/>
        <many-to-one name="Hospital_Id" column="hospital_id" class="MasHospital"
         not-null="false">
         </many-to-one>

	</class>	
</hibernate-mapping>