<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PatientDetentionRegister"
		table="PATIENT_DETENTION_REGISTER"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="PATIENT_DETENTION_REGISTER_ID"
		>
			<generator class="increment"/>
		</id>

		<property
			name="DetentionRegisterDate"
			column="DETENTION_REGISTER_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="Remarks"
			column="REMARKS"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="DetainedFrom"
			column="DETAINED_FROM"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="DetainedTo"
			column="DETAINED_TO"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LastChgDate"
			column="LAST_CHG_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="LastChgTime"
			column="LAST_CHG_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Treatment"
			column="TREATMENT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="FromTime"
			column="FROM_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="ToTime"
			column="TO_TIME"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="RequisitionDate"
			column="REQUISITION_DATE"
			type="date"
			not-null="false"
		/>
		<property
			name="ReviewAt"
			column="review_at"
			type="string"
			not-null="false"
			length="10"
		/>
		
		<many-to-one
			name="Visit"
			column="VISIT_ID"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="HIN_ID"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="HOSPITAL_ID"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="LAST_CHG_BY"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="MedicalOfficer"
			column="medical_officer_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="DmaRegister"
			column="dma_register_id"
			class="DmaRegisterHeader"
			not-null="false"
		>
		</many-to-one>
	</class>	
</hibernate-mapping>