	<queryString><![CDATA[select distinct a.ad_no, a.hin_id, a.label,concat(a.last_chg_date,concat('/',a.last_chg_time))as date_,
a.item_reply, b.orderno, c.hin_no,
concat(c.s_first_name,concat(c.s_middle_name,c.s_last_name))AS s_name,
 concat(c.p_first_name,concat(c.p_middle_name,c.p_last_name))AS patient_name,
   mas_relation.relation_name AS relation_name,
      d.rank_name AS RANK_NAME,
     f.discharge_date AS dod,
     f.Date_Of_Addmission as doa,
     f.ad_no as adNo,
     c.age as PATIENT_AGE,
    mas_administrative_sex.administrative_sex_name as sex,
    mas_unit.unit_name as unit_name,
    (select concat(ms.first_name,concat(ms.middle_name,ms.last_name)) from mas_employee ms where ms.employee_id=$P{userId} and rownum<=1)as ename,
(select  mr.rank_name from mas_employee ms,mas_rank mr where ms.employee_id=$P{userId} and mr.rank_id=ms.rank_id and rownum<=1)as rank_name,
(select ms.designation from mas_employee ms where ms.employee_id=$P{userId} and rownum<=1)as design,
(select distinct concat(ms.first_name,concat(ms.middle_name,ms.last_name)) from mas_employee ms,discharge_summary ds where ms.employee_id=ds.emp_id and ds.emp_id=$P{empId} and rownum<=1)as moic,
(select distinct mr.rank_name from mas_employee ms,discharge_summary ds,mas_rank mr where ms.employee_id=ds.emp_id and ds.emp_id=$P{empId} and mr.rank_id=ms.rank_id and rownum<=1)as rankName,
(select distinct ms.designation from mas_employee ms,discharge_summary ds where ms.employee_id=ds.emp_id and ds.emp_id=$P{empId} and rownum<=1)as dign
from discharge_summary a, discharge_items_category b, patient c,mas_rank d,mas_relation mas_relation,mas_administrative_sex mas_administrative_sex,
mas_unit mas_unit,mas_employee e,inpatient f
where a.item_code = b.id and a.item_reply is not null
and a.hin_id = c.hin_id
and c.rank_id=d.rank_id
and c.relation_id = mas_relation.relation_id
and c.sex_id = mas_administrative_sex.administrative_sex_id
and c.unit_id = mas_unit.unit_id
and a.ad_no=f.ad_no and f.doctor_id=e.employee_id
and c.service_no = $P{serviceNo}
and a.ad_no=$P{adNo}
and a.CATEGORY_NAME = $P{casetype}
order by b.orderno]]></queryString>

