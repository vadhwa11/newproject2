	<queryString><![CDATA[select 
P.service_no, U.unit_name, R.rank_name, T.trade_name, 
(P_first_name||' '|| p_middle_name ||' '|| p_last_name) as name,
ma_date, H.hospital_name

from av_medical_exam_MA_MO A
left outer join patient p on A.hin_id=P.hin_id
left outer join mas_unit U on P.unit_id=U.unit_id
left outer join mas_rank R on P.rank_id=R.rank_id
left outer join mas_trade T on P.trade_id=T.trade_id
left outer join mas_hospital H on A.hospital_id=H.hospital_id
where A.ma_date between $P{fromDate} and $P{toDate}
and H.hospital_id=$P{hospitalId} and H.command_id=$P{commandId}]]></queryString>

