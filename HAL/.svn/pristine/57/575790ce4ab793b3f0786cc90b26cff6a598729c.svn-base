<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="ActivityDetails"
		table="ACTIVITY_DETAILS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="ID"
		>
			<generator class="native"></generator>  
		</id>

		<property
			name="ActivityDate"
			column="ACTIVITY_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="ActivityDetails"
			column="ACTIVITY_DETAILS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ActivityOfficers"
			column="ACTIVITY_OFFICERS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ActivityAirmen"
			column="ACTIVITY_AIRMEN"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ActivityFamilies"
			column="ACTIVITY_FAMILIES"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ActivityRemarks"
			column="ACTIVITY_REMARKS"
			type="string"
			not-null="false"
			length="50"
		/>
      <many-to-one name="Hospital_id" column="hospital_id" class="MasHospital"
       not-null = "false">
       </many-to-one>
       <many-to-one name="DepartmentId" column="department_id" class="MasDepartment"
       not-null = "false">
       </many-to-one>

	</class>	
</hibernate-mapping>