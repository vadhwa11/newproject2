	<queryString><![CDATA[SELECT
(patient.p_first_name||' '||patient.p_last_name)AS patientName,
mas_rank.rank_name AS mas_rank_rank_name,
mas_administrative_sex.administrative_sex_name AS administrative_sex_name,
mas_diagnosis_conclusion.diagnosis_conclusion_name AS diagnosis_conclusion_name,
mas_disposal.disposal_name AS mas_disposal_disposal_name,
visit.age AS visit_age,
visit.working_diagnosis As working_diagnosis,
patient.service_no AS patient_service_no,
mas_trade.trade_name AS mas_trade_trade_name,
hospital.hospital_name as hospital_name,
opd_patient_details.disposal as disposal,
visit.visit_id
FROM
patient patient LEFT OUTER JOIN visit visit ON patient.hin_id = visit.hin_id
LEFT OUTER JOIN mas_rank mas_rank ON patient.rank_id = mas_rank.rank_id
LEFT OUTER JOIN mas_administrative_sex mas_administrative_sex ON patient.sex_id = mas_administrative_sex.administrative_sex_id
LEFT OUTER JOIN mas_trade mas_trade ON patient.trade_id = mas_trade.trade_id
LEFT OUTER JOIN mas_diagnosis_conclusion mas_diagnosis_conclusion ON visit.diagnosis_id = mas_diagnosis_conclusion.diagnosis_conclusion_id
LEFT OUTER JOIN mas_disposal mas_disposal ON visit.disposal_id = mas_disposal.disposal_id
LEFT OUTER JOIN mas_hospital hospital on visit.hospital_id=hospital.hospital_id
LEFT OUTER JOIN opd_patient_details opd_patient_details on opd_patient_details.visit_id=visit.visit_id
WHERE
     visit.visit_no = $P{visitNo}
     And patient.service_no = $P{serviceNo}
     And patient.hin_no = $P{hinNo}
     And hospital.hospital_id=$P{hospitalId}]]></queryString>

