	<queryString><![CDATA[select t.sr_no,i.pvms_no,i.nomenclature,
u.unit_name,sum(t.qty_request),
  sum(t.mmf_qty),d.department_name as from_department,
  d1.department_name as to_department,('Demand No: '||m.demand_no||' & Demand Date:') as demand_det
  ,m.demand_date
from mas_store_item_conversion c,
mas_store_unit u,mas_store_item i, store_internal_indent_t t,
store_internal_indent_m m
LEFT OUTER JOIN mas_department d ON m.department_id = d.department_id
LEFT OUTER JOIN mas_department d1 ON m.to_store = d1.department_id
where c.purchase_unit_id = u.unit_id and
      i.item_conversion_id =c.item_conversion_id and
      t.item_id = i.item_id and
      d.department_id = m.department_id and    
      m.id = t.internal_id and
      m.id=$P{demandNo} and 
      m.department_id = $P{FromDept} 
group by t.sr_no,i.pvms_no,i.nomenclature,u.unit_name,d.department_name,d1.department_name,m.demand_no,m.demand_date,t.item_id
order by i.pvms_no
     ]]></queryString>

