	<queryString><![CDATA[SELECT
     store_indent_m.`indent_date` AS store_indent_m_indent_date,
     store_indent_m.`department_id` AS store_indent_m_department_id,
     mas_store_item.`pvms_no` AS mas_store_item_pvms_no,
     mas_store_item.`nomenclature` AS mas_store_item_nomenclature,
     store_indent_m.`cost` AS store_indent_m_cost,
     store_indent_t.`remarks` AS store_indent_t_remarks,
     mas_department.`department_name` AS mas_department_department_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `store_indent_m` store_indent_m LEFT OUTER JOIN `store_indent_t` store_indent_t ON store_indent_m.`indent_id` = store_indent_t.`indent_id`
     LEFT OUTER JOIN `mas_store_item` mas_store_item ON store_indent_t.`item_id` = mas_store_item.`item_id`
     LEFT OUTER JOIN `mas_department` mas_department ON store_indent_m.`department_id` = mas_department.`department_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON store_indent_m.`hospital_id` = mas_hospital.`hospital_id`
WHERE 
    store_indent_m.indent_date >= $P{From_Date}
AND store_indent_m.indent_date <= $P{To_Date}
AND store_indent_m.indent_type = 's']]></queryString>

