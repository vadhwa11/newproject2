<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="AnnualMediacalExamination" table="annual_medical_examination">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="ame_id">
			<generator class="native"></generator>
		</id>

		<property
			name="NextReview"
			column="next_review"
			type="date"
			not-null="false"
		/>
		<property
			name="LastBoard"
			column="last_board"
			type="date"
			not-null="false"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="CondiationStatus"
			column="condition_status"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="LastChgBy"
			column="last_chg_by"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Period"
			column="period"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Duration"
			column="duration"
			type="string"
			not-null="false"
			length="30"
		/>
		<many-to-one
			name="AfmsfDet"
			column="emp_afmsf_det_id"
			class="EmpAfmsfDet"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Category"
			column="category"
			class="MasMedicalCategory"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastBoardAt"
			column="last_board_at"
			class="MasUnit"
			not-null="false"
		>
		</many-to-one>
		
	</class>	
</hibernate-mapping>