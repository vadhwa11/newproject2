	<queryString><![CDATA[select me.service_no,(me.first_name + ' ' + isnull(me.middle_name,'') + ' ' + isnull(me.last_name,'') ) as empname,
rank.rank_name,unit.unit_name,trade.trade_name,me.cell_no_emergency as mobile_no,bg.blood_group_name,me.local_address as address,
(select categories from mas_medical_examination_report 
left join category cat on presentMedicalCategory=cat.categoryid 
 where serviceno=me.service_no and rownum =1 and medical_examination_id = 
(select max(medical_examination_id) from mas_medical_examination_report where serviceno=me.service_no)
) medcat
from mas_employee me
left join mas_blood_group bg on me.blood_group_id=bg.blood_group_id
left join mas_rank rank on me.rank_id=rank.rank_id
left join mas_unit unit on me.unit_id=unit.unit_id
left join mas_trade trade on me.trade_id=trade.trade_id
where me.status='y' $P!{qry}

union

select me.service_no,(me.s_first_name + ' ' + isnull(me.s_middle_name,'') + ' ' + isnull(me.s_last_name,'') ) as empname,
rank.rank_name,unit.unit_name,trade.trade_name,me.mobile_number as mobile_no,bg.blood_group_name,dbms_lob.substr(me.address, 1000, 1 ) as address,
(select categories from mas_medical_examination_report 
left join category cat on presentMedicalCategory=cat.categoryid 
 where serviceno=me.service_no and rownum =1 and medical_examination_id = 
(select max(medical_examination_id) from mas_medical_examination_report where serviceno=me.service_no)
) medcat
from patient me
left join mas_blood_group bg on me.sr_blood_group_id=bg.blood_group_id
left join mas_rank rank on me.rank_id=rank.rank_id
left join mas_unit unit on me.unit_id=unit.unit_id
left join mas_trade trade on me.trade_id=trade.trade_id
where me.status='y' and me.relation_id=8  $P!{pQry}]]></queryString>

