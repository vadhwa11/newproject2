	<queryString><![CDATA[SELECT
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     mas_hospital_A.`hospital_name` AS mas_hospital_A_hospital_name,
     CONCAT(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`,"  (",mas_relation.`relation_name`,") ")as patientName,
     CONCAT(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)as serviceName,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     patient.`service_no` AS patient_service_no,
     md_cardic_claim_advance.`retirement_date` AS md_cardic_claim_advance_retirement_date,
     md_cardic_claim_advance.`working_diagnosis` as working_diagnosis ,
      md_cardic_claim_advance.`pao` as pao ,
     mas_reference.`reference_name` as reference_name ,
     mas_relation.`relation_name` AS mas_relation_relation_name,
     mas_icd.`icd_name` AS mas_icd_icd_name,
    
     md_cardic_claim_advance.`qualifying_amount` AS md_cardic_claim_advance_qualifying_amount ,
     mas_charge_code.`charge_code_name` as treatement 
FROM
     `mas_hospital` mas_hospital RIGHT OUTER JOIN `md_cardic_claim_advance` md_cardic_claim_advance ON mas_hospital.`hospital_id` = md_cardic_claim_advance.`hospital_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital_A ON md_cardic_claim_advance.`payable_to` = mas_hospital_A.`hospital_id`
     LEFT OUTER JOIN `patient` patient ON md_cardic_claim_advance.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `mas_icd` mas_icd ON md_cardic_claim_advance.`diagnosis_id` = mas_icd.`icd_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`
     Left outer join `mas_reference` mas_reference on md_cardic_claim_advance.`reffered_to`=mas_reference.`reference_id`
     left outer join `mas_charge_code` mas_charge_code on mas_charge_code.`charge_code_id`=md_cardic_claim_advance.`treatment_detail`
$P!{QUERY}]]></queryString>

