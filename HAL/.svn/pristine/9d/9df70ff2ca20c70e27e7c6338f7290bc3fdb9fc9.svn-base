	<queryString><![CDATA[select msad.ad_no,p.service_no,mr.rank_name,
(p_first_name||' '||nvl(p_middle_name,'')||' '||nvl(p_last_name,'')) as patient_name,
p.age,mt.trade_name,mu.unit_name,msad.date_of_admission,msad.disposed_off,msad.date_of_discharge,
msad.no_of_days,mh.hospital_name,msad.inpatient_id,msad.remarks,msad.diagnosis
from monthly_sick_adm_details msad 
inner join monthly_sick_adm_header msah
on msad.MONTHLY_ADM_HEADER_ID = msah.MONTHLY_SICK_ADM_HEADER_ID 
inner join patient p on msad.hin_id=p.hin_id
left outer join inpatient ip on msad.inpatient_id=ip.inpatient_id
left outer join mas_rank mr on p.rank_id=mr.rank_id
left outer join mas_unit mu on p.unit_id=mu.unit_id
left outer join mas_trade mt on p.trade_id=mt.trade_id
left outer join mas_hospital mh on msah.hospital_id=mh.hospital_id
where extract(month from monthly_adm_report_date) = $P{month} and extract(year from monthly_adm_report_date) = $P{year}  and msah.hospital_id=$P{hospitalId}  $P!{qry}]]></queryString>

