	<queryString><![CDATA[select msi.pvms_no,msi.nomenclature,dt.batch_no,dt.expiry_date,
dt.received_qty,dt.unit_rate,dt.amount_value,hd.grn_date
,u.first_name,md.department_name,mh.hospital_name

 from store_grn_t dt
left outer join store_grn_m hd on hd.grn_master_id=dt.grn_master_id
left outer join users u on u.user_id=hd.last_chg_by
left outer join mas_store_item msi on msi.item_id=dt.item_id
left outer join mas_department md on md.department_id=hd.department_id
left outer join mas_hospital mh on mh.hospital_id=hd.hospital_id

 WHERE   
  hd.department_id = $P{deptId} AND hd.receive_type='ReturnedMedicine' AND
(convert(date, hd.grn_date) >=$P{fromDate} AND convert(date, hd.grn_date)<=$P{toDate})]]></queryString>

