	<queryString><![CDATA[SELECT
     patient.`next_of_kin_name` AS patient_next_of_kin_name,
     patient.`next_of_kin_address` AS patient_next_of_kin_address,
     patient.`next_of_kin_relation_id` AS patient_next_of_kin_relation_id,
     mas_relation.`relation_name` AS mas_relation_relation_name,
     patient.`next_of_kin_phone_number` AS patient_next_of_kin_phone_number,
     concat(patient.`service_no`,' ',mas_rank.`rank_name`)as SnRank,
     concat(patient.`p_first_name`,' ',patient.`p_middle_name`,' ',patient.`p_last_name`)as PName,
     concat(patient.`s_first_name`,' ',patient.`s_middle_name`,' ',patient.`s_last_name`)as SName,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     expiry_details.`expiry_date` AS expiry_details_expiry_date,
     expiry_details.`expiry_time` AS expiry_details_expiry_time,
     patient.`service_no` AS patient_service_no
FROM
     `mas_relation` mas_relation INNER JOIN `patient` patient ON mas_relation.`relation_id` = patient.`next_of_kin_relation_id`
     INNER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     INNER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
     INNER JOIN `expiry_details` expiry_details ON patient.`hin_id` = expiry_details.`hin_id`
WHERE
     patient.hin_id = $P{HIN_ID}]]></queryString>

