<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="IpdClinicalChart" table="ipd_clinical_chart">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="ipdclinicalchart_id">
			<generator class="native" />
		</id>

		<property name="AdNo" column="ad_no" type="string" not-null="false"
			length="30" />
		<property name="Temperature" column="temperature" type="java.lang.Float"
			not-null="false" length="12" />
		<property name="Pulse" column="pulse" type="integer"
			not-null="false" length="10" />
		<property name="Respiration" column="respiration" type="integer"
			not-null="false" length="10" />
		<property name="Bp" column="bp" type="string" not-null="false"
			length="8" />
		<property name="Bowel" column="bowel" type="string" not-null="false"
			length="3" />
		<property name="Pain" column="pain" type="string" not-null="false"
			length="3" />
		<property name="Fhr" column="fhr" type="string" not-null="false"
			length="8" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="timestamp"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="ClinicalDate" column="clinical_date" type="date"
			not-null="false" />
		<property name="ClinicalTime" column="clinical_time" type="string"
			not-null="false" length="10" />
		<property name="Remarks" column="remarks" type="string"
			not-null="false" length="30" />
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
		<many-to-one name="Hin" column="hin_id" class="Patient"
			not-null="false">
		</many-to-one>
		<many-to-one name="Inpatient" column="inpatient_id" class="Inpatient"
			not-null="false">
		</many-to-one>

	</class>
</hibernate-mapping>