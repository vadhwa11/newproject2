	<queryString><![CDATA[SELECT
     patient_investigation_details. quantity  AS patient_details_quantity,
     dg_mas_investigation. instructions  AS dg_mas_instructions,
     mas_charge_code. charge_code_code  AS mas_charge_code_code,
      mas_sub_chargecode.sub_chargecode_name as subChargeCodeName,
     mas_charge_code. charge_code_name AS mas_charge_code_name,
     patient_investigation_header. investigation_header_id  AS investigation_header_id,
     patient_investigation_header. clinical_notes  AS header_clinical_notes,
     patient_investigation_details.Clinical_Notes AS investigation_remarks
FROM
      patient_investigation_header LEFT OUTER JOIN  patient_investigation_details ON patient_investigation_header. investigation_header_id  = patient_investigation_details. investigation_header_id 
     LEFT OUTER JOIN  mas_charge_code ON patient_investigation_details. charge_code_id  = mas_charge_code. charge_code_id 
     LEFT OUTER JOIN  mas_sub_chargecode ON mas_sub_chargecode.sub_chargecode_id=mas_charge_code.sub_chargecode_id     
     LEFT OUTER JOIN  dg_mas_investigation ON mas_charge_code. charge_code_id  = dg_mas_investigation. charge_code_id 
     LEFT OUTER JOIN  visit ON patient_investigation_header. visit_id  = visit. visit_id 
     LEFT OUTER JOIN  patient ON patient_investigation_header. hin_id  = patient. hin_id 
     AND patient. hin_id  = visit. hin_id 
WHERE
     visit. visit_id  = $P{visitNo}
     And patient. service_no  = $P{serviceNo}
     And patient. hin_no  = $P{hinNo}]]></queryString>

