	<queryString><![CDATA[select 
distinct
p.service_no, r.rank_name,  t.trade_name, u.unit_name, p.age,
(p.p_first_name||' '||p.p_last_name) as name, ds.diagnosis, expr.expiry_date, expr.place_of_death,
rl.relation_name, ds.patient_condition, h.hospital_name, expr.cause_of_death, expr.expiry_details_id

from expiry_details expr

left outer join patient p on expr.hin_id=p.hin_id
left outer join patient_discharge_slip ds on ds.hin_id=p.hin_id
left outer join mas_rank r on p.rank_id=r.rank_id
left outer join mas_trade t on p.trade_id=t.trade_id
left outer join mas_unit u on p.unit_id=u.unit_id
left outer join mas_relation rl on p.relation_id=rl.relation_id
left outer join mas_hospital h on p.hospital_id=h.hospital_id

where expr.expiry_date between $P{fromDate} and $P{toDate} and expr.hospital_id=$P{hospitalId} ]]></queryString>

