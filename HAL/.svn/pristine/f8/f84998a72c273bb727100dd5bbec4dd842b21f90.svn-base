	<queryString><![CDATA[select
    
     ipdcaredetail.ad_no as ipdcaredetail_ad_no,
     patient.service_no as patient_service_no,
     (patient.p_first_name||' '||patient.p_last_name)as patientname,
     mas_nursing_care.nursing_name as mas_nursing_care_nursing_name,
     ipdcaredetail.care1_time as ipdcaredetail_care1_time,
     ipdcaredetail.care2_time as ipdcaredetail_care2_time,
     ipdcaredetail.care3_time as ipdcaredetail_care3_time,
     ipdcaredetail.care4_time as ipdcaredetail_care4_time,
     ipdcaredetail.care5_time as ipdcaredetail_care5_time,
     ipdcaredetail.care6_time as ipdcaredetail_care6_time,
     ipdcaredetail.care7_time as ipdcaredetail_care7_time,
     ipdcaredetail.care8_time as ipdcaredetail_care8_time,
     ipdcaredetail.care9_time as ipdcaredetail_care9_time,
     ipdcaredetail.care10_time as ipdcaredetail_care10_time,
     ipdcaredetail.care11_time as ipdcaredetail_care11_time,
     ipdcaredetail.care12_time as ipdcaredetail_care12_time,
     ipdcaredetail.date_of_care as ipdcaredetail_date_of_care,
     mas_hospital.hospital_name as hospital_name,
mas_department.department_name as department_name
from
nursingcare_setup nursingcare_setup left outer join ipdcaredetail ipdcaredetail on nursingcare_setup.nursingcare_setup_id =ipdcaredetail.nursingcare_setup_id
left outer join mas_department mas_department on ipdcaredetail.department_id=mas_department.department_id
left outer join inpatient inpatient on nursingcare_setup.inpatient_id=inpatient.inpatient_id and nursingcare_setup.ad_no=inpatient.ad_no
left outer join patient patient on inpatient.hin_id=patient.hin_id 
left outer join mas_nursing_care mas_nursing_care on nursingcare_setup.nursing_id=mas_nursing_care.nursing_id
left outer join mas_hospital mas_hospital on ipdcaredetail.hospital_id=mas_hospital.hospital_id
where ipdcaredetail.hospital_id=$P{hospitalId} and
     mas_department.department_id = $P{Ward}
   $P!{query}]]></queryString>

