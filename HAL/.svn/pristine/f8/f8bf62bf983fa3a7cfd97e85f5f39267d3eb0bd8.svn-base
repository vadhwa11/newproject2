	<queryString><![CDATA[select prescription_header.prescription_no,prescription_header.prescription_date,visit.working_diagnosis,pre_details.dosage,mas_item.pvms_no,mas_item.nomenclature 
,(mas_emp.first_name||' '||mas_emp.middle_name||' '||mas_emp.last_name) as emp_name,
(patient.p_first_name||' '||p_middle_name||' '||p_last_name) as patient_name
from patient_prescription_header prescription_header 
left outer join visit  on visit.visit_id= prescription_header.visit_id
left outer join patient_prescription_details pre_details on pre_details.prescription_id= prescription_header.prescription_id 
left outer join mas_store_item mas_item on mas_item.item_id=pre_details.item_id
left outer join mas_employee mas_emp on mas_emp.employee_id=prescription_header.emp_id
left outer join  patient on patient.hin_id=prescription_header.hin_id
where prescription_date between to_date($P{from_date},'dd/mm/yyyy') and to_date($P{to_date},'dd/mm/yyyy')]]></queryString>

