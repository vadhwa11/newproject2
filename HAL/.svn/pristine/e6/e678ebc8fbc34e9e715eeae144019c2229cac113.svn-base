	<queryString><![CDATA[SELECT
     
item.nomenclature,total,dosage,no_of_days,au.item_unit_name,
f.frequency_name, prescription_time
FROM
ot_booking ot_booking
left OUTER JOIN inpatient inpatient on ot_booking.inpatient_id=inpatient.inpatient_id
left outer join patient_prescription_header pph   ON inpatient.inpatient_id = pph.inpatient_id
left OUTER JOIN patient_prescription_details ppd ON  pph.prescription_id= ppd.prescription_id
left outer join mas_store_item  item   on  ppd.item_id= item.item_id
left outer join mas_store_item_conversion  au   on  item.item_conversion_id= au.item_conversion_id
left outer join mas_frequency  f   on  f.frequency_id= ppd.frequency_id
left outer join ot_post_anaesthesia_procedure  opre on   opre.ot_booking_id=ot_booking.booking_id
where ppd.ot_stage ='POSTANS' and ppd.ot_stage is not null and ot_booking.inpatient_id is not null 
and  opre.ot_post_anaesthesia_procedure_id=$P{mainId}]]></queryString>

