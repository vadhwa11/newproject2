	<queryString><![CDATA[select a.sr_no,
b.pvms_no,
b.nomenclature,
d.item_unit_name,
sum(a.qty_request) as a,
f.mmf_qty,issue_date,
SUBSTRING (g.ISSUE_NO,CHARINDEX('/',g.ISSUE_NO)+1,5) "period",

sum(a.qty_issued) as b,
a.remarks,
CONCAT('MO I/C ',d1.department_name ) as from_department,
CONCAT ('MO I/C ',d2.department_name) as to_department,
CONCAT ('Demand No: ',e.demand_no,',Demand Date:') as demand_Details,
e.demand_date,
msu.UNIT_NAME,
f.REASON_FOR_DEMAND
from store_issue_t a left outer join store_issue_m g on a.issue_m_id = g.id
left outer join store_internal_indent_m e on e.id=g.request_no
left outer join store_internal_indent_t f on e.id=f.internal_id and a.item_id = f.item_id
left outer join mas_store_item b on a.item_issued = b.item_id
left outer join mas_store_item c on a.item_id = c.item_id
left outer join mas_store_item_conversion d on b.item_conversion_id = d.item_conversion_id
left outer join mas_store_unit msu  on msu.UNIT_ID =d.PURCHASE_UNIT_ID
LEFT OUTER JOIN mas_department d1 ON g.department_id = d1.department_id
LEFT OUTER JOIN mas_department d2 ON g.to_store = d2.department_id
where a.issue_m_id=$P{issueId}
group by a.sr_no,b.pvms_no,b.nomenclature,d.item_unit_name,f.mmf_qty,issue_date,g.ISSUE_NO,
a.remarks,d1.department_name,d2.department_name,e.demand_no,e.demand_date, a.item_id,msu.UNIT_NAME,
f.REASON_FOR_DEMAND
order by b.pvms_no]]></queryString>

