	<queryString><![CDATA[SELECT
     mas_store_supplier.supplier_code AS supplier_supplier_code,
     mas_store_supplier.supplier_name AS supplier_supplier_name,
     (mas_store_supplier.local_address1|| mas_store_supplier.local_address2|| mas_store_supplier.local_city) as address,
     mas_store_supplier.tin_no AS supplier_tin_no,
      mas_store_supplier.local_pin_code AS supplier_local_pin_code,
      mas_store_supplier.emailId AS supplier_emailId,
     mas_store_supplier.faxNumber AS supplier_faxNumber,
     mas_store_supplier.mobileNo AS supplier_mobileNo,
     mas_store_supplier_type.supplier_type_name AS supplier_type_name,
     mas_manufacturer.manufacturer_name AS manufacturer_name    
   
FROM
     mas_store_supplier_type mas_store_supplier_type RIGHT OUTER JOIN mas_store_supplier mas_store_supplier ON mas_store_supplier_type.supplier_type_id = mas_store_supplier.supplier_type_id
     LEFT OUTER JOIN mas_store_vendor_wise_manufact mas_store_vendor_wise_manufact ON mas_store_supplier.supplier_id = mas_store_vendor_wise_manufact.supplier_id
     LEFT OUTER JOIN mas_manufacturer mas_manufacturer ON mas_store_vendor_wise_manufact.manufacturer_id = mas_manufacturer.manufacturer_id

WHERE mas_store_supplier.supplier_id =  $P{VENDOR_ID}]]></queryString>

