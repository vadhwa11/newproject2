	<queryString><![CDATA[SELECT
     machine_activity.`entry_id` AS machine_activity_entry_id,
     machine_activity.`activity_date` AS machine_activity_activity_date,
     machine_activity.`time_on` AS machine_activity_time_on,
     machine_activity.`time_off` AS machine_activity_time_off,
     machine_activity.`total_hrs` AS machine_activity_total_hrs,
     machine_activity.`machine_id` AS machine_activity_machine_id,
     mas_laundry_machine.`machine_id` AS mas_laundry_machine_machine_id,
     mas_laundry_machine.`mahine_name` AS mas_laundry_machine_mahine_name
FROM
     `mas_laundry_machine` mas_laundry_machine INNER JOIN `machine_activity` machine_activity ON mas_laundry_machine.`machine_id` = machine_activity.`machine_id`
WHERE
     machine_activity.`activity_date` between $P{fromDate} and  $P{toDate}
and  mas_laundry_machine.`machine_id`= if($P{machineName}!=0 ,$P{machineName},mas_laundry_machine.`machine_id`)
group by mas_laundry_machine.`machine_id` , mas_laundry_machine.`mahine_name`, machine_activity.`machine_id`,
machine_activity.`entry_id` , machine_activity.`activity_date`,
machine_activity.`time_off`, machine_activity.`total_hrs`
order by mas_laundry_machine.`machine_id`,machine_activity.`activity_date`]]></queryString>

