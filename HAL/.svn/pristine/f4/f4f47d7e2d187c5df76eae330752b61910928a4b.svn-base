	<queryString><![CDATA[select p.service_no,(p_first_name||' '||nvl(p_middle_name,'')||' '||nvl(p_last_name,'')) as pname,procedure_date 
,(s_first_name||' '||nvl(s_middle_name,'')||' '||nvl(s_last_name,'')) as sname,
rel.relation_name,mr.rank_name,mu.UNIT_NAME,p.age,ms.ADMINISTRATIVE_SEX_NAME,
nvl2(procedure_id,nc.NURSING_NAME,procedure_name) as procedurename,opd.initial_diagnosis,
pd.remarks as remarks,
h.hospital_name as hospital_name
,(mer.rank_name||' '||first_name||' '||nvl(middle_name,'')||' '||nvl(last_name,'')) as moname,
ph.hin_id
from procedure_details pd inner join procedure_header ph on pd.PROCEDURE_HEADER_ID=ph.PROCEDURE_HEADER_ID
inner join visit v on ph.visit_id=v.visit_id
left join opd_patient_details opd on v.visit_id = opd.visit_id
inner join mas_nursing_care nc on pd.procedure_id= nc.nursing_id
inner join patient p on ph.hin_id=p.hin_id 
left join mas_relation rel on p.relation_id=rel.relation_id
left join mas_rank mr on p.rank_id=mr.rank_id
left join mas_unit mu on p.unit_id=mu.unit_id
left join mas_administrative_sex ms on p.sex_id=ms.ADMINISTRATIVE_SEX_ID
left join mas_hospital h on ph.hospital_id=h.hospital_id
left join mas_employee me on ph.medical_officer_id=me.employee_id
left join mas_rank mer on p.rank_id=mer.rank_id
 where procedure_date between $P{fromDate} and $P{toDate}
and ph.hospital_id=$P{hospitalId}
 $P!{qry}]]></queryString>

