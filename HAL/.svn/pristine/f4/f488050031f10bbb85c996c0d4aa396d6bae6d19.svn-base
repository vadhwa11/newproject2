     pension_form8_entry.`other_assessed_dues` AS pension_form8_entry_other_assessed_dues,
     pension_form8_entry.`amount_of_gratuity` AS pension_form8_entry_amount_of_gratuity,
     pension_form8_entry.`total` AS pension_form8_entry_total,
     mas_personnel_details.`designation_id` AS mas_personnel_details_designation_id,
     mas_designation.`designation_name` AS mas_designation_designation_name
