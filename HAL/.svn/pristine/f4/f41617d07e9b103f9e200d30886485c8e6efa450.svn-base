	<queryString><![CDATA[SELECT dept.department_name,st.service_type_name,if(st.service_type_name="AirForce",count(*),0) as num

FROM
     `inpatient` ip RIGHT OUTER JOIN `mas_department` dept on ip.department_id=dept.department_id
      LEFT join patient p on p.hin_id=ip.hin_id
     
      LEFT join mas_service_type st on st.service_type_id=p.service_type_id
and st.service_type_name="AirForce" 

GROUP BY dept.department_name]]></queryString>

	<field name="department_name" class="java.lang.String"/>
	<field name="service_type_name" class="java.lang.String"/>
	<field name="num" class="java.lang.Long"/>

