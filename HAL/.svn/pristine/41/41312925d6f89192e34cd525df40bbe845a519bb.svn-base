	<queryString><![CDATA[SELECT
     BIRTHDEATHREG."REGNO" AS BIRTHDEATHREG_REGNO,
     BIRTHDEATHREG."NAME" AS BIRTHDEATHREG_NAME,
     BIRTHDEATHREG."DOR" AS BIRTHDEATHREG_DOR,
     BIRTHDEATHREG."DOD" AS BIRTHDEATHREG_DOD,
     EXPIRY_DETAILS."D_DEATH_CAUSE_TEXT" AS EXPIRY_DETAILS_D_DEATH_CAUSE_T,
     EXPIRY_DETAILS."S_DEATH_CAUSE_TEXT" AS EXPIRY_DETAILS_S_DEATH_CAUSE_T,
     EXPIRY_DETAILS."C_DEATH_CAUSE_TEXT" AS EXPIRY_DETAILS_C_DEATH_CAUSE_T,
     EXPIRY_DETAILS."CONSEQUENCE_OF" AS EXPIRY_DETAILS_CONSEQUENCE_OF,
     PATIENT."SERVICE_NO" AS PATIENT_SERVICE_NO,
     MAS_RANK."RANK_NAME" AS MAS_RANK_RANK_NAME,
     MAS_UNIT."UNIT_NAME" AS MAS_UNIT_UNIT_NAME,
     MAS_HOSPITAL."HOSPITAL_NAME" AS MAS_HOSPITAL_HOSPITAL_NAME,
     MAS_RANK_EMP."RANK_NAME" AS MAS_RANK_EMP_RANK_NAME,
     (MAS_EMPLOYEE."FIRST_NAME"||' '||nvl(MAS_EMPLOYEE."MIDDLE_NAME",'')||' '||nvl(MAS_EMPLOYEE."LAST_NAME",''))EMPLOYEE_NAME,
     PATIENT."STATION" AS PATIENT_STATION
FROM
     "HMS"."EXPIRY_DETAILS" EXPIRY_DETAILS RIGHT OUTER JOIN "HMS"."BIRTHDEATHREG" BIRTHDEATHREG ON EXPIRY_DETAILS."EXPIRY_DETAILS_ID" = BIRTHDEATHREG."EXPIRY_DETAILS_ID"
     LEFT OUTER JOIN "HMS"."PATIENT" PATIENT ON BIRTHDEATHREG."HIN_ID" = PATIENT."HIN_ID"
     LEFT OUTER JOIN "HMS"."MAS_HOSPITAL" MAS_HOSPITAL ON BIRTHDEATHREG."HOSPITAL_ID" = MAS_HOSPITAL."HOSPITAL_ID"
     LEFT OUTER JOIN "HMS"."MAS_EMPLOYEE" MAS_EMPLOYEE ON BIRTHDEATHREG."EMP_ID" = MAS_EMPLOYEE."EMPLOYEE_ID"
     LEFT OUTER JOIN "HMS"."MAS_RANK" MAS_RANK_EMP ON MAS_EMPLOYEE."RANK_ID" = MAS_RANK_EMP."RANK_ID"
     LEFT OUTER JOIN "HMS"."MAS_RANK" MAS_RANK ON PATIENT."RANK_ID" = MAS_RANK."RANK_ID"
     LEFT OUTER JOIN "HMS"."MAS_UNIT" MAS_UNIT ON PATIENT."UNIT_ID" = MAS_UNIT."UNIT_ID"
WHERE
     	 BIRTHDEATHREG.inpatient_id=$P{inpatientId} and  BIRTHDEATHREG.bdtype = 'd']]></queryString>

