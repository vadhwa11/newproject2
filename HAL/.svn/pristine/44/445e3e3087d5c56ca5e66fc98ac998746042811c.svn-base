	<queryString><![CDATA[SELECT inpatient.date_of_addmission,inpatient.time_of_addmission,inpatient.discharge_date , inpatient.discharge_time,inpatient.age,inpatient.ad_no,
patient.permanent_address,isnull(patient.phone_number,patient.mobile_number) as ph_no,
patient.p_first_name +' '+ isnull(patient.p_middle_name,'')+' '+isnull(patient.p_last_name,'') as patientname, 
mas_employee.department_no, patient.s_first_name+' '+isnull(patient.s_middle_name,'')+' '+isnull(patient.s_last_name,'')as empname,
mas_rank.rank_name, mas_relation.relation_name, patient.service_no
FROM inpatient inpatient
LEFT outer join patient patient ON inpatient.hin_id = patient.hin_id
LEFT outer join mas_employee mas_employee ON patient.employee_id=mas_employee.employee_id
LEFT outer join mas_rank mas_rank ON mas_rank.rank_id=mas_employee.rank_id
LEFT outer join mas_relation mas_relation ON mas_relation.relation_id=patient.relation_id
where inpatient.inpatient_id=$P{adNo}]]></queryString>

