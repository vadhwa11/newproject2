	<queryString><![CDATA[SELECT
     PATIENT_DETENTION_REGISTER.DETAINED_FROM AS detained_from,
     PATIENT_DETENTION_REGISTER.DETAINED_TO AS detained_to,
     PATIENT_DETENTION_REGISTER.TREATMENT AS treatment,
     PATIENT_DETENTION_REGISTER.FROM_TIME AS from_time,
     PATIENT_DETENTION_REGISTER.TO_TIME AS to_time,
     (PATIENT.P_FIRST_NAME||' '||PATIENT.P_MIDDLE_NAME||' '||PATIENT.P_LAST_NAME)AS PATIENT_NAME,
     (PATIENT.S_FIRST_NAME||' '||PATIENT.S_MIDDLE_NAME||' '||PATIENT.S_LAST_NAME)AS SERV_PERS_NAME,
     PATIENT.SERVICE_NO AS PATIENT_SERVICE_NO,
     MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_NAME AS sex,
     (EMP_RANK.RANK_NAME || ' '||MAS_EMPLOYEE.FIRST_NAME||' '||MAS_EMPLOYEE.MIDDLE_NAME||' '||MAS_EMPLOYEE.LAST_NAME)AS EMPLOYEE_NAME,
     MAS_HOSPITAL.HOSPITAL_NAME AS MAS_HOSPITAL_HOSPITAL_NAME,
     MAS_RELATION.RELATION_NAME AS MAS_RELATION_RELATION_NAME,
     MAS_UNIT.UNIT_NAME AS MAS_UNIT_UNIT_NAME,
     MAS_RANK.RANK_NAME AS MAS_RANK_RANK_NAME,
     PATIENT.AGE AS PATIENT_AGE,
     PATIENT_DETENTION_REGISTER.REQUISITION_DATE AS PATIENT_DETENTION_REGISTER_REQ
FROM
    PATIENT PATIENT INNER JOIN PATIENT_DETENTION_REGISTER PATIENT_DETENTION_REGISTER ON PATIENT.HIN_ID = PATIENT_DETENTION_REGISTER.HIN_ID
     LEFT OUTER JOIN MAS_ADMINISTRATIVE_SEX MAS_ADMINISTRATIVE_SEX ON PATIENT.SEX_ID = MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_ID
     LEFT OUTER JOIN MAS_RANK MAS_RANK ON PATIENT.RANK_ID = MAS_RANK.RANK_ID
     LEFT OUTER JOIN MAS_RELATION MAS_RELATION ON PATIENT.RELATION_ID = MAS_RELATION.RELATION_ID
     LEFT OUTER JOIN MAS_UNIT MAS_UNIT ON PATIENT.UNIT_ID = MAS_UNIT.UNIT_ID
     LEFT OUTER JOIN MAS_EMPLOYEE MAS_EMPLOYEE ON PATIENT_DETENTION_REGISTER.MEDICAL_OFFICER_ID = MAS_EMPLOYEE.EMPLOYEE_ID
     LEFT OUTER JOIN MAS_RANK EMP_RANK ON PATIENT.RANK_ID = EMP_RANK.RANK_ID
     LEFT OUTER JOIN MAS_HOSPITAL MAS_HOSPITAL ON PATIENT_DETENTION_REGISTER.HOSPITAL_ID = MAS_HOSPITAL.HOSPITAL_ID
where trunc(PATIENT_DETENTION_REGISTER.detention_register_date) between $P{fromDate} and $P{toDate}
 and PATIENT_DETENTION_REGISTER.hospital_id=$P{hospitalId}
 $P!{qry}]]></queryString>

