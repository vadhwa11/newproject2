b.curr_year_mmf as 'appr MMF',e.item_unit_name,b.Curr_year_mmf as current,b.prev_year_mmf as previous,
sum(if(a.mmf_for_the_year=$P{mmfForTheYear}-1,b.curr_year_mmf,0)) as 'prev year',
sum(if(a.mmf_for_the_year=$P{mmfForTheYear},b.curr_year_mmf,0)) as 'Appr1 MMF'
from
store_mmf_department_m a,  store_mmf_department_t b, mas_store_item c,mas_department d,
mas_store_item_conversion e
where a.id = b.store_mmf_department_m_id and e.item_conversion_id=c.item_conversion_id and
a.store_ward_dept=d.department_id
and b.item_id = c.item_id
and a.mmf_store_type=$P{storeType} and c.pvms_no=$P{pvms_no}
group by b.item_id]]></queryString>

