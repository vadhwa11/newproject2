<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PatientPrescriptionHeader"
		table="patient_prescription_header"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="prescription_id"
		>
			<generator class="native"/>
		</id>

		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="PrescriptionDate"
			column="prescription_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="PrescriptionTime"
			column="prescription_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="PrescriptionNo"
			column="prescription_no"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="InjectionStatus"
			column="injection_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Specialty"
			column="SPECIALTY"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="OtherTreatment"
			column="OTHER_TREATMENT"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="FacStatus"
			column="fac_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="RcStatus"
			column="rc_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="NipStatus"
			column="nip_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="DispensaryIssueNo"
			column="dispensary_issue_no"
			type="string"
			not-null="false"
			length="50"
		/>		
		<property
			name="IssuedDate"
			column="issued_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="IssuedTime"
			column="issued_time"
			type="string"
			not-null="false"
			length="15"
		/>
			<property
			name="PresPrintCount"
			column="pres_print_count"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="NisPrintCount"
			column="nis_print_count"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="NipPrintCount"
			column="nip_print_count"
			type="integer"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="OtSurgery"
			column="ot_surgery_id"
			class="OpdSurgeryHeader"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Zonal"
			column="zonal_id"
			class="MasZonal"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Visit"
			column="visit_id"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Emp"
			column="emp_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="OpdPatientDetails"
			column="opd_patient_details_id"
			class="OpdPatientDetails"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Division"
			column="division_id"
			class="MasDivision"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="NipApprovedBy"
			column="nip_approved_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="IssuedBy"
			column="issued_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="PatientPrescriptionDetails">
			<key column="prescription_id" />
			<one-to-many class="PatientPrescriptionDetails" />
		</set>



	</class>	
</hibernate-mapping>