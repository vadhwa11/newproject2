	<queryString><![CDATA[SELECT 
     MAS_ICD.ICD_NAME AS ICDDIAGNOSIS,
     MAS_ICD.ICD_CODE AS ICDCODE     
    
FROM DISCHARGE_ICD_CODE DISCHARGE_ICD_CODE
LEFT JOIN INPATIENT INPATIENT ON DISCHARGE_ICD_CODE.INPATIENT_ID =INPATIENT.INPATIENT_ID
LEFT JOIN MAS_ICD MAS_ICD ON DISCHARGE_ICD_CODE.ICD_ID=MAS_ICD.ICD_ID
left outer join ot_booking  ot_booking on   ot_booking.INPATIENT_ID=DISCHARGE_ICD_CODE.INPATIENT_ID

left outer join  ot_post_anaesthesia_procedure  opre  on opre.ot_booking_id=ot_booking.booking_id
where opre.ot_post_anaesthesia_procedure_id=$P{mainId}

group by   MAS_ICD.ICD_NAME, MAS_ICD.ICD_CODE]]></queryString>

