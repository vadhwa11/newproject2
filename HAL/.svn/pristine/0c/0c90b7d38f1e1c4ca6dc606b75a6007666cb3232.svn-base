	</parameter>
	<parameter name="curr_date" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="query" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select a.*, b.stock,nvl((select nvl(sum(store_issue_t.qty_issued),0) from store_issue_m store_issue_m
left outer JOIN store_issue_t store_issue_t ON 
store_issue_m.id=store_issue_t.ISSUE_M_ID where store_issue_m.department_id=24 and store_issue_t.item_id=a.item_id_issue
and store_issue_m.to_store=35 and store_issue_m.issue_date=to_date($P{curr_date},'dd/mm/yyyy')
group by store_issue_t.item_id),0) as receivedQty
from (
SELECT
    mas_store_item.pvms_no AS item_pvms_no,
    mas_store_item.nomenclature AS item_nomenclature
    ,sum(store_op_patient_issue_t.qty_issued) AS op_patent_isue_t_qty_isued
    ,store_op_patient_issue_t.item_id_issue
FROM
     store_op_patient_issue_m store_op_patient_issue_m inner JOIN store_op_patient_issue_t store_op_patient_issue_t 
     ON store_op_patient_issue_m.op_issue_id = store_op_patient_issue_t.op_issue_id
     left outer JOIN mas_store_item mas_store_item ON store_op_patient_issue_t.item_id_issue = mas_store_item.item_id
  $P!{query}   
  left outer join store_item_batch_stock on (store_op_patient_issue_t.item_id_issue=store_item_batch_stock.item_id 
     and      store_op_patient_issue_t.batch_no=store_item_batch_stock.batch_no and 
      store_op_patient_issue_t.rate=store_item_batch_stock.cost_price
     and  store_item_batch_stock.department_id=35)
WHERE store_op_patient_issue_m.issue_date=to_date($P{curr_date},'dd/mm/yyyy') 
    AND store_op_patient_issue_m.department_id =35  $P!{query}
   GROUP BY mas_store_item.pvms_no,mas_store_item.nomenclature,store_op_patient_issue_t.item_id_issue)a
   left outer join 
   (select store_item_batch_stock.item_id,sum(store_item_batch_stock.closing_stock) as stock from store_item_batch_stock
where store_item_batch_stock.department_id=35 and store_item_batch_stock.item_id in (
select 
distinct store_op_patient_issue_t.item_id_issue 
   from 
   store_op_patient_issue_m store_op_patient_issue_m inner JOIN store_op_patient_issue_t store_op_patient_issue_t 
     ON store_op_patient_issue_m.op_issue_id = store_op_patient_issue_t.op_issue_id
WHERE store_op_patient_issue_m.issue_date=to_date($P{curr_date},'dd/mm/yyyy') 
    AND store_op_patient_issue_m.department_id =35)  
    group by store_item_batch_stock.item_id )b on a.item_id_issue=b.item_id
]]></queryString>

