     `discharge_icd_code` discharge_icd_code
     RIGHT OUTER JOIN `inpatient` inpatient ON discharge_icd_code.`inpatient_id` =inpatient.`inpatient_id` 
     RIGHT OUTER JOIN `mas_icd` mas_icd ON discharge_icd_code.`icd_id` = mas_icd.`icd_id` 
     RIGHT OUTER JOIN `mas_icd_sub_category` mas_icd_sub_category ON mas_icd.`icd_sub_category_id` =mas_icd_sub_category.`icd_sub_category_id` 

where  discharge_icd_code.`inpatient_id`=$P{inpatient_id}]]></queryString>

