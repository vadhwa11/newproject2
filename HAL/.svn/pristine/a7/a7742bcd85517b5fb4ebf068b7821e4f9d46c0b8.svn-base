	<queryString><![CDATA[SELECT
     dg_orderhd.`orderhd_id` AS dg_orderhd_order_id,
     1 as number,
     dg_sample_collection_header.`sample_collection_header_id` as sample_collection_header_id,
     concat(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`) as patient_name,
     concat(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`) as service_person_name,
     patient.`age` AS age,
     mas_administrative_sex.`administrative_sex_name` AS sex,
     mas_relation.`relation_name`,
     dg_orderhd.`order_no` AS dg_orderhd_order_no,
     dg_orderhd.`order_status` AS dg_orderhd_order_status,
     patient.`service_no` AS service_no,
     dg_orderhd.`order_date` AS dg_orderhd_order_date,
     dg_orderhd.`order_time` AS dg_orderhd_order_time,
     dg_orderhd.`patient_type` as patient_type,
     mas_department.`department_name` as department_name,
     concat(patient.`age`,"/",mas_administrative_sex.`administrative_sex_name`) AS ageSex
    FROM
        `mas_department` mas_department RIGHT OUTER JOIN `dg_orderhd` dg_orderhd ON mas_department.`department_id` = dg_orderhd.`department_id`
     LEFT OUTER JOIN `patient` patient ON dg_orderhd.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`
     LEFT OUTER JOIN `dg_sample_collection_header` dg_sample_collection_header ON dg_orderhd.`orderhd_id` = dg_sample_collection_header.`order_id`
     LEFT OUTER JOIN `mas_administrative_sex` mas_administrative_sex ON patient.`sex_id` = mas_administrative_sex.`administrative_sex_id`
     $P!{QUERY} 
      AND dg_sample_collection_header.`order_status` != 'X'
order by patient.s_first_name,patient.s_middle_name,patient.s_last_name,dg_orderhd_order_date]]></queryString>

