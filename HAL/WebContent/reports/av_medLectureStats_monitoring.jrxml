	<queryString><![CDATA[select 

aircrew_date, lecture_subject,number_attended,remarks,
u.unit_name,
h.hospital_name

from avi_aircrew_medical_lectures l
left outer join mas_unit u on l.place_of_lecture=u.unit_id
left outer join mas_hospital h on l.hospital_id = h.hospital_id
where 
l.aircrew_date  between $P{fromDate} and $P{todate}
and h.hospital_id=$P{hospitalId} and h.command_id=$P{commandId}]]></queryString>

