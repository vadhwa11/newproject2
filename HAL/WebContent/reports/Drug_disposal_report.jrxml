	<queryString><![CDATA[SELECT 
sirt.batch_no, sirt.action_date,action_taken,sirt.expiry_date, msi.pvms_no, msi.nomenclature,msb.brand_name,md.department_name,sirt.received_qty
FROM store_internal_return_t sirt
Left join store_internal_return_m sirm ON sirt.return_main_id=sirm.id
Left join mas_store_item msi ON sirt.item_id=msi.item_id
Left join mas_store_brand msb ON sirt.brand_id=msb.brand_id
Left join mas_department md ON md.department_id= sirm.from_department_id

Where sirt.received_qty IS NOT Null and sirt.disposal_no=$P{DisposalNo} and sirm.hospital_id=$P{hospitalId}]]></queryString>

