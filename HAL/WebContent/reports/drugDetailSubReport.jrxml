	<queryString><![CDATA[SELECT DISTINCT
     patient_prescription_details.frequency_id AS patient_frequency_id,
     patient_prescription_details.dosage AS patient_details_dosage,
     patient_prescription_details.no_of_days AS patient_no_of_days,
     patient_prescription_details.total AS patient_details_total,
     patient_prescription_details.type AS patient_details_type,
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_frequency.frequency_name AS frequency_name,
     mas_store_item.nomenclature AS mas_item_nomenclature,
     patient_prescription_details.prescription_id AS patient__prescription_id,
     patient_prescription_details.remarks as patient_details_remarks,
     patient_prescription_details.route AS route 
FROM
     mas_store_item mas_store_item RIGHT OUTER JOIN patient_prescription_details patient_prescription_details ON mas_store_item.item_id = patient_prescription_details.item_id
     LEFT OUTER JOIN mas_frequency mas_frequency ON patient_prescription_details.frequency_id = mas_frequency.frequency_id
     LEFT OUTER JOIN patient_prescription_header patient_prescription_header ON patient_prescription_details.prescription_id = patient_prescription_header.prescription_id
     LEFT OUTER JOIN visit visit ON patient_prescription_header.visit_id = visit.visit_id
     LEFT OUTER JOIN patient patient ON patient_prescription_header.hin_id = patient.hin_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON patient.hospital_id = mas_hospital.hospital_id
     AND patient.hin_id = visit.hin_id
WHERE
     visit.visit_no = $P{visitNo}
     And patient.service_no = $P{serviceNo}
     And patient.hin_no = $P{hinNo}
     And mas_hospital.hospital_id=$P{hospitalId}]]></queryString>

