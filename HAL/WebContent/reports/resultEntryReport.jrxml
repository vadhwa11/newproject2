	<queryString><![CDATA[SELECT
     patient.`service_no` AS patient_service_no,
     concat(patient.`p_first_name`,patient.`p_middle_name`,patient.`p_last_name`)AS patient_name,
     patient.`age` AS patient_age,
     patient.`date_of_birth` AS patient_date_of_birth,
     mas_department.`department_name` AS mas_department_department_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `mas_hospital` mas_hospital LEFT OUTER JOIN `patient` patient ON mas_hospital.`hospital_id` = patient.`hospital_id`
     RIGHT OUTER JOIN `dg_sample_collection_header` dg_sample_collection_header ON patient.`hin_id` = dg_sample_collection_header.`hin_id`
     AND mas_hospital.`hospital_id` = dg_sample_collection_header.`hospital_id`
     LEFT OUTER JOIN `mas_department` mas_department ON dg_sample_collection_header.`department_id` = mas_department.`department_id`
WHERE
     dg_sample_collection_header.`diagnosis_no` = $P{diagNo}
     And patient.`service_no` = $P{serviceNo}
     And patient.`hin_no` = $P{hinNo}]]></queryString>

