	<queryString><![CDATA[SELECT
     mas_store_item.`pvms_no` AS mas_store_item_pvms_no,
     mas_store_item.`nomenclature` AS mas_store_item_nomenclature,
     mas_store_item.`cost_price` AS mas_store_item_cost_price,
     mas_store_supplier.`supplier_name` AS mas_store_supplier_supplier_name,
     mas_manufacturer.`manufacturer_name` AS mas_manufacturer_manufacturer_name,
     store_grn_m.`grn_no` AS store_grn_m_grn_no,
     store_grn_m.`grn_date` AS store_grn_m_grn_date,
     store_grn_m.`at_so_no` AS store_grn_m_at_so_no,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `mas_store_supplier` mas_store_supplier RIGHT OUTER JOIN `mas_store_item` mas_store_item ON mas_store_supplier.`supplier_id` = mas_store_item.`supplier_id`
     LEFT OUTER JOIN `mas_manufacturer` mas_manufacturer ON mas_store_item.`manufacturer_id` = mas_manufacturer.`manufacturer_id`
     LEFT OUTER JOIN `store_grn_t` store_grn_t ON mas_store_item.`item_id` = store_grn_t.`item_id`
     LEFT OUTER JOIN `store_grn_m` store_grn_m ON store_grn_t.`grn_master_id` = store_grn_m.`grn_master_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON store_grn_m.`hospital_id` = mas_hospital.`hospital_id`
WHERE
     mas_store_item.pvms_no = $P{Pvms_No}
 AND mas_store_item.section_id = $P{Section_Id}
 AND mas_store_item.imported_item = 'Y']]></queryString>

