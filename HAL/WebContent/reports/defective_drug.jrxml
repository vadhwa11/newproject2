	<queryString><![CDATA[select 
i.nomenclature, f.manufacturer_name, u.unit_name,
t.defect_qty, t.batch_no, t.manufactureing_date, t.expiry_date, t.disposal,


(case 
when b.brand_generic='B' then 'B'
when b.brand_generic='G' then 'G' 
end) as brand_generic


from  store_defective_drug_t t
left outer join store_defective_drug_m m on t.DEFECT_M_ID= m.DEFECT_M_ID
left outer join mas_store_item i on t.ITEM_ID =i.ITEM_ID
left outer join mas_manufacturer f on t.MANUFACTURED_BY= f.MANUFACTURER_ID
left outer join mas_store_item_conversion c on i.ITEM_CONVERSION_ID= c.ITEM_CONVERSION_ID
left outer join mas_store_unit u on c.item_conversion_id=u.unit_id
left outer join store_item_batch_stock b on t.item_id=b.item_id
where t.disposal='n'
group by i.nomenclature, f.manufacturer_name, u.unit_name,
t.defect_qty, t.batch_no, t.manufactureing_date, t.expiry_date, t.disposal, b.brand_generic]]></queryString>

