	<queryString><![CDATA[SELECT
     hr_orderly_duty_entry.`duty_date` AS hr_orderly_duty_entry_duty_date,
     mas_employee.`first_name` AS mas_employee_first_name,
     mas_employee.`service_no` AS mas_employee_service_no,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     concat(hr_duty_time_master.`duty_from_time`," - ",hr_duty_time_master.`duty_to_time`) AS duty_time,
     mas_department.`department_name` as department_name
FROM
     `hr_orderly_duty_entry` hr_orderly_duty_entry LEFT JOIN mas_department `mas_department` on hr_orderly_duty_entry.`department_id`=mas_department.`department_id`
     left JOIN mas_employee `mas_employee` ON hr_orderly_duty_entry.`emp_id`=mas_employee.`employee_id`
    LEFT JOIN `mas_rank` mas_rank ON mas_employee.`rank_id` = mas_rank.`rank_id`
    LEFT JOIN `hr_duty_time_master` hr_duty_time_master ON hr_orderly_duty_entry.`duty_time_id` = hr_duty_time_master.`id`
where hr_orderly_duty_entry.duty_date between $P{fromDate} and $P{toDate}]]></queryString>

