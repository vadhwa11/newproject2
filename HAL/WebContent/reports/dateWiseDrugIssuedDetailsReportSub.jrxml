	<queryString><![CDATA[select pph.prescription_date,sum(ppd.total)
from patient_prescription_header pph 
left outer join patient_prescription_details ppd on pph.prescription_id=ppd.prescription_id 
left outer join inpatient i on pph.inpatient_id=i.inpatient_id
left outer join patient p on i.hin_id=p.hin_id
left outer join mas_hospital mh on pph.hospital_id=mh.hospital_id
left outer join mas_store_item msi on ppd.item_id=msi.item_id
where pph.prescription_date between to_date($P{fromDate},'dd/mm/yyyy')  
and to_date($P{toDate},'dd/mm/yyyy')
 and pph.inpatient_id is not null and ppd.item_id= $P{itemId} 
and p.hospital_id=$P{hospitalId} and ppd.total>0
and pph.hin_id=$P{hinId}
group by pph.prescription_date]]></queryString>

