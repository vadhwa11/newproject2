item.nomenclature,total,dosage,no_of_days,au.item_unit_name,
f.frequency_name
FROM
ot_booking ot_booking
LEFT OUTER JOIN VISIT VISIT  ON ot_booking.visit_id=VISIT.VISIT_ID


left outer join patient_prescription_header pph   ON VISIT.VISIT_ID = pph.VISIT_ID
left OUTER JOIN patient_prescription_details ppd ON  pph.prescription_id= ppd.prescription_id
left outer join mas_store_item  item   on  ppd.item_id= item.item_id
left outer join mas_store_item_conversion  au   on  item.item_conversion_id= au.item_conversion_id
left outer join mas_frequency  f   on  f.frequency_id= ppd.frequency_id


where ppd.ot_stage ='POSTSURGERY'  and ot_booking.booking_id=$P{bookingId}]]></queryString>

