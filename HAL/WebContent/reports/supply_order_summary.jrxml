	<queryString><![CDATA[SELECT concat(store_po_header.`po_number`,"  ",DATE_FORMAT(store_po_header.`po_date`, '%d-%b-%Y')) AS SONumber,
       mas_store_supplier.`supplier_name` AS SupplierName,
       store_po_header.`net_amount` AS Amount,
case when store_po_header.`status` = 'c' then 'Cancelled' else '' end  as Status
FROM
`store_po_header` store_po_header
LEFT OUTER JOIN `mas_store_supplier` mas_store_supplier
ON store_po_header.`supplier_id` = mas_store_supplier.`supplier_id`
LEFT OUTER JOIN `store_po_detail` store_po_detail ON  store_po_header.`po_id` = store_po_detail.`po_id`
LEFT OUTER JOIN `mas_store_item` mas_store_item ON store_po_detail.`item_id` = mas_store_item.`item_id`
where  store_po_header.department_id = $P{deptId} and po_date between $P{fromDate} and $P{toDate}
$P!{stringVariable}
group by store_po_header.`po_number`]]></queryString>

