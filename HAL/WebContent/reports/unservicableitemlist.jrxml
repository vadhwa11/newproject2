	<queryString><![CDATA[SELECT
     mas_store_item.`pvms_no` AS mas_store_item_pvms_no,
     mas_store_item.`nomenclature` AS mas_store_item_nomenclature,
     store_disposal_t.`qty` AS store_disposal_t_qty,
     store_disposal_t.`cost` AS store_disposal_t_cost,
     store_disposal_t.`remarks` AS store_disposal_t_remarks,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `store_disposal_m` store_disposal_m LEFT OUTER JOIN `store_disposal_t` store_disposal_t ON store_disposal_m.`id` = store_disposal_t.`disposal_m_id`
     LEFT OUTER JOIN `mas_store_item` mas_store_item ON store_disposal_t.`item_id` = mas_store_item.`item_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON store_disposal_m.`hospital_id` = mas_hospital.`hospital_id`

WHERE store_disposal_m.`department_id` = $P{DEPART}
AND store_disposal_m.`date` >= $P{FROM_DATE}
AND store_disposal_m.`date` <= $P{TO_DATE}]]></queryString>

