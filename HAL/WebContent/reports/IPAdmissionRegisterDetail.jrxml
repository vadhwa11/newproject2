	<queryString><![CDATA[select distinct
     inpatient.init_diagnosis as inpatient_init_diagnosis,
     inpatient.ad_no as inpatient_ad_no,
     inpatient.date_of_addmission as inpatient_date_of_addmission,
     inpatient.time_of_addmission as inpatient_time_of_addmission,
     inpatient.diagnosis_id as inpatient_diagnosis_id,
     patient.service_no as patient_service_no,
     (patient.s_first_name || ' ' || patient.s_middle_name || ' ' || patient.s_last_name)as sname,
     (patient.p_first_name || ' ' || patient.p_middle_name || ' ' || patient.p_last_name)as patient_name,
     mas_department.department_name as mas_department_department_name,
     inpatient.init_diagnosis as diagnosis,
     mas_relation.relation_name as mas_relation_relation_name,
     mas_unit.unit_name as mas_unit_unit_name,
     mas_rank.rank_name as mas_rank_rank_name,
     mas_service_type.service_type_name as service_type_name,
     patient.age as patient_age,
     patient.sex_id as patient_sex_id,
     mas_administrative_sex.administrative_sex_name as administrative_sex_name,
     mas_hospital.hospital_name as hospital_name

from
    
      inpatient inpatient 
	inner join patient patient on patient.hin_id = inpatient.hin_id
inner join  mas_rank mas_rank  on mas_rank.rank_id = patient.rank_id
     inner join mas_unit mas_unit on patient.unit_id = mas_unit.unit_id
     inner join mas_relation mas_relation on patient.relation_id = mas_relation.relation_id
     inner join mas_service_type mas_service_type on patient.service_type_id = mas_service_type.service_type_id
     inner join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
     inner join mas_department mas_department on inpatient.ad_ward_id = mas_department.department_id
     inner join mas_hospital mas_hospital on inpatient.hospital_id=mas_hospital.hospital_id
left outer join discharge_icd_code dic on inpatient.inpatient_id=dic.inpatient_id
left outer join mas_icd icd on dic.icd_id=icd.icd_id
where
     trunc(inpatient.date_of_addmission)  between $P{fromDate} and  $P{toDate}
and inpatient.ad_status != 'c' 

$P!{qry}

order  by  concat(inpatient.date_of_addmission,inpatient.time_of_addmission) asc]]></queryString>

