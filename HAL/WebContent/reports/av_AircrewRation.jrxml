	<queryString><![CDATA[select 
service_no, aircrew_date, aircrew_time, full_name, age,
R.rank_name, U.unit_name, S.administrative_sex_name, I.item_name, 
I.authorised_qty, D.supplied_qty, Hosp.hospital_name
from av_aircrew_ration_hd H
left outer join mas_rank R on H.rank_id=R.rank_id
left outer join mas_unit U on H.unit_id=U.unit_id
left outer join mas_administrative_sex S on H.sex_id=S.administrative_sex_id
left outer join av_aircrew_ration_dt D on H.id=D.ration_hd_id
left outer join av_authorised_item I on D.auth_item_id=I.id
left outer join mas_hospital Hosp on H.hospital_id=hosp.hospital_id
$P!{query}]]></queryString>

