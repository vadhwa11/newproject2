	<queryString><![CDATA[SELECT
     bl_refund_header.`refund_no` AS bl_refund_header_refund_no,
     bl_refund_header.`refund_date` AS bl_refund_header_refund_date,
     CONCAT(patient.`p_first_name`,' ',patient.`p_middle_name`,' ',patient.`p_last_name`,' ',inpatient.`ad_no`)as Particulars,
     bl_refund_header.`total_refund_amt` AS bl_refund_header_total_refund_amt
FROM
     `bl_refund_header` bl_refund_header LEFT OUTER JOIN `patient` patient ON bl_refund_header.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `inpatient` inpatient ON patient.`hin_id` = inpatient.`hin_id`
WHERE
    bl_refund_header.refund_date between $P{fromDate} and $P{toDate}]]></queryString>

