	<queryString><![CDATA[SELECT
     patient.`service_no` AS patient_service_no,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_address` AS mas_unit_unit_address,
     mas_unit.`unit_name` AS mas_unit_unit_name,
 CONCAT(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`)as patientName,
     CONCAT(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)as serviceName,
     md_cardic_claim_advance.`retirement_date` AS md_cardic_claim_advance_retirement_date,
     md_cardic_claim_advance.`basic_pay` AS md_cardic_claim_advance_basic_pay,
     md_cardic_claim_advance.`treatment_detail` AS md_cardic_claim_advance_treatment_detail,
     md_cardic_claim_advance.`qualifying_amount` AS md_cardic_claim_advance_qualifying_amount
FROM
     `patient` patient RIGHT OUTER JOIN `md_cardic_claim_advance` md_cardic_claim_advance ON patient.`hin_id` = md_cardic_claim_advance.`hin_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`]]></queryString>

