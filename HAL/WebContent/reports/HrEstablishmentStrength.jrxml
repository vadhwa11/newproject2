	<queryString><![CDATA[SELECT
     CONCAT(mas_employee.`first_name`," ",mas_employee.`middle_name`," ",mas_employee.`last_name`)AS name,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_employee.`service_no` AS mas_employee_service_no,
     mas_employee.`status` AS mas_employee_status,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     hr_specialist_master.`specialist_name` AS hr_specialist_master_specialist_name,
     mas_medical_category.`medical_category_name` AS mas_medical_category_medical_category_name,
     mas_trade.`trade_name` AS mas_trade_trade_name,
     mas_employee.`join_date` AS mas_employee_join_date
FROM
     `mas_rank` mas_rank RIGHT OUTER JOIN `mas_employee` mas_employee ON mas_rank.`rank_id` = mas_employee.`rank_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON mas_employee.`unit_id` = mas_unit.`unit_id`
     LEFT OUTER JOIN `hr_specialist_master` hr_specialist_master ON mas_employee.`speciality_id` = hr_specialist_master.`id`
     LEFT OUTER JOIN `emp_afmsf_det` emp_afmsf_det ON mas_employee.`employee_id` = emp_afmsf_det.`employee_id`
     LEFT OUTER JOIN `mas_trade` mas_trade ON mas_employee.`trade_id` = mas_trade.`trade_id`
     LEFT OUTER JOIN `mas_medical_category` mas_medical_category ON emp_afmsf_det.`medical_category_id` = mas_medical_category.`medical_category_id`
WHERE
     mas_trade.trade_name = "Med Asst"
     and mas_employee.unit_id = $P{unit}]]></queryString>

