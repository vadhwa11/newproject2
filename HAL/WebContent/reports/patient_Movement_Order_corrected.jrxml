	<queryString><![CDATA[SELECT
     mis_frw.`frw_serial_no` AS mis_frw_frw_serial_no,
     mis_frw.`por` AS mis_frw_por,
     patient.`service_no` AS patient_service_no,
     mas_disposal.`disposal_name` AS mas_disposal_disposal_name,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     concat(patient.`p_first_name`," ",
     patient.`p_middle_name`," ",
     patient.`p_last_name`) AS patient_name,
	to_class
FROM
     `patient` patient INNER JOIN `mis_frw` mis_frw ON patient.`hin_id` = mis_frw.`hin_id`
     LEFT JOIN `mas_disposal` mas_disposal ON mis_frw.`disposal_id` = mas_disposal.`disposal_id`
     LEFT JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
where service_no=$P{serviceNo} $P!{disposalName}]]></queryString>

