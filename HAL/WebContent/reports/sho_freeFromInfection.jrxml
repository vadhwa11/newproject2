	<queryString><![CDATA[select
ffi.infection_date, ffi.inf_purpose, ffi.inf_place, ffi.inf_particular, ffi.inf_fit_unfit,
h.hospital_name
from free_from_infection ffi
left outer join mas_hospital h on ffi.hospital_id=h.hospital_id
where infection_date between $P{fromDate} and $P{toDate} and ffi.hospital_id=$P{hospitalId}
order by infection_date desc]]></queryString>

