	<queryString><![CDATA[SELECT
     mas_bed.bed_no AS mas_bed_bed_no,
     mas_department.department_name AS mas_department_department_name,
     mas_room.room_code AS mas_room_room_code,
mas_bed_status.bed_status_name as a,
case when  mas_bed.status ='Y' or mas_bed.status='y' then 'Active' else 'Inactive' end AS mas_bed_status_bed_status_name
FROM 
mas_bed mas_bed
left outer JOIN   mas_department mas_department  ON mas_department.department_id = mas_bed.department_id
     left outer JOIN mas_room mas_room ON mas_department.department_id = mas_room.department_id
     AND mas_room.room_id = mas_bed.room_id
left outer JOIN  mas_bed_status mas_bed_status ON mas_bed.bed_status_id = mas_bed_status.bed_status_id
ORDER BY
     mas_department.department_name ASC]]></queryString>

