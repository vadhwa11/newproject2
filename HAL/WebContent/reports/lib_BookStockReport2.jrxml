	<parameter name="QUERY" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     lib_book_stock_taking_hd.`stock_taking_no` AS lib_book_stock_taking_hd_stock_taking_no,
     mas_book.`book_name` AS mas_book_book_name,
     lib_book_stock.`issue_qty` AS lib_book_stock_issue_qty,
     lib_book_stock.`closing_stock_qty` AS lib_book_stock_closing_stock_qty,
     (lib_book_stock.`closing_stock_qty`-lib_book_stock.`issue_qty`)as available_qty,
     mas_publisher.`publisher_name` AS mas_publisher_publisher_name,
     mas_book_sub_class.`sub_class_name` AS mas_book_sub_class_sub_class_name,
     mas_book_category.`book_category_name` AS mas_book_category_book_category_name,
     mas_book_class.`class_name` AS mas_book_class_class_name,
     mas_book.`year_publication` AS mas_book_year_publication,
     mas_book.`author_name` AS mas_book_author_name,
     mas_book.`types` AS mas_book_types,
     if(mas_book.`types`='b',"Book",(if(mas_book.`types`='j',"Journal",(if(mas_book.`types`='m',"Magazine","All")))))as mas_type,
     mas_book.`edition` AS mas_book_edition,
     mas_book.`price` AS mas_book_price,
     mas_department.`department_name` AS mas_department_department_name
FROM
     `lib_book_stock_taking_hd` lib_book_stock_taking_hd RIGHT OUTER JOIN `lib_book_stock_taking_dt` lib_book_stock_taking_dt ON lib_book_stock_taking_hd.`id` = lib_book_stock_taking_dt.`sock_taking_hd_id`
     LEFT OUTER JOIN `lib_book_stock` lib_book_stock ON lib_book_stock_taking_dt.`book_sock_id` = lib_book_stock.`id`
     LEFT OUTER JOIN `mas_book` mas_book ON lib_book_stock.`book_id` = mas_book.`book_id`
     LEFT OUTER JOIN `mas_book_category` mas_book_category ON mas_book.`book_category_id` = mas_book_category.`book_category_id`
     LEFT OUTER JOIN `mas_book_class` mas_book_class ON mas_book.`book_class_id` = mas_book_class.`book_class_id`
     LEFT OUTER JOIN `mas_book_sub_class` mas_book_sub_class ON mas_book.`book_sub_class_id` = mas_book_sub_class.`book_sub_class_id`
     LEFT OUTER JOIN `mas_publisher` mas_publisher ON mas_book.`publisher_id` = mas_publisher.`publisher_id`
     LEFT OUTER JOIN `mas_department` mas_department ON mas_book.`department_id` = mas_department.`department_id`
     AND mas_book_class.`book_class_id` = mas_book_sub_class.`book_class_id`
     AND mas_book_category.`book_category_id` = mas_book_class.`book_category_id`
 $P!{QUERY}]]></queryString>

