	<queryString><![CDATA[SELECT
     store_internal_indent_m.demand_no AS store_m_demand_no,
     store_internal_indent_m.DEMAND_DATE AS indent_date,
     md.department_name AS from_dept,
    md1.department_name AS to_dept,
     mas_store_item.pvms_no AS mas_pvms_no,
     mas_store_item.nomenclature AS mas_nomenclature,
     store_internal_indent_t.qty_request AS store_t_qty_request,au.item_unit_name
FROM
     store_internal_indent_m store_internal_indent_m LEFT OUTER JOIN store_internal_indent_t store_internal_indent_t ON store_internal_indent_m.id = store_internal_indent_t.internal_id
     LEFT OUTER JOIN mas_store_item mas_store_item ON store_internal_indent_t.item_id = mas_store_item.item_id
     LEFT OUTER JOIN mas_department md ON store_internal_indent_m.department_id = md.department_id
LEFT OUTER JOIN mas_department md1 ON store_internal_indent_m.to_store = md1.department_id
left outer join mas_store_item_conversion au on mas_store_item.item_conversion_id=au.item_conversion_id
where store_internal_indent_m.id = $P{mrId}  and store_internal_indent_t.qty_request>0
and store_internal_indent_m.hospital_id = $P{hospitalId}

order by mas_store_item.nomenclature]]></queryString>

