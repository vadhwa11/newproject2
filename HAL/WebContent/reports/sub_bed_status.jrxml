	<queryString><![CDATA[select  i.REMG,ADM,b.Death,c.DIS,d.REMD,e.TIN,f.TOUT,g.SIL,h.DIL from
(SELECT count(*) as ADM FROM inpatient inpatient  where inpatient.date_of_addmission = $P{toDate} and  inpatient.department_id = $P{deptId}) a,
(SELECT count(*) as Death FROM expiry_details expiry_details where expiry_details.expiry_date  = $P{toDate}  and  expiry_details.ward_id = $P{deptId}) b,
(SELECT count(*) as DIS FROM discharge discharge where discharge.date_of_discharge = $P{toDate} and  discharge.ward_id = $P{deptId}) c,
(SELECT count(*) as REMD FROM inpatient inp2 where inp2.date_of_addmission < $P{toDate} and inp2.ad_status = 'A' and inp2.department_id = $P{deptId}) d,
(SELECT count(distinct(inp3.inpatient_id)) as TIN FROM inpatient inp3 where inp3.date_of_addmission = $P{toDate} and inp3.department_id = $P{deptId} and inp3.ad_ward_id !=$P{deptId}) e,
(SELECT count(distinct(inp4.inpatient_id)) as TOUT FROM inpatient inp4 where inp4.date_of_addmission = $P{toDate} and inp4.department_id != $P{deptId} and inp4.ad_ward_id =$P{deptId}) f,
(SELECT count(distinct(sil.inpatient_id)) as SIL FROM sil_dil_status sil where sil.last_chg_date = $P{toDate} and sil.department_id = $P{deptId} and sil.condition_status ='SIL' ) g,
(SELECT count(distinct(dil.inpatient_id)) as DIL FROM sil_dil_status dil where dil.last_chg_date = $P{toDate} and dil.department_id = $P{deptId} and dil.condition_status ='DIL' ) h,
(SELECT count(*) as REMG FROM inpatient inp5 where inp5.date_of_addmission <= $P{toDate} and inp5.ad_status = 'A' and inp5.department_id = $P{deptId}) i
;]]></queryString>

