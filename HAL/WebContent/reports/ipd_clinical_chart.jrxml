	<parameter name="fromDate" isForPrompting="true" class="java.util.Date"/>
	<parameter name="toDate" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT
     ipd_clinical_chart.`ad_no` AS ipd_clinical_chart_ad_no,
     ipd_clinical_chart.`temperature` AS ipd_clinical_chart_temperature,
     ipd_clinical_chart.`pulse` AS ipd_clinical_chart_pulse,
     ipd_clinical_chart.`respiration` AS ipd_clinical_chart_respiration,
     ipd_clinical_chart.`bp` AS ipd_clinical_chart_bp,
     ipd_clinical_chart.`bowel` AS ipd_clinical_chart_bowel,
     ipd_clinical_chart.`pain` AS ipd_clinical_chart_pain,
     ipd_clinical_chart.`fhr` AS ipd_clinical_chart_fhr,
     ipd_clinical_chart.`clinical_date` AS ipd_clinical_chart_clinical_date,
     ipd_clinical_chart.`clinical_time` AS ipd_clinical_chart_clinical_time,
     ipd_clinical_chart.`remarks` AS ipd_clinical_chart_remarks,
     concat(patient.`p_first_name`," ",patient.`p_last_name`)AS patientName,
     mas_department.`department_name` AS mas_department_department_name
FROM
     `patient` patient LEFT OUTER JOIN `ipd_clinical_chart` ipd_clinical_chart ON patient.`hin_id` = ipd_clinical_chart.`hin_id`
     INNER JOIN `mas_department` mas_department ON ipd_clinical_chart.`department_id` = mas_department.`department_id`
WHERE
     mas_department.`department_name` = $P{departmentName}
     and ipd_clinical_chart.`clinical_date` >= $P{fromDate}
     and ipd_clinical_chart.`clinical_date` <= $P{toDate}]]></queryString>

