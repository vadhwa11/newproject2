	<queryString><![CDATA[SELECT
     DMA_REGISTER_DETAILS."DOSAGE" AS DMA_REGISTER_DETAILS_DOSAGE,
     DMA_REGISTER_DETAILS."ISSUE_QTY" AS DMA_REGISTER_DETAILS_ISSUE_QTY,
     MAS_FREQUENCY."FREQUENCY_NAME" AS MAS_FREQUENCY_FREQUENCY_NAME,
     MAS_STORE_ITEM."NOMENCLATURE" AS MAS_STORE_ITEM_NOMENCLATURE,
     MAS_STORE_ITEM."PVMS_NO" AS MAS_STORE_ITEM_PVMS_NO,
     DMA_REGISTER_DETAILS."REMARKS" AS REMARKS,
     DMA_REGISTER_DETAILS."ROUTE" AS DMA_REGISTER_DETAILS_ROUTE,
     DMA_REGISTER_DETAILS."NO_OF_DAYS" AS DMA_REGISTER_DETAILS_NO_OF_DAY,
patient.service_no as service_no
FROM
     "DMA_REGISTER_HEADER" DMA_REGISTER_HEADER INNER JOIN "DMA_REGISTER_DETAILS" DMA_REGISTER_DETAILS ON DMA_REGISTER_HEADER."DMA_REGISTER_HEADER_ID" = DMA_REGISTER_DETAILS."DMA_REGISTER_HEADER_ID"
     LEFT OUTER JOIN "MAS_STORE_ITEM" MAS_STORE_ITEM ON DMA_REGISTER_DETAILS."ITEM_ID" = MAS_STORE_ITEM."ITEM_ID"
     LEFT OUTER JOIN "MAS_FREQUENCY" MAS_FREQUENCY ON DMA_REGISTER_DETAILS."FREQUENCY_ID" = MAS_FREQUENCY."FREQUENCY_ID"
left join "PATIENT" patient on DMA_REGISTER_HEADER.hin_id=patient.hin_id
where dma_register_date between $P{fromDate} and $P{toDate} and DMA_REGISTER_HEADER.hospital_id=$P{hospitalId}
$P!{qry}]]></queryString>

