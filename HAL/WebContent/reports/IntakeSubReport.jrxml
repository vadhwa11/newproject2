	<queryString><![CDATA[SELECT    
     ipd_intake.ip_intake_date AS ipd_intake_date,
     sum(ipd_intake.intake),
     sum(ipd_intake.iv_count)
FROM
     ipd_intake_output_chart ipd_intake_output_chart
     LEFT OUTER JOIN ipd_intake ipd_intake ON ipd_intake_output_chart.intakeoutput_id = ipd_intake.intakeoutput_id
WHERE
     ipd_intake_output_chart.ad_no = $P{adNo}
  and ipd_intake_output_chart.intake_date >= $P{fromDate}
     and ipd_intake_output_chart.intake_date <=  $P{toDate}
group by  ipd_intake.ip_intake_date]]></queryString>

