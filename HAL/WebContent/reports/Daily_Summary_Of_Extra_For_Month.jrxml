	<queryString><![CDATA[SELECT
     diet_daily_extra_summary.`diet_summary_date` as date,
     Sum(diet_daily_extra_summary.`extra_item_qty`)AS qty,
     mas_diet_indent_item.`indent_item_name` AS mas_diet_indent_item_indent_item_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `mas_department` mas_department RIGHT OUTER JOIN `diet_daily_extra_summary` diet_daily_extra_summary ON mas_department.`department_id` = diet_daily_extra_summary.`department_id`
     LEFT OUTER JOIN `mas_diet_indent_item` mas_diet_indent_item ON diet_daily_extra_summary.`indent_item_id` = mas_diet_indent_item.`diet_indent_item_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON diet_daily_extra_summary.`hospital_id` = mas_hospital.`hospital_id`
WHERE
     MONTHNAME(diet_daily_extra_summary.`diet_summary_date`) = $P{MONTH}
     and YEAR(diet_daily_extra_summary.`diet_summary_date`) = $P{YEAR}
     and  diet_daily_extra_summary.`department_id`    = $P{DEPART}
GROUP BY
     diet_daily_extra_summary.`indent_item_id`,
     diet_daily_extra_summary.`diet_summary_date`]]></queryString>

