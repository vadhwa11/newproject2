	</parameter>
	<queryString><![CDATA[SELECT
d.indent_item_id,m.indent_item_name,extra_item_qty,diet_summary_date,main.Unit_Name
FROM diet_daily_extra_summary d ,mas_diet_indent_item m , (SELECT
Indent_Item_id,d.Unit_ID,Unit_Name FROM diet_extra_item_formula d
,mas_store_unit m Where d.Unit_ID = m.Unit_ID) as main
Where d.indent_item_id = m.Diet_indent_item_id And d.indent_item_id =
main.Indent_Item_id And m.Extra_item ='y'
And department_id = $P{departmentid} And diet_summary_date=$P{DietSummaryDate}

Union All

SELECT main.indent_item_id,m.indent_item_name,0 as
extra_item_qty,$P{DietSummaryDate} as diet_summary_date,main.Unit_Name
FROM mas_diet_indent_item m , (SELECT
Indent_Item_id,d.Unit_ID,Unit_Name FROM diet_extra_item_formula d
,mas_store_unit m Where d.Unit_ID = m.Unit_ID) as main
Where m.Diet_indent_item_id = main.Indent_Item_id And m.Extra_item ='y'
And m.Diet_indent_item_id NOT IN ( SELECT indent_item_id FROM
diet_daily_extra_summary Where 
department_id = $P{departmentid} And diet_summary_date=$P{DietSummaryDate})]]></queryString>

