	<queryString><![CDATA[select 
sgm.grn_date,qty_advised,

 shortage ,rr_remarks,surplus,
msi.item_id, msi.pvms_no, msi.nomenclature, sgt.received_qty,sgt.rejected_qty,sgt.accepted_qty,sgt.unit_rate,sgt.amount_value,sgt.expiry_date,sgt.manufacturer_date,sgt.batch_no,
sgm.grn_no, u.item_unit_name,m.manufacturer_name,msb.brand_name,
sgm.challan_no,sgm.receive_type, ph.po_number,irh.rc_no,sgm.invoice_amount,sgm.invoice_date,sgm.invoice_no,
isnull(mss.supplier_name,'')+' '+isnull(mss.address1,'')+' '+isnull(mss.address2,'') as vendor_name,
isnull(m.manufacturer_name,'')+' '+ 
case when m.manufacturer_name!='' then '/' else '' end +isnull(msb.brand_name,'') as manf_brand_name,
d.department_name,
sgt.discount,
sgt.amount_value_without_discount
from store_grn_m sgm
left outer join store_grn_t sgt on sgm.grn_master_id=sgt.grn_master_id
left outer join mas_manufacturer m on sgt.manufacturer_id=m.manufacturer_id
left outer join mas_store_brand msb   on msb.brand_id=sgt.brand_id
left outer join mas_store_item msi on msi.item_id=sgt.item_id
left outer join mas_store_item_conversion u on msi.item_conversion_id=u.item_conversion_id
left outer join store_po_header ph  on ph.po_id=sgm.po_id
left outer join rc_header irh  on irh.id=sgm.rc_id
left outer join mas_store_supplier mss  on mss.supplier_id=sgm.supplier_id
left outer join mas_department d on sgm.department_id=d.department_id
where sgm.grn_master_id=$P{grnId}]]></queryString>

