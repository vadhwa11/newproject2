	<queryString><![CDATA[select 
(me.first_name+' '+isnull(me.middle_name,'')+' '+isnull(me.last_name,'')) as surgaon_name,
patient.age,
(patient.p_first_name+ ' ' +patient.p_middle_name+ ' ' +patient.p_last_name)AS patient_p_first_name,
obs.role,surgery_date,surgery_time,ot.ot_name,
opah.anashteic_details,
substring(lower(surgery_start_time),0,6) as stime,
substring(lower(surgery_end_time),0,6) as etime,
case when ob.ot_booking_status='C' then 'Completed' else 'Pending' end as status,surgery_start_time,surgery_end_time,
STUFF((SELECT distinct ',' + icd.icd_name
         FROM discharge_icd_code dcc 
LEFT OUTER JOIN mas_icd icd ON icd.icd_id=dcc.icd_id
         WHERE dcc.inpatient_id=ob.inpatient_id
            FOR XML PATH(''), TYPE
            ).value('.', 'NVARCHAR(MAX)')
        ,1,1,'') as icd_name,

STUFF((SELECT distinct ',' + MAS_CHARGE_CODE.charge_code_name
FROM opd_surgery_detail osd 
left outer join MAS_CHARGE_CODE MAS_CHARGE_CODE  on  osd.CHARGE_CODE_ID = MAS_CHARGE_CODE.CHARGE_CODE_ID
left outer join ot_pre_anesthesia_detail ot_pre_anesthesia_detail on ot_pre_anesthesia_detail.opd_surgery_detail =osd.id
left outer join ot_booking_dt ot_booking_dt on ot_pre_anesthesia_detail.id= ot_booking_dt.ot_pre_anesthesia_detail
left outer join ot_booking ot_booking on ot_booking_dt.ot_booking_hd=ot_booking.booking_id
WHERE
 ot_booking.surgery_date  between $P{fromDate} and  $P{toDate}
            FOR XML PATH(''), TYPE
            ).value('.', 'NVARCHAR(MAX)')
        ,1,1,'') as kk
from ot_booking ob
left outer join ot_book_surgeon obs on obs.booking_id = ob.booking_id
left outer join ot_booking_dt ot_booking_dt on ot_booking_dt.ot_booking_hd=ob.booking_id

left outer join ot_pre_anesthesia_detail opad on ot_booking_dt.ot_pre_anesthesia_detail= opad.id
left outer join ot_pre_anesthesia_hd opah on opad.anesthesia_hd_id = opah.id
left outer join mas_employee me on me.employee_id= obs.employee_id
left outer join patient patient on patient.hin_id= ob.hin_id



 LEFT OUTER JOIN mas_ot ot ON ot.ot_id=ob.ot_id
where  ob.surgery_date  between $P{fromDate} and  $P{toDate}
and obs.role='Surgeon'
$P!{qry}

group by 
me.first_name,me.middle_name,me.last_name,
patient.age,
patient.p_first_name,patient.p_middle_name,patient.p_last_name,ob.inpatient_id,
ob.ot_booking_status,obs.role,surgery_date,surgery_time,ot_name,
opah.anashteic_details,surgery_start_time,surgery_end_time]]></queryString>

