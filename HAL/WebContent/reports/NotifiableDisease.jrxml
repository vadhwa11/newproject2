	<queryString><![CDATA[select  
(patient.p_first_name ||' '||patient.p_middle_name ||' '||patient.p_last_name) as patientName,
patient.service_no,
patient.age, s.administrative_sex_name,
patient.total_service_period,
patient.permanent_address,
notifiable_disease.date_of_onset,
notifiable_disease.date_of_reporting_sick,
notifiable_disease.details_of_case,
notifiable_disease.clinical,
notifiable_disease.bacteriological,
notifiable_disease.disinfection,
notifiable_disease.contact,
notifiable_disease.suspected_source_of_infection,
notifiable_disease.general_remarks,
mas_unit.UNIT_NAME,
mas_rank.RANK_NAME,
notifiable_disease.general_remarks,
notifiable_disease.date_of_preventive,
notifiable_disease.date_of_admission
from notifiable_disease notifiable_disease
left outer join patient patient on  notifiable_disease.hin_id = patient.hin_id
left outer join mas_administrative_sex s on s.administrative_sex_id=patient.sex_id
left outer join mas_rank on patient.RANK_ID=mas_rank.RANK_ID
left outer join mas_unit on patient.UNIT_ID=mas_unit.UNIT_ID

where notifiable_disease.id=$P{notifiableId}]]></queryString>

