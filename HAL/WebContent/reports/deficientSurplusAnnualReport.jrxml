	<queryString><![CDATA[SELECT
     emp_afmsf_det.`emp_name` AS emp_afmsf_det_emp_name,
     emp_afmsf_det.`letter_no` AS emp_afmsf_det_letter_no,
     emp_afmsf_det.`posted_to` AS emp_afmsf_det_posted_to,
     emp_afmsf_det.`vide_with_date` AS emp_afmsf_det_vide_with_date,
     emp_afmsf_det.`service_no` AS emp_afmsf_det_service_no,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_unit1.`unit_name` AS posted_to,
     emp_afmsf_det.`remarks` AS emp_afmsf_det_remarks,
     emp_afmsf_det.`fmsf_date` AS emp_afmsf_det_fmsf_date
FROM
     `mas_rank` mas_rank LEFT JOIN `emp_afmsf_det` emp_afmsf_det ON mas_rank.`rank_id` = emp_afmsf_det.`rank_id`
     RIGHT OUTER JOIN `mas_unit` mas_unit ON emp_afmsf_det.`unit_id` = mas_unit.`unit_id`
 	RIGHT OUTER JOIN `mas_unit` mas_unit1 ON emp_afmsf_det.`posted_to` = mas_unit1.`unit_id`
WHERE
    emp_afmsf_det.`fmsf_date` between $P{fromDate} and $P{toDate} and
	emp_afmsf_det.`afmsf_type`=$P{afmsfType}]]></queryString>

