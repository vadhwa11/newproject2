	<queryString><![CDATA[select ifnull(d.REMD,0) + ifnull(a.ADM,0) - ifnull(c.DIS,0) as Total,ADM,b.Death,ifnull(c.DIS,0) - ifnull(b.Death,0) as DIS,d.REMD from
(SELECT count(*) as ADM FROM inpatient inp where inp.date_of_addmission = $P{toDate} and inp.ad_status !="C") a,
(SELECT count(*) as Death FROM discharge expiry_details where expiry_details.date_of_discharge  = $P{toDate}
and expiry_details.discharge_status_id = '3' and expiry_details.disposed_to_id  = '16' 
and expiry_details.disposal_id = '8') b,
(SELECT count(*) as DIS FROM discharge where discharge.date_of_discharge = $P{toDate}) c,
(SELECT count(*) as REMD FROM inpatient inp2 where inp2.date_of_addmission <= DATE_ADD($P{toDate},INTERVAL '-1' DAY)  and
 DATE_ADD($P{toDate},INTERVAL '0' DAY) <= ifNull(inp2.discharge_date,NOW()) and inp2.ad_status !="C" ) d]]></queryString>

