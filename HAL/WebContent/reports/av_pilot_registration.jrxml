	<queryString><![CDATA[select 
dt.service_no, dt.age, dt.full_name,U.unit_name, R.rank_name, H.hospital_name, c.Categories,dt.date_of_last_me

from av_pilot_registration_dt dt
left outer join av_pilot_registration_hd hd on dt.av_pilot_registration_hd_id=hd.id
left outer join mas_unit U on hd.unit_id=U.unit_id
