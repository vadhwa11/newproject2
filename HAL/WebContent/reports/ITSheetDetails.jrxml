	<queryString><![CDATA[select
emp_id,
t_year,
t_desc,

sum(month_jan) as jan_amount,
sum(month_feb) as feb_amount,
sum(month_mar) as mar_amount,
sum(month_apr) as apr_amount,
sum(month_may) as may_amount,
sum(month_jun) as jun_amount,
sum(month_jul) as jul_amount,
sum(month_aug) as aug_amount,
sum(month_sep) as sep_amount,
sum(month_oct) as oct_amount,
sum(month_nov) as nov_amount,
sum(month_dec) as dec_amount

from(

select
*,
(case when main.t_month = 1  then main.t_amount else 0 end) as month_jan,
(case when main.t_month = 2  then main.t_amount else 0 end) as month_feb,
(case when main.t_month = 3  then main.t_amount else 0 end) as month_mar,
(case when main.t_month = 4  then main.t_amount else 0 end) as month_apr,
(case when main.t_month = 5  then main.t_amount else 0 end) as month_may,
(case when main.t_month = 6  then main.t_amount else 0 end) as month_jun,
(case when main.t_month = 7  then main.t_amount else 0 end) as month_jul,
(case when main.t_month = 8  then main.t_amount else 0 end) as month_aug,
(case when main.t_month = 9  then main.t_amount else 0 end) as month_sep,
(case when main.t_month = 10 then main.t_amount else 0 end) as month_oct,
(case when main.t_month = 11 then main.t_amount else 0 end) as month_nov,
(case when main.t_month = 12 then main.t_amount else 0 end) as month_dec


 from

(

SELECT
     hr_payroll_process_header.employee_id AS emp_id,
     hr_payroll_process_header.year AS t_year,
     hr_payroll_process_header.month AS t_month,
     hr_payroll_process_header.basic AS t_amount,
     'Basic' AS t_desc
FROM
     hr_payroll_process_header hr_payroll_process_header LEFT OUTER JOIN hr_payroll_process_detail hr_payroll_process_detail ON hr_payroll_process_header.payroll_process_header_id = hr_payroll_process_detail.payroll_process_header_id
     LEFT OUTER JOIN hr_employee_pay_elements hr_employee_pay_elements ON hr_payroll_process_detail.emp_pay_element_id = hr_employee_pay_elements.id
     LEFT OUTER JOIN hr_mas_pay_element hr_mas_pay_element ON hr_employee_pay_elements.pay_element_id = hr_mas_pay_element.pay_element_id
     where hr_payroll_process_header.last_chg_date between $P{fDate} and $P{tDate}  
   and hr_payroll_process_header.employee_id = $P{empId} 

union

SELECT
     hr_payroll_process_header.employee_id AS emp_id,
     hr_payroll_process_header.year AS t_year,
     hr_payroll_process_header.month AS t_month,
     hr_payroll_process_detail.payelement_amount AS t_amount,
     hr_mas_pay_element.pay_element_desc AS t_desc

FROM
     hr_payroll_process_header hr_payroll_process_header LEFT OUTER JOIN hr_payroll_process_detail hr_payroll_process_detail ON hr_payroll_process_header.payroll_process_header_id = hr_payroll_process_detail.payroll_process_header_id
     LEFT OUTER JOIN hr_employee_pay_elements hr_employee_pay_elements ON hr_payroll_process_detail.emp_pay_element_id = hr_employee_pay_elements.id
     LEFT OUTER JOIN hr_mas_pay_element hr_mas_pay_element ON hr_employee_pay_elements.pay_element_id = hr_mas_pay_element.pay_element_id
   where hr_payroll_process_header.last_chg_date between $P{fDate} and $P{tDate}  
    and hr_payroll_process_header.employee_id = $P{empId} 

union all

select distinct * from
(SELECT
     hr_payroll_process_header.employee_id AS emp_id,
     hr_payroll_process_header.year AS t_year,
     hr_payroll_process_header.month AS t_month,
     hr_payroll_process_header.itax AS t_amount,
     'Income Tax' AS t_desc
FROM
     hr_payroll_process_header hr_payroll_process_header LEFT OUTER JOIN hr_payroll_process_detail hr_payroll_process_detail ON hr_payroll_process_header.payroll_process_header_id = hr_payroll_process_detail.payroll_process_header_id
     left OUTER JOIN hr_employee_pay_elements hr_employee_pay_elements ON hr_payroll_process_detail.emp_pay_element_id = hr_employee_pay_elements.id
     left OUTER JOIN hr_mas_pay_element hr_mas_pay_element ON hr_employee_pay_elements.pay_element_id = hr_mas_pay_element.pay_element_id
     where hr_payroll_process_header.last_chg_date between $P{fDate} and $P{tDate}
     and hr_payroll_process_header.employee_id = $P{empId}) as aa

 ) as main

 ) as main2
where  t_desc is not null
group by emp_id,t_year,t_desc
]]></queryString>

