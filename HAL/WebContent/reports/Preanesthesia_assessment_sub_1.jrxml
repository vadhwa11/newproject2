	<queryString><![CDATA[select 
pfh.PATIENT_PRESENT_COMPLAINT_NAME,
pmh.disease_status,
pmh.duration
FROM
patient_family_history pfh
left outer join patient_medical_History pmh on pfh.patient_family_history_id=pmh.patient_family_history_id
left outer join opd_surgery_header  osh on  pmh.hin_id=osh.hin_id
left outer  join opd_surgery_detail osd on osd.opd_surgery_id=osh.opd_surgery_id
left outer join ot_pre_anesthesia_detail opad on osd.id= opad.opd_surgery_detail
left outer join ot_pre_anesthesia_hd opah on opad.anesthesia_hd_id= opah.id

where opad.anesthesia_hd_id=$P{oshId}
and template_code='CH'
group by pfh.PATIENT_PRESENT_COMPLAINT_NAME,
pmh.disease_status,
pmh.duration]]></queryString>

