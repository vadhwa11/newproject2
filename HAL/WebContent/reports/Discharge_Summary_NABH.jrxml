	<queryString><![CDATA[select a.ad_no, a.hin_id, a.label,
case
  when a.label = 'Diagnosis'
     then (a.label||':'||a.item_reply )
       else
        (case when a.label='Advise on Discharge(will be printed on Discharge slip also)'
              then ('Advise on Discharge'||': '||a.item_reply)
              else(case when a.label != 'Brief Case Summary(For Printing on Discharge slip)'
                       then a.item_reply else ' ' end)
         end)
end item_reply, b.orderno, c.hin_no,hospital_name
from discharge_summary a left join mas_hospital d on a.hospital_id=d.hospital_id, discharge_items_category b, patient c
where a.item_code = b.id and a.item_reply!=' '
and a.hin_id = c.hin_id
and c.service_no = $P{serviceNo}
and a.ad_no=$P{adNo}
order by b.orderno]]></queryString>

