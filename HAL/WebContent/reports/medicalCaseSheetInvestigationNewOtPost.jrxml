	<queryString><![CDATA[SELECT
     
     mas_charge_code.charge_code_name AS investigation_name,
dg_orderdt.ot_remarks
FROM
ot_booking ot_booking
LEFT OUTER JOIN VISIT VISIT  ON ot_booking.visit_id=VISIT.VISIT_ID
LEFT OUTER JOIN dg_orderhd dg_orderhd ON VISIT.VISIT_ID = dg_orderhd.VISIT_ID
left OUTER JOIN dg_orderdt dg_orderdt ON dg_orderdt.orderhd_id = dg_orderhd.orderhd_id
LEFT OUTER JOIN  mas_charge_code  mas_charge_code ON dg_orderdt.charge_code_id  = mas_charge_code. charge_code_id 
where dg_orderdt.ot_stage ='POSTSURGERY' 
and   ot_booking.booking_id=$P{bookingId}
]]></queryString>

