	<queryString><![CDATA[select 
opd.INITIAL_DIAGNOSIS, opd.DISPOSAL,
(p.p_first_name ||' '||p.p_last_name) as patient_name,
r.rank_name, t.trade_name, p.age, p.service_no, u.unit_name, h.hospital_name

from opd_patient_details opd
left outer join visit v on opd.visit_id=v.visit_id
left outer join patient p on p.hin_id=v.hin_id
left outer join mas_rank r on p.rank_id=r.rank_id
left outer join mas_trade t on t.trade_id=p.trade_id
left outer join mas_unit u on p.unit_id=u.unit_id
left outer join mas_hospital h on h.hospital_id=p.hospital_id

where opd.visit_id=$P{vId}]]></queryString>

