	<queryString><![CDATA[select h.voucher_header_id, ma.acc_desc as main_acc,
fmsl.sub_led_desc, h.voucher_no, h.voucher_date, m.hospital_name,
m.address, d.narration as dn, isnull(d.dr_amount,0) as dr_amount,
isnull(d.cr_amount,0)as cr_amount,h.narration as hn,
h.cheque_no,h.issue_date,
(select 
 case
  when aa.cl_balance_cr!=0.00 then aa.cl_balance_cr else aa.cl_balance_dr 
 end 
 from fa_mas_account aa where aa.acc_id=$P{accId}) as balance,
(select aa.acc_desc from fa_mas_account aa where aa.acc_id=$P{accId}) as main
from (select fvd.* from fa_voucher_details fvd
where voucher_header_id in(
select voucher_header_id as a from fa_voucher_header where voucher_header_id in(
select voucher_header_id from fa_voucher_details where account_id=$P{accId} )) and account_id!=$P{accId}) d
left join fa_voucher_header h on d.voucher_header_id=h.voucher_header_id
left join fa_mas_account ma on d.account_id=ma.acc_id
left join mas_hospital m on h.hospital_id=m.hospital_id
left join fa_mas_sub_led fmsl on d.sub_led_id=fmsl.sub_led_id
where  h.voucher_header_id=$P{vhId} and m.hospital_id=$P{hospital_id}]]></queryString>

