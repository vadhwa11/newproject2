	<queryString><![CDATA[SELECT
     mas_charge_code.charge_code_name,
mas_ot.ot_name,mas_bed.bed_no,department_name, surgery_start_time,surgery_end_time,surgery_date
FROM
opd_surgery_header opd_surgery_header
left outer join  opd_surgery_detail  opd_surgery_detail on  opd_surgery_detail.opd_surgery_id=opd_surgery_header.opd_surgery_id
left outer join  mas_charge_code mas_charge_code on  mas_charge_code.charge_code_id=opd_surgery_detail.charge_code_id
left outer join  mas_sub_chargecode mas_sub_chargecode on  mas_sub_chargecode.sub_chargecode_id=mas_charge_code.sub_chargecode_id
left outer join visit visit on visit.visit_id=opd_surgery_header.visit_id
left outer join ot_booking ot_booking on ot_booking.visit_id=visit.visit_id
left outer join mas_ot mas_ot on mas_ot.ot_id=ot_booking.ot_id
left outer join mas_bed mas_bed on mas_bed.bed_id=ot_booking.bed_id
left outer join mas_department mas_department on mas_department.department_id=ot_booking.department_id
WHERE
    visit.visit_id=$P{visitId} and mas_sub_chargecode.sub_chargecode_code='MinorSur'

group by      mas_charge_code.charge_code_name,mas_ot.ot_name,mas_bed.bed_no,department_name, surgery_start_time,surgery_end_time,surgery_date]]></queryString>

