	<queryString><![CDATA[SELECT
     BIRTHDEATHREG."REGNO" AS BIRTHDEATHREG_REGNO,
     BIRTHDEATHREG."FNAME" AS BIRTHDEATHREG_FNAME,
     BIRTHDEATHREG."MNAME" AS BIRTHDEATHREG_MNAME,
     BIRTHDEATHREG."NAME" AS BIRTHDEATHREG_NAME,
     BIRTHDEATHREG."DOB" AS BIRTHDEATHREG_DOB,
     BIRTHDEATHREG."DOR" AS BIRTHDEATHREG_DOR,
     BIRTHDEATHREG."REMARKS" AS BIRTHDEATHREG_REMARKS,
     to_char(BIRTHDEATHREG."ADDRESS_PERMANENT") AS BIRTHDEATHREG_ADDRESS_PERMANEN,
     BIRTHDEATHREG."TIME" AS BIRTHDEATHREG_TIME,
     PATIENT."HIN_NO" AS PATIENT_HIN_NO,
     INPATIENT."AD_NO" AS INPATIENT_AD_NO,
     MAS_HOSPITAL."HOSPITAL_NAME" AS MAS_HOSPITAL_HOSPITAL_NAME,
     PATIENT."AGE" AS PATIENT_AGE,
     PATIENT."MOTHER_HIN_NO" AS PATIENT_MOTHER_HIN_NO
FROM
     "HMS"."PATIENT" PATIENT RIGHT OUTER JOIN "HMS"."BIRTHDEATHREG" BIRTHDEATHREG ON PATIENT."HIN_ID" = BIRTHDEATHREG."HIN_ID"
     LEFT OUTER JOIN "HMS"."INPATIENT" INPATIENT ON BIRTHDEATHREG."INPATIENT_ID" = INPATIENT."INPATIENT_ID"
     LEFT OUTER JOIN "HMS"."MAS_HOSPITAL" MAS_HOSPITAL ON BIRTHDEATHREG."HOSPITAL_ID" = MAS_HOSPITAL."HOSPITAL_ID"
where BIRTHDEATHREG."INPATIENT_ID" = $P{inpatientId} and BIRTHDEATHREG.bdtype='b']]></queryString>

