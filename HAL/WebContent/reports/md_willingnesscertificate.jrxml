	<queryString><![CDATA[SELECT
     patient.`service_no` AS patient_service_no,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_unit.`unit_address` AS mas_unit_unit_address,
     mas_reference.`reference_name` as reference_name ,
     CONCAT(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`," (",mas_relation.`relation_name`,") ")as patientName,
     CONCAT(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)as serviceName,
     mas_charge_code.`charge_code_name` AS md_cardic_claim_advance_treatment_detail,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
case 
when (md_cardic_claim_advance.`payable_to`) then (select mr.`reference_name`  from `md_cardic_claim_advance` mcca LEFT OUTER JOIN `mas_reference` mr ON mcca.`payable_to` = mr.`reference_id` where mcca.`id`=$P{id})
else md_cardic_claim_advance.`payable_to_name`
end   AS mas_hospital_A_hospital_name
FROM
     `patient` patient RIGHT OUTER JOIN `md_cardic_claim_advance` md_cardic_claim_advance ON patient.`hin_id` = md_cardic_claim_advance.`hin_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON md_cardic_claim_advance.`hospital_id` = mas_hospital.`hospital_id`
     left outer join `mas_reference` mas_reference on mas_reference.`reference_id`= md_cardic_claim_advance.`reffered_to`
     left outer join `mas_relation` mas_relation on mas_relation.`relation_id`=patient.`relation_id`
     left outer join `mas_charge_code` mas_charge_code on mas_charge_code.`charge_code_id`=md_cardic_claim_advance.`treatment_detail`
where 
md_cardic_claim_advance.`id`=$P{id}]]></queryString>

