	<parameter name="fromDate" isForPrompting="false" class="java.util.Date"/>
	<parameter name="toDate" isForPrompting="false" class="java.util.Date"/>
	<queryString><![CDATA[SELECT
     CONCAT(app_investigation_appointments.`from_time_slot`, app_investigation_appointments.`to_time_slot`) AS slot,
     app_investigation_appointments.`investigation_date` AS app_investigation_appointments_investigation_date,
     app_investigation_appointments.`service_no` AS app_investigation_appointments_service_no,
     app_investigation_appointments.`sex` AS app_investigation_appointments_sex,
     app_investigation_appointments.`age` AS app_investigation_appointments_age,
     app_investigation_appointments.`patient_name` AS app_investigation_appointments_patient_name,
     app_investigation_appointments.`service_person_name` AS app_investigation_appointments_service_person_name,
     app_investigation_appointments.`investigation_appointment_no` AS app_investigation_appointments_investigation_appointment_no,
     patient.`hin_no` AS patient_hin_no,
     patient.`patient_status` AS patient_type,
     app_equipment_master.`equipment_name` AS app_equipment_master_equipment_name,
     mas_department.`department_name` AS mas_department_department_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     mas_rank.`rank_name` AS rank_name
FROM
     `patient` patient RIGHT OUTER JOIN `app_investigation_appointments` app_investigation_appointments ON patient.`hin_id` = app_investigation_appointments.`hin_id`
     LEFT OUTER JOIN `app_equipment_master` app_equipment_master ON app_investigation_appointments.`equipment_id` = app_equipment_master.`equipment_id`
     LEFT OUTER JOIN `mas_department` mas_department ON app_investigation_appointments.`department_id` = mas_department.`department_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON app_investigation_appointments.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `inpatient` inpatient ON inpatient.`inpatient_id` = app_investigation_appointments.`inpatient_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
$P!{QUERY}]]></queryString>

