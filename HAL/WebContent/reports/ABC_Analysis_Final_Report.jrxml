select a.cost_price,d.pvms_no,d.nomenclature,$P{hospitalName},a.item_id,a.stock_id,a.closing_stock as stock_qty,a.closing_stock*a.cost_price as value,(a.closing_stock*a.cost_price)*100/$P{sumOfStock} as per,SUM((b.closing_stock*b.cost_price)*100/$P{sumOfStock}) as cum,'B Class Items >= ' || $P{costPriceForBClass} || '& < ' || $P{costPriceForAClass}  as class from store_item_batch_stock a,store_item_batch_stock b,mas_store_item d where a.item_id=d.item_id and a.cost_price >= b.cost_price and a.cost_price <$P{costPriceForAClass} and a.cost_price >=$P{costPriceForBClass} GROUP BY a.cost_price,d.pvms_no,d.nomenclature,a.item_id,a.stock_id,a.closing_stock union
select a.cost_price,d.pvms_no,d.nomenclature,$P{hospitalName},a.item_id,a.stock_id,a.closing_stock as stock_qty,a.closing_stock*a.cost_price as value,(a.closing_stock*a.cost_price)*100/$P{sumOfStock} as per,SUM((b.closing_stock*b.cost_price)*100/$P{sumOfStock}) as cum,concat('C Class Items < ',$P{costPriceForBClass}) as class from store_item_batch_stock a,store_item_batch_stock b,mas_store_item d where a.item_id=d.item_id and a.cost_price >= b.cost_price and a.cost_price <$P{costPriceForBClass} GROUP BY a.cost_price,d.pvms_no,d.nomenclature,a.item_id,a.stock_id,a.closing_stock order by cost_price]]></queryString>

