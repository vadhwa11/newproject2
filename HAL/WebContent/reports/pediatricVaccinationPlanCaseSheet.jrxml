	<queryString><![CDATA[SELECT
     opd_vaccin_mst.vaccin_name AS opd_vaccin_mst_vaccin_name,
     opd_vaccination_plan.vaccin_date AS vaccination_plan_vaccin_date,
     opd_vaccination_plan.vaccin_complete_date AS plan_vaccin_complete_date,
     opd_vaccination_plan.remarks AS opd_vaccination_plan_remarks,
     opd_vaccination_plan.dob AS opd_vaccination_plan_dob,
     mas_department.department_name AS mas_department_department_name,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     visit.visit_no AS visit_visit_no,
     visit.age AS visit_age,
     visit.visit_date AS visit_visit_date,
     patient.service_no AS patient_service_no,
     (patient.p_first_name||patient.p_middle_name||patient.p_last_name)AS patient_name,
     opd_vaccin_mst.vaccin_duration AS opd_vaccin_mst_vaccin_duration
FROM
     opd_vaccin_mst opd_vaccin_mst RIGHT OUTER JOIN opd_vaccination_plan opd_vaccination_plan ON opd_vaccin_mst.vaccin_id = opd_vaccination_plan.vaccin_id
     LEFT OUTER JOIN mas_department mas_department ON opd_vaccination_plan.department_id = mas_department.department_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON opd_vaccination_plan.hospital_id = mas_hospital.hospital_id
     LEFT OUTER JOIN visit visit ON opd_vaccination_plan.visit_id = visit.visit_id
     INNER JOIN patient patient ON opd_vaccination_plan.hin_id = patient.hin_id
     AND patient.hin_id = visit.hin_id
     AND mas_hospital.hospital_id = patient.hospital_id
     AND mas_department.department_id = visit.department_id
WHERE
     visit.visit_no = $P{visitNo}
     And patient.service_no = $P{serviceNo}
     And patient.hin_no = $P{hinNo}
]]></queryString>

