	<queryString><![CDATA[select dental_treatment_detail.teeth,dental_treatment_detail.dtc,
dental_treatment_detail.treatment,dental_treatment_detail.remark,
aneshthesia.anesthesia_name as aneshthesianame,
 opd_patient_details.anesthesia_remark as aneshthesiaremark,
 opd_patient_details.teeth_extracted as teethextracted,
     opd_patient_details.teeth_conserves_with_rt as conservesrt,
     opd_patient_details.teeth_conserves_without_rt as conserveswithoutrt

 from
 
 
  dental_treatment_detail dental_treatment_detail 
 left join dental_treatment_header dental_treatment_header
on dental_treatment_detail.dental_treatment_header_id = dental_treatment_header.dental_treatment_header_id
 left join patient patient on dental_treatment_header.hin_id = patient.hin_id
 left join visit visit on dental_treatment_header.visit_id = visit.visit_id
 left join opd_patient_details opd_patient_details on opd_patient_details.visit_id = visit.visit_id
  left  join mas_anesthesia  aneshthesia on opd_patient_details.anesthesia_id = aneshthesia.anesthesia_id
 
where
visit.visit_id=$P{visit_id}]]></queryString>

