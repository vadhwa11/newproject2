     `mas_personnel_details` mas_personnel_details LEFT OUTER JOIN `pension_form7_details` pension_form7_details ON mas_personnel_details.`personnel_id` = pension_form7_details.`personnel_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON mas_personnel_details.`hospital_id` = mas_hospital.`hospital_id`
