	<queryString><![CDATA[SELECT
     mas_personnel_details.`personnel_name` AS mas_personnel_details_personnel_name,
     mas_personnel_details.`pass_no` AS mas_personnel_details_pass_no,
     mas_personnel_details.`father_husband_name` AS mas_personnel_details_father_husband_name,
     mas_personnel_details.`religion_id` AS mas_personnel_details_religion_id,
