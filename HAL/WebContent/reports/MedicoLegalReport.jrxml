	<queryString><![CDATA[SELECT
     (patient.p_first_name|| ' ' ||patient.p_middle_name|| ' ' ||patient.p_last_name)AS patientName,
     (patient.s_first_name|| ' ' ||patient.s_middle_name|| ' ' ||patient.s_last_name)AS servicePersonName,
     mas_unit.unit_name AS mas_unit_unit_name,
     mas_rank.rank_name AS mas_rank_rank_name,
     mlc_case.incident_date AS mlc_case_incident_date,
     mlc_case.incident_time AS mlc_case_incident_time,
     mlc_case.type_and_no_of_vehicle AS type_and_no_of_vehicle,
     mlc_case.name_and_address_of_driver AS and_address_of_driver,
     mlc_case.patient_condition AS mlc_case_patient_condition,
     mlc_case.mlc_date AS mlc_case_mlc_date,
     mlc_case.mlc_time AS mlc_case_mlc_time,
     mlc_case.injury_details AS mlc_case_injury_details,
     (patient.s_first_name|| ' ' ||patient.s_last_name)AS servicePersonName,
     patient.service_no AS patient_service_no,
     mlc_case.ad_no AS mlc_case_ad_no,
     mlc_case.incident_place AS mlc_case_incident_place,
     mlc_case.brought_by AS mlc_case_brought_by,
     mas_record_office_address.address AS office_address_address,
     mlc_case.mlc_no AS mlc_case_mlc_no,
     mlc_case.brought_by_addr AS mlc_case_brought_by_addr,
     mlc_case.remarks AS mlc_case_remarks,
     mlc_case.patient_add AS mlc_case_patient_add,
     mlc_case.injury_type AS mlc_case_injury_type,
     inpatient_A.init_diagnosis AS inpatient_A_init_diagnosis,
     mas_hospital.hospital_name AS hospital_name
FROM
     patient patient LEFT OUTER JOIN mas_rank mas_rank ON patient.rank_id = mas_rank.rank_id
     LEFT OUTER JOIN mas_unit mas_unit ON patient.unit_id = mas_unit.unit_id
     LEFT OUTER JOIN mlc_case mlc_case ON patient.hin_id = mlc_case.hin_id
     LEFT OUTER JOIN mas_record_office_address mas_record_office_address ON patient.record_office_address_id = mas_record_office_address.record_office_address_id
     LEFT OUTER JOIN inpatient inpatient_A ON patient.hin_id = inpatient_A.hin_id AND inpatient_A.mlc = 'y'
     LEFT OUTER JOIN mas_hospital mas_hospital ON patient.hospital_id = mas_hospital.hospital_id
WHERE
     mlc_case.mlc_no = $P{mlcNo}
 And mas_hospital.hospital_id=$P{hospitalId}]]></queryString>

