	<queryString><![CDATA[select qm.question,qo.option_s_no,qo.qa_option_value_name,patient.age,r.relation_name,
(patient.p_first_name+ ' ' +patient.p_middle_name+ ' ' +patient.p_last_name)AS patient_p_first_name,
mas_question_heading.question_heading_name,mas_question_heading.question_heading_id

  from opd_qa_transation qt
   LEFT OUTER JOIN  opd_qa_master qm on qm.id=qt.question_id
   LEFT OUTER JOIN  mas_qa_option_value qo on qo.qa_option_value_id=qt.answer_option_id
   LEFT OUTER JOIN  mas_question_heading mas_question_heading on mas_question_heading.question_heading_id=qm.question_heading_id
   LEFT OUTER JOIN  visit v on v.visit_id=qt.visit_id
   LEFT OUTER JOIN  opd_patient_details opd on qt.opd_patient_details_id=opd.id
   LEFT OUTER JOIN  patient patient on patient.hin_id=v.hin_id
   LEFT OUTER JOIN  mas_relation r on patient.relation_id=r.relation_id

   where qt.visit_id=$P{visitId} and qt.opd_patient_details_id=$P{opdPatientDetailsId}]]></queryString>

