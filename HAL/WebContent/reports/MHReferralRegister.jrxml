	<queryString><![CDATA[select 
     mh_referral.mobile_no as mh_referral_mobile_no,
     mh_referral.referral_date as mh_referral_referral_date,
     mh_referral.service_no as mh_referral_service_no,
     mh_referral.age as mh_referral_age,  
     mh_referral.diagnosis as mh_referral_diagnosis,
     mh_referral.refer_to as mh_referral_refer_to,
     mh_referral.referred_for as mh_referral_referred_for,
     mas_rank.rank_name as mas_rank_rank_name,
     mas_relation.relation_name as mas_relation_relation_name,
     (emp_rank.rank_name||' '||mas_employee.first_name||' '||nvl(mas_employee.middle_name,'')||' '||nvl(mas_employee.last_name,''))as employee_name,
     mas_administrative_sex.administrative_sex_name as mas_administrative_sex_adminis,
     hospital.hospital_name as hospital_name,
     mh_referral.service_person_name as mh_referral_service_person_nam,
     mh_referral.patient_name as mh_referral_patient_name,
   mh_referral.run_date as run_date,
unit.unit_name as unit
from
     mas_rank mas_rank right outer join mh_referral mh_referral on mas_rank.rank_id = mh_referral.rank_id
     left outer join mas_relation mas_relation on mh_referral.relation_id = mas_relation.relation_id
     left outer join mas_employee mas_employee on mh_referral.referred_by_id = mas_employee.employee_id
left outer join mas_rank emp_rank on mas_employee.rank_id=emp_rank.rank_id
     left outer join mas_blood_group mas_blood_group on mh_referral.blood_group_id = mas_blood_group.blood_group_id
     left outer join mas_hospital hospital on mh_referral.hospital_id = hospital.hospital_id
     inner join patient patient on mh_referral.hin_id = patient.hin_id
 left outer join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
 left outer join mas_unit unit on patient.unit_id=unit.unit_id
 where run_date between $P{fromDate} and  $P{toDate}  and mh_referral.hospital_id=$P{hospitalId} $P!{qry}]]></queryString>

