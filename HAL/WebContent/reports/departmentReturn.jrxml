	<queryString><![CDATA[SELECT
     m.return_no AS store_grn_return_m_return_no,
     m.return_date AS store_grn_return_m_return_date,
d.department_name as d,
df.department_name as df,
h.hospital_name,
m.reason,
     t.expiry_date,
     t.rate,
     t.batch_no,
     t.item_amount,
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
t.return_qty,
item_unit_name
FROM
     store_internal_return_m m 
LEFT OUTER JOIN store_internal_return_t t ON m.id = t.return_main_id
     LEFT OUTER JOIN mas_store_item mas_store_item ON t.item_id = mas_store_item.item_id
     LEFT OUTER JOIN mas_department d ON m.to_department_id = d.department_id
     LEFT OUTER JOIN mas_department df ON m.from_department_id = df.department_id
     LEFT OUTER JOIN mas_hospital h ON m.hospital_id = h.hospital_id
     left JOIN mas_store_item_conversion mas_store_item_conversion ON mas_store_item.item_conversion_id = mas_store_item_conversion.item_conversion_id
WHERE
     m.id = $P{RETURN_ID}
 AND m.from_department_id = $P{DEPART}]]></queryString>

