	<queryString><![CDATA[SELECT count(*) as total,md.department_name,b.nv,c.ovo,d.c,e.w,f.v,diet_date,
 mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM diet_details dd1
right join mas_department md on dd1.department_id=md.department_id
left join
(SELECT count(*) as nv,dd2.department_id as d2
FROM diet_details dd2
left join mas_diet_type mdt on dd2.diet_type_id = mdt.diet_type_id
where diet_date=$P{DIETDATE} and diet_type_code='NV'
group by d2) b on md.department_id = b.d2
left join
(SELECT count(*) as ovo,dd3.department_id as d3
FROM diet_details dd3
left join mas_diet_type mdt on dd3.diet_type_id = mdt.diet_type_id
where diet_date=$P{DIETDATE} and diet_type_code='OVO'
group by d3) c on md.department_id = c.d3
left join
(SELECT count(*) as c,dd4.department_id as d4
FROM diet_details dd4
left join mas_diet di on dd4.diet_id = di.diet_id
where diet_date=$P{DIETDATE} and di.diet_code = 'C'
group by d4) d on md.department_id = d.d4
left join
(SELECT count(*) as w,dd5.department_id as d5
FROM diet_details dd5
where diet_date=$P{DIETDATE} and patient_condition='w'
group by d5) e on md.department_id = e.d5
left join
(SELECT count(*) as v,dd6.department_id as d6
FROM diet_details dd6
left join mas_diet_type mdt on dd6.diet_type_id = mdt.diet_type_id
where diet_date=$P{DIETDATE} and diet_type_code='V'
group by d6) f on md.department_id = f.d6

left JOIN `mas_hospital` mas_hospital ON dd1.`hospital_id` = mas_hospital.`hospital_id`

where diet_date=$P{DIETDATE}
group by md.department_id order by md.department_id]]></queryString>

