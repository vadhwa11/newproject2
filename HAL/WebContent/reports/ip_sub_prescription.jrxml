	<queryString><![CDATA[SELECT distinct
     patient_prescription_details.frequency_id AS frequency,
     patient_prescription_details.dosage AS dosage,
     patient_prescription_details.no_of_days AS days,
     patient_prescription_details.total AS total,
     patient_prescription_details.type AS type,
      patient_prescription_details.INSTRUCTION AS INSTRUCTION,
     mas_store_item.pvms_no AS pvms_no,
  mas_store_item_conversion.ITEM_UNIT_NAME as au,
     patient_prescription_details.remarks as remarks,
     patient_prescription_details.route as route,
     mas_store_item.nomenclature AS nomenclature,
     mas_store_item.common_name as common_name,
     mas_frequency.frequency_name as frequency_name ,
     patient_prescription_details.prescription_id AS prescription_id,
      ((CASE WHEN mas_store_item.nomenclature is null  THEN mas_store_item.common_name ELSE mas_store_item.nomenclature END))as nomenclature1
     
     
     
   

FROM
     mas_store_item mas_store_item RIGHT OUTER JOIN
 patient_prescription_details patient_prescription_details 
ON mas_store_item.item_id = patient_prescription_details.item_id
left join patient_prescription_header pph on patient_prescription_details.prescription_id=pph.prescription_id
left join inpatient ip on pph.inpatient_id=ip.inpatient_id
 left join MAS_STORE_ITEM_CONVERSION mas_store_item_conversion on
     mas_store_item.ITEM_CONVERSION_ID = mas_store_item_conversion.ITEM_CONVERSION_ID
      Left outer join mas_frequency mas_frequency on patient_prescription_details.frequency_id=mas_frequency.frequency_id
where  ip.ad_no= $P{adNo} and pph.hospital_id=$P{hospitalId}]]></queryString>

