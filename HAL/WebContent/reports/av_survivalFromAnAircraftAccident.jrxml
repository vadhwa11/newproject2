		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select 
surname, first_name, crew_duty, service_no, uninjured, Land_Injuried, Dead, 
Witnessed, Lost_Contact_Min, Condition_Seen_Last_Witness, 
Substance_Call, Notification_Call, No_Delay, After, Overdue_Action, Other_Notification, Shallow_Depth,

R.rank_name,

(case
when Land_Maintain_Cntct='y'  then'Yes'
end) as Land_Maintain_Cntct,

(case
when Water_Ditching='d' then 'Ditching'
when Water_Ditching='e' then 'Parachute Escape'
when Water_Ditching='u' then 'Rescue Services Used'
when Water_Ditching='n' then 'Rescue Services Not Used'
when Water_Ditching='r' then 'Casualty Not Recovered'
when Water_Ditching='c' then 'Casualty Recovered'
end) as Water_Ditching,

(case
when Escape_Ditch_Witness='a' then 'From Air'
when Escape_Ditch_Witness='g' then 'From Ground'
when Escape_Ditch_Witness='s' then 'From Shore'
when Escape_Ditch_Witness='w' then 'Not Witnessed'
when Escape_Ditch_Witness='k' then 'Not Known'
end) as Escape_Ditch_Witness,

(case
when Descent_Seat='w' then 'Witnessed'
when Descent_Seat='n' then 'Not Witnessed'
end) as Descent_Seat,

(case
when Time_Escape_Ditch='e' then 'Estimated'
when Time_Escape_Ditch='a' then 'Accurate'
end) as Time_Escape_Ditch,

(case
when Rt_Call_Inident='n' then 'Not Made'
when Rt_Call_Inident='m' then 'Made'
end) as Rt_Call_Inident,

(case
when Sea='f' then 'Fresh Water'
when Sea='l' then 'Location'
when Sea='o' then 'Open Sea'
end) as Sea

from av_accident A
left outer join mas_rank R on A.rank_id=R.rank_id
where A.id=$P{AV_ACCIDENT_ID}]]></queryString>

