     LEFT OUTER JOIN `patient` patient ON md_general_covering_dt.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON md_general_covering_hd.`hospital_id` = mas_hospital.`hospital_id`
$P!{QUERY}]]></queryString>

