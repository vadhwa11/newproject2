     "PATIENT_DETENTION_REGISTER" PATIENT_DETENTION INNER JOIN "DMA_REGISTER_HEADER" DMA_REGISTER_HEADER ON PATIENT_DETENTION."DMA_REGISTER_ID" = DMA_REGISTER_HEADER."DMA_REGISTER_HEADER_ID"
