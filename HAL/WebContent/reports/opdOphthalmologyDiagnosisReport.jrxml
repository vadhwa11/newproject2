	<queryString><![CDATA[SELECT
     visit_A.visit_no AS visit_A_visit_no,
     patient.service_no AS patient_service_no,
     (patient.p_first_name||patient.p_middle_name||patient.p_last_name)AS patient_name,
     patient.age AS patient_age,
     visit_A.visit_date AS visit_A_visit_date,
     patient.date_of_birth AS patient_date_of_birth,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     opd_oph_diagnosis_header.right_eye_surgery AS header_right_eye_surgery,
     opd_oph_diagnosis_header.left_eye_surgery AS header_left_eye_surgery,
     opd_oph_diagnosis_header.plan AS opd_oph_diagnosis_header_plan,
     opd_oph_diagnosis_header.systemic AS diagnosis_header_systemic,
     opd_oph_diagnosis_header.ocular AS diagnosis_header_ocular,
     opd_oph_diagnosis_header.next_review_date AS header_next_review_date,
     opd_oph_diagnosis_details.medicine AS diagnosis_details_medicine,
     opd_oph_diagnosis_details.drug_type AS diagnosis_details_drug_type,
     opd_oph_diagnosis_details.eye AS opd_oph_diagnosis_details_eye,
     mas_anesthesia.anesthesia_name AS mas_anesthesia_anesthesia_name,
     mas_frequency.frequency_name AS mas_frequency_frequency_name
FROM
     patient patient RIGHT OUTER JOIN opd_oph_diagnosis_header opd_oph_diagnosis_header ON patient.hin_id = opd_oph_diagnosis_header.hin_id
     LEFT OUTER JOIN visit visit_A ON opd_oph_diagnosis_header.visit_id = visit_A.visit_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON opd_oph_diagnosis_header.hospital_id = mas_hospital.hospital_id
     LEFT OUTER JOIN opd_oph_diagnosis_details opd_oph_diagnosis_details ON opd_oph_diagnosis_header.oph_diagnosis_header_id = opd_oph_diagnosis_details.oph_diagnosis_header_id
     LEFT OUTER JOIN mas_anesthesia mas_anesthesia ON opd_oph_diagnosis_header.anesthesia_id = mas_anesthesia.anesthesia_id
     LEFT OUTER JOIN mas_frequency mas_frequency ON opd_oph_diagnosis_details.frequency_id = mas_frequency.frequency_id
WHERE
     visit_A.visit_no = $P{visitNo}
     And patient.service_no = $P{serviceNo}
     And patient.hin_no = $P{hinNo}
]]></queryString>

