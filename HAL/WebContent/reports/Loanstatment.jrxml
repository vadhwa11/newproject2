	<queryString><![CDATA[SELECT
     hr_loan_header.monthly_install AS monthly_install,
     hr_mas_loan.interest_percent AS loan_interest,
     hr_loan_header.loan_p_amount AS loan_p_amount,
     hr_loan_header.loan_period AS loan_period,
     hr_loan_header.loan_date AS loan_date,
     hr_loan_detail.install_amount AS install_amount,
     mas_employee.employee_code AS employee_code,
     CONCAT(mas_employee.first_name,mas_employee.middle_name,mas_employee.last_name) as employee,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     hr_mas_loan.loan_description AS loan_description
FROM
     hr_loan_header hr_loan_header LEFT OUTER JOIN hr_loan_detail hr_loan_detail ON hr_loan_header.loan_header_id = hr_loan_detail.loan_header_id
     LEFT OUTER JOIN mas_employee mas_employee ON hr_loan_header.employee_id = mas_employee.employee_id
     LEFT OUTER JOIN hr_mas_loan hr_mas_loan ON hr_loan_header.loan_id = hr_mas_loan.loan_id
     LEFT OUTER JOIN mas_department mas_department ON mas_employee.department_id = mas_department.department_id 
     LEFT OUTER JOIN mas_hospital mas_hospital ON mas_employee.hospital_id = mas_hospital.hospital_id
	
 $P!{querySql}]]></queryString>

