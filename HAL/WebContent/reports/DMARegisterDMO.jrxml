	<queryString><![CDATA[SELECT
patient.service_no as service_no,
     DMA_REGISTER_HEADER."CALL_TIME" AS CALL_TIME,
     DMA_REGISTER_HEADER."TIME_ATTENDED" AS TIME_ATTEN,
     DMA_REGISTER_HEADER."DMO_TRANS_AMBU" AS DMO_TRANS_AMBU,
	 DMA_REGISTER_HEADER."REMARKS" AS remarks,
     (MAS_EMPLOYEE."FIRST_NAME" || ' ' || MAS_EMPLOYEE."MIDDLE_NAME" || ' ' ||  MAS_EMPLOYEE."LAST_NAME") AS EMPLOYEE_NAME
FROM
     "MAS_EMPLOYEE" MAS_EMPLOYEE right JOIN "DMA_REGISTER_HEADER" DMA_REGISTER_HEADER ON MAS_EMPLOYEE."EMPLOYEE_ID" = DMA_REGISTER_HEADER."DMO_ID"
left join "PATIENT" patient on DMA_REGISTER_HEADER.hin_id=patient.hin_id
where dmo_called='y' and dma_register_date between $P{fromDate} and $P{toDate} and DMA_REGISTER_HEADER.hospital_id=$P{hospitalId}
$P!{qry}]]></queryString>

