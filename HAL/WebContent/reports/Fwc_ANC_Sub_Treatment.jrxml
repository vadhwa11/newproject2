	<queryString><![CDATA[select
ppd.dosage, ppd.route, ppd.remarks, ppd.ct, ppd.no_of_days, fq.frequency_name, 
c.item_unit_name, msi.nomenclature
from opd_patient_details opd
left outer join visit v on opd.visit_id=v.visit_id
left outer join patient_prescription_header pph on pph.opd_patient_details_id=opd.id
left outer join patient_prescription_details ppd on pph.prescription_id=ppd.prescription_id 
left outer join mas_frequency fq on ppd.frequency_id= fq.frequency_id
left outer join mas_store_item msi on ppd.item_id=msi.item_id
left outer join mas_store_item_conversion c on msi.item_conversion_id=c.item_conversion_id

WHERE
   
   v.visit_id = $P{visitId}]]></queryString>

