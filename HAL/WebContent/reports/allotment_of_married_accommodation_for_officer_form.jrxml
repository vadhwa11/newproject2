	<parameter name="allotmentNo" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT (select mas_hospital.hospital_name from mas_hospital where hospital_id = accom_registration.hospital_id ) as hosptital_name,
     accom_allotment.`allotment_no` AS accom_allotment_allotment_no,
     accom_allotment.`allotment_date` AS accom_allotment_allotment_date,
     accom_allotment.`allotment_time` AS accom_allotment_allotment_time,
     accom_registration.`service_no` AS accom_registration_sevice_no,
     accom_registration.`department_id` AS accom_registration_department_id,
     accom_registration.`service_person_name` AS accom_registration_service_person_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_trade.`trade_name` AS mas_unit_trade_name,
     mas_rank.`rank_name` AS mas_unit_rank_name,
     mas_location.`location_name` As mass_location_location_name,
     mas_pool.`pool_name` AS mas_pool_pool_name,
     mas_smq.`smq_name` AS mas_smq_smq_name,
     accom_registration.`registration_date` AS accom_registration_registration_date,
     accom_allotment.`remarks` AS accom_allotment_remarks,
     mas_location.`location_name` AS mas_location_location_name
FROM
`accom_registration` accom_registration
RIGHT OUTER JOIN `accom_allotment` accom_allotment ON accom_registration.`accom_id` = accom_allotment.`accom_id`
LEFT OUTER JOIN `mas_unit` mas_unit ON accom_registration.`unit_id` = mas_unit.`unit_id`
 INNER JOIN `mas_pool` mas_pool ON accom_registration.`pool_id` = mas_pool.`pool_id`
left outer join mas_smq on mas_smq.smq_id = accom_allotment.smq_id
left outer join mas_location on mas_location.location_id = mas_smq.location_id
left outer join mas_trade on accom_registration.trade_id = mas_trade.trade_id
left outer join mas_rank on mas_rank.rank_id = accom_registration.rank_id

WHERE    `accom_registration`.reg_type = 'o' and `accom_registration`.reg_status = 'o'
and          accom_allotment.`allotment_no` = $P{allotmentNo}]]></queryString>

