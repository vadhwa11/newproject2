	<queryString><![CDATA[select main.*,mb.pages,
mb.book_name,mb.book_no,mb.author_name,mb.book_class_id,
(select class_code from mas_book_class m where m.book_class_id = mb.book_class_id) as class_code,
(select publisher_name from mas_publisher m where m.publisher_id = mb.publisher_id) as publisher_name,
mb.publisher_id,mb.year_publication
from (
select lh.crv_date, lh.crv_no,lh.bill_no,lh.bill_date,lh.source_of_receipt,
lh.accession_no,
ld.book_id,ld.price,
ld.volume from lib_crv_dt ld left outer join lib_crv_hd lh
on ld.crv_hd_id = lh.crv_hd_id
where lh.crv_date between $P{fromDate} and $P{toDate}) as main
left outer join
mas_book mb on
main.book_id = mb.book_id;]]></queryString>

