	<queryString><![CDATA[select  m1.issue_date as IDATE, 
m1.issue_no as CIVNo,
m1.issue_date,
s1.pvms_no as PVMSNo,
s1.nomenclature as Nomenclature,
s1.section_id as Section1,
b1.expiry_date,
mm1.manufacturer_name,
b1.manufacture_date,
sb1.brand_name,
s1.source_of_supply as Source1, u1.item_unit_name as AU, b1.batch_no as BatchNo,
t1.qty_request as RQTY,
t1.qty_issued as QTY,
b1.barcode_no,
d1.department_name as d,
td1.department_name as td,
e.demand_no,
(isnull(me.first_name,'')+' '+isnull(me.middle_name,'')+' '+isnull(me.last_name,'') ) as requestedBy,
(isnull(me1.first_name,'')+' '+isnull(me1.middle_name,'')+' '+isnull(me1.last_name,'') ) as approvedBy,
substring(ISSUE_NO,CHARINDEX('/',ISSUE_NO)+3,DATALENGTH(ISSUE_NO))  "period"
from store_issue_m  m1
left outer join mas_department  d1  on d1.department_id=m1.department_id 
left outer join mas_department  td1  on td1.department_id=m1.to_store
left outer join store_issue_t  t1  on t1.issue_m_id=m1.id 
left outer join mas_store_item  s1  on s1.item_id=t1.item_id 
left outer join store_item_batch_stock  b1  on b1.stock_id=t1.batch_stock_id
left outer join mas_store_item_conversion  u1  on   s1.item_conversion_id=u1.item_conversion_id
left outer join store_internal_indent_m e on e.id=m1.request_no
left outer join mas_employee me on me.employee_id=e.requested_by
left outer join mas_employee me1 on me1.employee_id=e.approved_by
left outer join mas_store_brand sb1  on sb1.brand_id=b1.brand_id
left outer join mas_manufacturer mm1  on mm1.manufacturer_id=b1.manufacturer_id
where  m1.id=$P{civNo}]]></queryString>

