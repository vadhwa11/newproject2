	<queryString><![CDATA[  SELECT
fa_mas_account.acc_code,
fa_mas_account.acc_desc,

CASE WHEN fa_mas_account.op_balance_cr is not null
  THEN fa_mas_account.op_balance_cr            
  ELSE fa_mas_account.op_balance_dr
       END AS op_bal, 
fa_mas_account.ytd_amount_cr,
fa_mas_account.ytd_amount_dr,
CASE WHEN fa_mas_account.cl_balance_cr is not null
 THEN fa_mas_account.cl_balance_cr    
 ELSE fa_mas_account.cl_balance_dr
       END AS cl_bal,  
fa_mas_account_sub_group.account_sub_group_name
FROM
    fa_mas_account_sub_group fa_mas_account_sub_group  INNER JOIN  fa_mas_account fa_mas_account  ON fa_mas_account_sub_group.account_sub_group_id = fa_mas_account.account_sub_group_id
ORDER BY
    fa_mas_account.acc_code]]></queryString>

