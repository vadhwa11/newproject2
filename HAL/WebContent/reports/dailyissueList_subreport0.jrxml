	<queryString><![CDATA[select sopit.item_id_issue,concat(sum(sopit.qty_issued),(concat('',sopim.prescription_no)))as qtyissueno 
from store_op_patient_issue_m sopim
left outer join store_op_patient_issue_t sopit on sopim.op_issue_id =sopit.op_issue_id

where   sopit.item_id_issue=$P{item_id} and sopim.issue_date 
between to_date($P{FROM_DATE} ,'dd/mm/yyyy') and to_date($P{TO_DATE} ,'dd/mm/yyyy') 
and sopim.hospital_id=$P{hospitalId}
group by sopit.item_id_issue,sopim.prescription_no
order by sopim.prescription_no]]></queryString>

