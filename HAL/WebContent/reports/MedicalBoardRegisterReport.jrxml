	<queryString><![CDATA[SELECT
     masmed.yearly_serial_no,
     mas_rank.rank_name,
     masmed.name_in_full,
     mas_trade.trade_name,
     mas_unit.unit_name,
     masmed.categorydate as categorydate,
     masmed.date_of_reporting date_of_reporting,
     masmed.medicalType as medicalType,
     masmed.date_of_completion as date_of_completion,
     NVL(mas_hospital.hospital_name,' ') AS mas_hospital_hospital_name,
     category.CATEGORIES
FROM  
     mas_medical_examination_report masmed
     left outer join mas_rank mas_rank on masmed.rank_id=mas_rank.rank_id
     left outer join mas_trade mas_trade on masmed.branch_id=mas_trade.trade_id
     left outer join mas_unit mas_unit on masmed.unit_id=mas_unit.unit_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON masmed.hospital_id = mas_hospital.hospital_id
     left outer join category on masmed.PastMedicalCategory=category.CATEGORYID
$P!{QUERY}]]></queryString>

