	<queryString><![CDATA[select item.pvms_no, mss.supplier_name, hd.rc_no, hd.rc_date,
 md.department_name,  item.nomenclature, dt.requested_qty, hd.from_date,dt.requested_qty,
dt.recom_qty,
dt.final_qty,
(isnull(e.first_name, '')+ ' ' +isnull(e.middle_name,'')+ ' ' +isnull(e.last_name,'')) as pharmacist,
hd.remarks,
dt.from_departments,
 hd.to_date from rc_details as dt
left Outer join rc_header as hd on dt.header_id = hd.id
left Outer join Mas_Department as md on md.department_id = hd.department_id
left outer join mas_store_item as item on dt.item_id = item.item_id
left outer join mas_store_supplier as mss on hd.supplier_id = mss.supplier_id
left Outer join users as u on u.user_id = hd.last_chg_by
left Outer join mas_employee as e on e.employee_id = u.employee_id
where hd.id = $P{rc_id}]]></queryString>

