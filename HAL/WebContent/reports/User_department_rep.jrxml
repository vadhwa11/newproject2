	<queryString><![CDATA[SELECT ud.department_id,ud.status,ud.user_id,md.department_name,us.user_name,us.login_name,
(select concat(hospital_name,' ',Address)  from mas_hospital where hospital_id =$P{hosptial_id})as hospital_name FROM user_department as ud
left join users as us
on ud.user_id = us.user_id
left outer join mas_department as md
on ud.department_id = md.department_id
where ud.department_id =   if($P{department_id}=0,ud.department_id,$P{department_id}) and 
us.status='y'
group by ud.department_id,md.department_name,ud.user_id,us.user_name, hospital_name
order by department_id,us.user_name;]]></queryString>

