	<queryString><![CDATA[SELECT
     mas_employee.`first_name` AS mas_employee_first_name,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     hr_specialist_master.`specialist_name` AS hr_specialist_master_specialist_name,
     mas_employee.`service_no` AS mas_employee_service_no,
     mas_employee.`join_date` AS mas_employee_join_date,
     mas_service_type.`service_type_name` AS mas_service_type_service_type_name,
     hr_class_master.`class_name` AS hr_class_master_class_name,
     mas_emp_category.`emp_category_name` AS mas_emp_category_emp_category_name,
     mas_division.`division_name` AS mas_division_division_name,
     mas_employee.`status` AS mas_employee_status,
     mas_department.`department_name` AS mas_department_department_name
FROM
     `mas_rank` mas_rank RIGHT OUTER JOIN `mas_employee` mas_employee ON mas_rank.`rank_id` = mas_employee.`rank_id`
     LEFT OUTER JOIN `hr_specialist_master` hr_specialist_master ON mas_employee.`speciality_id` = hr_specialist_master.`id`
     LEFT OUTER JOIN `mas_service_type` mas_service_type ON mas_employee.`service_type_id` = mas_service_type.`service_type_id`
     LEFT OUTER JOIN `mas_emp_category` mas_emp_category ON mas_employee.`emp_category_id` = mas_emp_category.`emp_category_id`
     LEFT OUTER JOIN `mas_division` mas_division ON mas_employee.`division_id` = mas_division.`division_id`
     LEFT OUTER JOIN `mas_department` mas_department ON mas_employee.`department_id` = mas_department.`department_id`
     LEFT OUTER JOIN `hr_class_master` hr_class_master ON hr_specialist_master.`id` = hr_class_master.`speciality_id`]]></queryString>

