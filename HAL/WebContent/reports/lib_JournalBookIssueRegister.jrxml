	<queryString><![CDATA[select * from

(select
     (select hospital_name from mas_hospital mh where mh.hospital_id = lh.hospital_id ) as hospitalname,
     lh.issue_date  AS issue_date,
     (select CONCAT( ifnull(me.first_name,''),' ',ifnull(me.middle_name,''),' ',ifnull(me.last_name,'') )as employeeName from mas_employee me where me.employee_id = lh.employee_id ) as employeeName,
     (select CONCAT( ifnull(mee.first_name,''),' ',ifnull(mee.middle_name,''),' ',ifnull(mee.last_name,'') ) as issuedbyname from mas_employee mee where mee.employee_id = lh.issued_by ) as issuedbyname,
     (select service_no from mas_employee me where me.employee_id = lh.employee_id ) as service_no,
     (select book_name from mas_book me where me.book_id = ld.book_id ) as mas_book_book_name,
     (select types from mas_book me where me.book_id = ld.book_id ) as types,
     ld.status,
     (select rank_name from mas_rank mk where mk.rank_id = (select distinct rank_id from mas_employee me where me.employee_id = lh.employee_id )) as mas_rank_rank_name,
     lh.hospital_id
     from
     lib_book_issue_header lh  LEFT OUTER JOIN lib_book_issue_detail ld  ON ld.issue_hd_id  = lh.id
   ) as main
$P!{QUERY}]]></queryString>

