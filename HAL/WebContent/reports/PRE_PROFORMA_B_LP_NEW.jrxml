	<queryString><![CDATA[select msi.BRANDED_GENERIC as cat , spd.DISP_TYPE, sum(spd.QUANTITY_ORDERED) ,sum(spd.AMOUNT) ,
spd.UNIT_RATE,spd.DISCOUNT_AMOUNT,sph.CATEGORY
,msi.NOMENCLATURE,msi.PVMS_NO 
,mss.SUPPLIER_NAME,
mm.MANUFACTURER_NAME as company,msi.ITEM_ID 
from  store_po_header sph
left outer join store_po_detail spd on sph.PO_ID = spd.PO_ID 
left outer join mas_store_item msi on  spd.ITEM_ID= msi.ITEM_ID
left outer join mas_store_supplier mss on sph.SUPPLIER_ID=mss.SUPPLIER_ID
left outer join mas_manufacturer mm on spd.MANUFACTURER_ID=mm.MANUFACTURER_ID
where   sph.PO_NUMBER =$P{poNo} and sph.HOSPITAL_ID=$P{hospitalId}
group by msi.BRANDED_GENERIC,spd.DISP_TYPE,
spd.UNIT_RATE,spd.DISCOUNT_AMOUNT,sph.CATEGORY
,msi.NOMENCLATURE,msi.PVMS_NO
,mss.SUPPLIER_NAME,
mm.MANUFACTURER_NAME ,msi.ITEM_ID]]></queryString>

