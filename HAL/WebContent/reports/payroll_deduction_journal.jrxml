	<queryString><![CDATA[SELECT ip.ad_no, (isnull(pa.p_first_name, '') + ' ' + isnull(pa.p_middle_name,'') + ' ' + isnull(pa.p_last_name, '')) PaName ,emp.service_no Empno, mr.relation_name,
emp.department_no,(isnull(emp.first_name,'') + ' ' + isnull(emp.middle_name,'') + ' ' + isnull(emp.last_name,'')) EmpName, md.division_name, md.diet_letter_authority,
DATEDIFF(day,ip.date_of_addmission,getDate()) as adminDays

FROM inpatient ip
INNER JOIN patient pa ON ip.hin_id=pa.hin_id
Left outer join mas_employee emp ON pa.employee_id=emp.employee_id
INNER JOIN mas_relation  mr ON pa.relation_id=mr.relation_id
INNER JOIN mas_division md ON emp.division_id=md.division_id
Where  ip.date_of_addmission  between $P{fromDate} and $P{toDate}
and md.division_id = $P{divisionId}]]></queryString>

