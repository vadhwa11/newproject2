	<parameter name="hinNo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     concat(mas_title.`title_name`,".",patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`)as patientName,
     concat(pulse," ",temperature," ",respiration)as ptr,
     concat(vom,"/",asp)as vomasp,
     inpatient.`age` AS inpatient_age,
     inpatient.`ad_no` AS inpatient_ad_no,
     inpatient.`date_of_addmission` AS inpatient_date_of_addmission,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_diagnosis_conclusion.`diagnosis_conclusion_name` AS mas_diagnosis_conclusion_diagnosis_conclusion_name,
     patient.`hin_no` AS patient_hin_no,
     patient.`service_no` AS patient_service_no,
     ipd_temperature.`time` AS ipd_temperature_time,
     ipd_temperature.`remarks` AS ipd_temperature_remarks,
     ipd_intake.`time` AS ipd_intake_time,
     ipd_intake.`intake` AS ipd_intake_intake,
     ipd_intake.`iv` AS ipd_intake_iv,
     ipd_intake.`oral` AS ipd_intake_oral,
     ipd_output.`time` AS ipd_output_time,
     ipd_output.`urine` AS ipd_output_urine,
     ipd_temperature.`bp` AS ipd_temperature_bp
FROM
     `patient` patient INNER JOIN `inpatient` inpatient ON patient.`hin_id` = inpatient.`hin_id`
     INNER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
     INNER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     INNER JOIN `mas_title` mas_title ON patient.`title_id` = mas_title.`title_id`
     INNER JOIN `ipd_intake_output_chart` ipd_intake_output_chart ON patient.`hin_id` = ipd_intake_output_chart.`hin_id`
     INNER JOIN `ipd_intake` ipd_intake ON ipd_intake_output_chart.`intakeoutput_id` = ipd_intake.`intakeoutput_id`
     INNER JOIN `ipd_output` ipd_output ON ipd_intake_output_chart.`intakeoutput_id` = ipd_output.`intakeoutput_id`
     INNER JOIN `ipd_temperature` ipd_temperature ON ipd_intake_output_chart.`intakeoutput_id` = ipd_temperature.`intakeoutput_id`
     INNER JOIN `mas_diagnosis_conclusion` mas_diagnosis_conclusion ON inpatient.`diagnosis_id` = mas_diagnosis_conclusion.`diagnosis_conclusion_id`
WHERE
     patient.`hin_no` = $P{hinNo}
     and inpatient.`ad_no` = $P{adNo}
     and ipd_intake_output_chart.date between $P{fromDate} and $P{toDate}]]></queryString>

