   concat(mas_employee.`first_name`,mas_employee.`middle_name`,mas_employee.`last_name`)AS employee_name,
     mas_department.`department_name` AS mas_department_department_name,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_employee.`service_no` AS mas_employee_service_no,
     mas_employee.`livingInOut` AS mas_employee_livingInOut,
     mas_employee.`living_in_date` AS mas_employee_living_in_date
FROM
     `mas_rank` mas_rank INNER JOIN `mas_employee` mas_employee ON mas_rank.`rank_id` = mas_employee.`rank_id`
     INNER JOIN `mas_department` mas_department ON mas_employee.`department_id` = mas_department.`department_id`
where 
mas_employee.`livingInOut` = '0' 
and mas_employee.`living_in_date` = $P{date}]]></queryString>

