	<queryString><![CDATA[SELECT
     store_balance_m.balance_no AS store_balance_m_balance_no,
     store_balance_m.balance_date AS store_balance_m_balance_date,
     store_balance_m.remarks AS store_balance_m_remarks,
  store_balance_m.last_chg_time AS time,
     store_balance_t.qty AS store_balance_t_qty,
     store_balance_t.unit_rate AS store_balance_t_unit_rate,
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
     store_balance_t.expiry_date AS store_balance_t_expiry_date,
     mas_store_item_conversion.item_unit_name AS mas_store_item_conversion_item_unit_name,
     mas_store_brand.brand_name AS mas_store_brand_brand_name,
     mas_manufacturer.manufacturer_name AS mas_manufacturer_manufacturer_name,
     store_balance_t.batch_no AS store_balance_t_batch_no,
     concat(mas_employee.first_name,' ',mas_employee.last_name)as Approved_By,
     mas_employee.first_name AS mas_employee_first_name,
     mas_employee.last_name AS mas_employee_last_name,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
mas_dep.department_name
FROM
     store_balance_t store_balance_t 
	 left outer JOIN store_balance_m store_balance_m ON store_balance_t.store_balance_m_id = store_balance_m.id
     left outer JOIN mas_store_item mas_store_item ON store_balance_t.item_id = mas_store_item.item_id
     left outer JOIN mas_store_item_conversion mas_store_item_conversion ON mas_store_item.item_conversion_id = mas_store_item_conversion.item_conversion_id
	 left outer join mas_store_brand mas_store_brand ON store_balance_t.brand_id = mas_store_brand.brand_id
     left outer join mas_manufacturer mas_manufacturer ON store_balance_t.manufacturer_id = mas_manufacturer.manufacturer_id
     left outer JOIN mas_employee mas_employee ON store_balance_m.approved_by = mas_employee.employee_id
     left outer join mas_hospital mas_hospital on store_balance_m.hospital_id = mas_hospital.hospital_id
     left outer join mas_department mas_dep on mas_dep.department_id = store_balance_m.department_id

WHERE   
  store_balance_m.department_id = $P{deptId} AND
(store_balance_m.balance_date between $P{fromDate} AND $P{toDate})
ORDER BY store_balance_m_balance_date]]></queryString>

