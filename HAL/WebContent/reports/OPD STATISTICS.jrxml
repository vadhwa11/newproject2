	<queryString><![CDATA[select distinct visit.visit_date as visit_date, patient.service_no AS patient_service_no,
(patient.p_first_name + ' ' + (patient.p_middle_name + '') + ' ' + COALESCE(patient.p_last_name,''))AS patientName,
 mas_relation.relation_name AS mas_relation_relation_name,
mas_rank.rank_name AS mas_rank_rank_name,
(patient.s_first_name + ' ' + COALESCE(patient.s_middle_name,'')  + ' ' + COALESCE(patient.s_last_name,''))AS servicePersonName,
 CASE WHEN charindex('NOT AVAILABLE IN ICD',icd.icd_name) = 0  
               THEN icd.icd_name 
               ELSE isnull(opd.initial_diagnosis,'')
       END as diagnosis,
(mas_employee.first_name+ ' ' + mas_employee.middle_name + ' ' + mas_employee.last_name)AS doctorName,
(case when (COALESCE(opd.days,0)!=0) then (opd.days + ' days') else ' '  end) as days,
hospital.hospital_name as hospital_name,visit.visit_id,
dep.department_name,
d.division_name
from
     patient patient right join visit visit on patient.hin_id = visit.hin_id
     left outer join mas_department mas_department on visit.department_id = mas_department.department_id
     left outer join mas_rank mas_rank on patient.rank_id = mas_rank.rank_id

     left outer join mas_relation mas_relation on patient.relation_id = mas_relation.relation_id
     left outer join mas_service_type mas_service_type on patient.service_type_id = mas_service_type.service_type_id
     left outer join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
     left outer join mas_service_status mas_service_status on patient.service_status_id = mas_service_status.service_status_id
left outer join mas_hospital hospital on visit.hospital_id=hospital.hospital_id
left outer join mas_employee mas_employee on visit.doctor_id=mas_employee.employee_id
left outer join mas_rank emp_mas_rank on mas_employee.rank_id = emp_mas_rank.rank_id
left outer join opd_patient_details opd on visit.visit_id=opd.visit_id
left outer join discharge_icd_code dic on visit.visit_id=dic.visit_id
left outer join mas_icd icd on dic.icd_id=icd.icd_id
left outer join mas_employee mas_emp on patient.employee_id=mas_emp.employee_id
left outer join mas_division d on d.division_id=mas_emp.division_id
left outer join mas_department dep on dep.department_id=visit.department_id

where visit.visit_date between $P{fromDate} and $P{toDate} and dic.visit_id is not null
and reporting_for in ('OPD','FollowUp') 
$P!{qry}
order by visit.visit_date asc]]></queryString>

