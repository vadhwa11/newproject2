	<queryString><![CDATA[SELECT
     i.`pvms_no`,
     i.`nomenclature`,
     mmf_in_qty as 'req MMF',
     t.`remarks`,
     m.`mmf_for_the_year`,
     m.`mmf_store_type`,
     concat('[',i.`pvms_no`,'] ',i.`nomenclature`)AS mas_store_item_nomenclature,
     curr_year_mmf as 'appr MMF',
     d.`department_name`,
     prev_year_mmf as previous,
     mas_store_item_conversion.`item_unit_name` AS mas_store_item_conversion_item_unit_name
FROM
     `store_mmf_department_m` m INNER JOIN `store_mmf_department_t` t ON m.`id` = t.`store_mmf_department_m_id`
     LEFT OUTER JOIN `mas_store_item` i ON t.`item_id` = i.`item_id`
     LEFT OUTER JOIN `mas_store_item_conversion` mas_store_item_conversion ON i.`item_conversion_id` = mas_store_item_conversion.`item_conversion_id`
     LEFT OUTER JOIN `mas_department` d ON m.`store_ward_dept` = d.`department_id`
WHERE
     mmf_for_the_year = $P{mmfForTheYear}
     and mmf_store_type = $P{storeType}
ORDER BY
    d.department_name ASC]]></queryString>

