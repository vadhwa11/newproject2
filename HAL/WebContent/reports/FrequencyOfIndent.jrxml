	<queryString><![CDATA[SELECT
     COUNT(store_internal_indent_m.`id`)AS no_of_indent,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     mas_department.`department_name` AS mas_department_department_name,
     mas_department_A.`department_name` AS mas_department_A_department_name
FROM
     `mas_department` mas_department RIGHT OUTER JOIN `store_internal_indent_m` store_internal_indent_m ON mas_department.`department_id` = store_internal_indent_m.`department_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON store_internal_indent_m.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `mas_department` mas_department_A ON store_internal_indent_m.`to_store` = mas_department_A.`department_id`
WHERE
     store_internal_indent_m.`to_store` = $P{DEPART}
AND store_internal_indent_m.`demand_date`BETWEEN$P{FROM_DATE} AND $P{TO_DATE}
AND store_internal_indent_m.`status` = 'o'
GROUP BY
     store_internal_indent_m.`department_id`]]></queryString>

