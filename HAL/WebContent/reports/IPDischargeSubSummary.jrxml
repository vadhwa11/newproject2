	<queryString><![CDATA[select max(c.service_type_name) service_type_name , count(c.service_type_id) no_of_patients,
(select count(p.service_type_id) as self
from inpatient ip,patient p where ip.discharge_date between $P{fromDate} and $P{toDate}
and ip.ad_status='D' and ip.hin_id = p.hin_id
and p.service_type_id = b.service_type_id and p.relation_id = '8') self,
(select count(p.service_type_id) as self
from inpatient ip,patient p where ip.discharge_date between $P{fromDate} and $P{toDate}
and ip.ad_status='D' and ip.hin_id = p.hin_id
and p.service_type_id = b.service_type_id and p.relation_id != '8') Family

From inpatient a, patient b,mas_service_type c
where a.hin_id = b.hin_id and b.service_type_id = c.service_type_id
and a.discharge_date between $P{fromDate} and $P{toDate}
and a.ad_status='D'  $P!{stringVariable2} group by b.service_type_id]]></queryString>

