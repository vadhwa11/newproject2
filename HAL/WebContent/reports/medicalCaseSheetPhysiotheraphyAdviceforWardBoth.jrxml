	<queryString><![CDATA[SELECT INPATIENT.AD_NO AS VISITNO,
     PATIENT.SERVICE_NO AS SERVICE_NO,
     PATIENT.HIN_NO AS HIN_NO,
     MAS_THERAPHY_TYPE.THERAPY_TYPE_NAME AS THERAPHYNAME,
     PHYSIO_REQUISITION_DETAIL.DURATION AS DURATION,
     PHYSIO_REQUISITION_DETAIL.NO_OF_DAYS AS NOOFDAYS,
     PHYSIO_REQUISITION_DETAIL.REMARK AS REMARKS,
     MAS_FREQUENCY.FREQUENCY_NAME AS FREQNAME
    
 FROM PHYSIO_REQUISITION_DETAIL PHYSIO_REQUISITION_DETAIL 
LEFT JOIN PHYSIO_REQUISITION_HEADER PHYSIO_REQUISITION_HEADER 
ON PHYSIO_REQUISITION_DETAIL.PHYSIO_REQUISITION_HEADER_ID = PHYSIO_REQUISITION_HEADER.PHYSIO_REQUISITION_ID
 LEFT JOIN PATIENT PATIENT ON PHYSIO_REQUISITION_HEADER.HIN_ID = PATIENT.HIN_ID
LEFT JOIN INPATIENT INPATIENT ON PHYSIO_REQUISITION_HEADER.INPATIENT_ID = INPATIENT.INPATIENT_ID
LEFT JOIN MAS_THERAPY_TYPE MAS_THERAPHY_TYPE ON PHYSIO_REQUISITION_DETAIL.THARAPHY_ID=MAS_THERAPHY_TYPE.THERAPY_TYPE_ID
LEFT JOIN MAS_FREQUENCY MAS_FREQUENCY ON PHYSIO_REQUISITION_DETAIL.FREQUENCY_ID = MAS_FREQUENCY.FREQUENCY_ID 
where
physio_requisition_header.inpatient_id=$P{inpatient_id} and physio_requisition_header.req_date=$P{caseSheetDate}
]]></queryString>

