	<queryString><![CDATA[SELECT
     mas_charge_code.`charge_code_code` AS mas_charge_code_charge_code_code,
     mas_charge_code.`charge_code_name` AS mas_charge_code_charge_code_name,
     mas_charge_code.`charge` AS mas_charge_code_charge,
     mas_charge_code.`status` AS mas_charge_code_status,
     mas_main_chargecode.`main_chargecode_name` AS mas_main_chargecode_main_chargecode_name,
     mas_sub_chargecode.`sub_chargecode_name` AS mas_sub_chargecode_sub_chargecode_name,
     mas_department.`department_name` AS mas_department_department_name,
     mas_sample.`sample_description` AS mas_sample_sample_description
FROM
     `mas_main_chargecode` mas_main_chargecode RIGHT OUTER JOIN `mas_charge_code` mas_charge_code ON mas_main_chargecode.`main_chargecode_id` = mas_charge_code.`main_chargecode_id`
     INNER JOIN `mas_sub_chargecode` mas_sub_chargecode ON mas_main_chargecode.`main_chargecode_id` = mas_sub_chargecode.`main_chargecode_id`
     AND mas_sub_chargecode.`sub_chargecode_id` = mas_charge_code.`sub_chargecode_id`
     LEFT OUTER JOIN `mas_department` mas_department ON mas_charge_code.`department_id` = mas_department.`department_id`
     LEFT OUTER JOIN `mas_sample` mas_sample ON mas_charge_code.`sample_id` = mas_sample.`sample_id`
ORDER BY
     mas_charge_code.`charge_code_name` ASC]]></queryString>

