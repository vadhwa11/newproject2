	<queryString><![CDATA[select serviceno,(mr.rank_name||' '||name_in_full) sname,pc.categories as present_cat,
spl_disability,dis_onset,c.categories as recommended_cat,last_review,course_of_illness,general_exam,
specilaistopinionremark,specilaisttreatmentadvice,
(emr.rank_name||' '||me.first_name||' '||me.middle_name||' '||me.last_name) as specialist_name,
mu.unit_name,hospital_name,SPECILAISTOPINIONDATE, 
mmer.diagnosis_1_clini, mmer.diagnosis_2_clini, mmer.onset_diag1, mmer.onset_diag2,
c.categories, mmer.last_board_date, mmer.next_board_date, mmer.presentcondition, mmer.medication,
mmer.cardiovascular_system, mmer.respiratory_system, mmer.central_nervous_system, mmer.gastro_intestinal_system,
mmer.temprature, mmer.pulse_rates, mmer.bp, mmer.rr_clinical, mmer.localexamination,
mmer.remarks_clinical, mmer.clinical_refer_mh, mmer.clinical_opd_dept

from mas_medical_examination_report mmer
left join patient p on mmer.hin_id=p.hin_id
left join category pc on p.category_id=pc.categoryid
left join mas_rank mr on mmer.rank_id=mr.rank_id
left join category c on mmer.medical_category_recommend = c.categoryid
left join users u on mmer.specialist_user_id=u.user_id
left join mas_employee me on u.employee_id=me.employee_id
left join mas_rank emr on me.rank_id=emr.rank_id
left join mas_unit mu on me.unit_id=mu.unit_id
left join mas_hospital mh on mmer.hospital_id=mh.hospital_id
where medical_examination_id=$P{MedExamId}]]></queryString>

