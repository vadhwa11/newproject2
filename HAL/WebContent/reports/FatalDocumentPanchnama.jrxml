	<queryString><![CDATA[SELECT
     inpatient.`ad_no` AS inpatient_ad_no,
     inpatient.`date_of_addmission` AS inpatient_date_of_addmission,
     patient.`service_no` AS patient_service_no,
     concat(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`)AS patientName,
     concat(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`)AS servicePersonName,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_relation.`relation_name` AS mas_relation_relation_name,
     inpatient.`age` AS inpatient_age,
     mas_administrative_sex.`administrative_sex_name` AS mas_administrative_sex_administrative_sex_name,
     expiry_details.`expiry_date` as expiryDate,
     mas_death_cause.`death_cause_name` as deathCause,
     `mis_fatal_tracking`.hod_perusal_rem AS HODRemarks,
     `mis_fatal_tracking`.comman_remarks_rem AS commanRemarks

  FROM
     `patient` patient LEFT JOIN `inpatient` inpatient ON patient.`hin_id` = inpatient.`hin_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`
     LEFT OUTER JOIN `expiry_details` expiry_details ON expiry_details.`inpatient_id` = inpatient.`inpatient_id`
     LEFT OUTER JOIN `mas_administrative_sex` mas_administrative_sex ON patient.`sex_id` = mas_administrative_sex.`administrative_sex_id`
     LEFT OUTER JOIN `mas_death_cause` mas_death_cause on mas_death_cause.`death_cause_id`=expiry_details.`d_death_cause_id`
     LEFT OUTER JOIN `mis_fatal_tracking` mis_fatal_tracking on inpatient.`inpatient_id`=mis_fatal_tracking.`inpatient_id`
 WHERE
     inpatient.`inpatient_id` = $P{inpatient_id}]]></queryString>

