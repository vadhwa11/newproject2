	<queryString><![CDATA[SELECT
    mas_employee.first_name as empName,
     hr_advance.advance_code AS hr_advance_advance_code,
     hr_advance.advance_date AS hr_advance_advance_date,
     hr_advance_detail.recovered_amount AS hr_advance_detail_recovered_amount,
     hr_advance.monthly_deduction AS hr_advance_monthly_deduction,
     hr_advance.advance_amount AS hr_advance_advance_amount,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,md.department_name
FROM
     hr_advance hr_advance 
     INNER JOIN hr_advance_detail hr_advance_detail ON hr_advance.advance_id = hr_advance_detail.advance_id
     INNER JOIN mas_employee mas_employee ON hr_advance.employee_id = mas_employee.employee_id
     INNER JOIN mas_hospital mas_hospital ON hr_advance.hospital_id = mas_hospital.hospital_id
     INNER JOIN mas_department md ON mas_employee.department_id = md.department_id     

$P!{query}

group by  hr_advance_detail.advance_id,mas_employee.first_name,hr_advance.advance_code, hr_advance.advance_date ,hr_advance_detail.recovered_amount,hr_advance.monthly_deduction,hr_advance.advance_amount,mas_hospital.hospital_name,md.department_name
order by hr_advance_detail.advance_id desc;]]></queryString>

