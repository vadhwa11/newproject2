	<queryString><![CDATA[SELECT DISTINCT
mas_store_item.nomenclature as nomenclature,
mas_store_brand.brand_name as brand_name,
patient_prescription_details.dosage as dosage,
 mas_frequency.frequency_name as frq,
patient_prescription_details.no_of_days as noOfDays,
patient_prescription_details.remarks as remarks,
patient_prescription_details.instruction As instruction,
patient_prescription_details.type As type,
patient_prescription_details.total As total,
concat(patient_prescription_details.dosage,concat( mas_frequency.frequency_name, patient_prescription_details.no_of_days))as Rx
    
FROM
     mas_store_item RIGHT OUTER JOIN patient_prescription_details ON mas_store_item.item_id = patient_prescription_details.item_id
     LEFT OUTER JOIN mas_frequency ON patient_prescription_details.frequency_id = mas_frequency.frequency_id
LEFT OUTER JOIN mas_store_brand ON patient_prescription_details.brand_id = mas_store_brand.brand_id
     LEFT OUTER JOIN patient_prescription_header ON patient_prescription_details.prescription_id = patient_prescription_header.prescription_id
     LEFT OUTER JOIN visit ON patient_prescription_header.visit_id = visit.visit_id
     LEFT OUTER JOIN patient ON patient_prescription_header.hin_id = patient.hin_id
     AND patient.hin_id = visit.hin_id
WHERE
     visit.visit_id = $P{visitNo}
     And patient.service_no = $P{serviceNo}
     And patient.hin_no = $P{hinNo}]]></queryString>

