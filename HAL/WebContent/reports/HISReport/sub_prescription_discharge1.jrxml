	<queryString><![CDATA[SELECT 
case when msi.common_name is null then msi.nomenclature else trim(concat(msi.common_name,concat(' ',msi.nomenclature))) end as nomenclature,  
  ppd.frequency_id AS frequency_id,
  ppd.dosage            AS dosage,
  ppd.no_of_days        AS no_of_days,
  ppd.total             AS total,
  ppd.type              AS details_type,
  msi.pvms_no                         AS pvms_no,
  ppd.remarks           AS remarks,
  msi.nomenclature                    AS mas_store_item_nomenclature,
  msi.common_name                     AS common_name,
  mf.frequency_name                   AS frequency_name ,
  ppd.prescription_id   AS prescription_id,
  CASE
    WHEN msi.high_risk_medicne = 'y'
    THEN concat(msi.common_name,concat(' ',concat(msi.nomenclature,concat(' ',msi.common_name))))
  END AS nomenclature1,
  CASE
    WHEN msi.high_risk_medicne = 'y'
    THEN concat(msi.common_name,concat(' ',concat(msi.nomenclature,concat(' ',msi.common_name))))
  END AS nomenclature2
FROM inpatient inp
LEFT JOIN patient_prescription_header pph
ON pph.inpatient_id=inp.inpatient_id
LEFT JOIN patient_prescription_details ppd
ON ppd.prescription_id=pph.prescription_id
LEFT JOIN mas_frequency mf
ON mf.frequency_id=ppd.frequency_id
LEFT JOIN mas_store_item msi
ON msi.item_id     =ppd.item_id 
where inp.inpatient_id= $P{prescriptionId} ]]></queryString>

