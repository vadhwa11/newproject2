	<queryString><![CDATA[SELECT
     discharge_summary.ad_no, discharge_summary.hin_id, discharge_summary.label, discharge_summary.item_reply, discharge_items_category.orderno, patient.hin_no,
     patient.service_no AS patient_service_no,
     concat(patient.s_first_name,concat(patient.s_middle_name,patient.s_last_name))AS s_name,
     concat(patient.p_first_name,concat(patient.p_middle_name,patient.p_last_name))AS patient_name,
     mas_relation.relation_name AS relation_name,
     mas_rank.rank_name AS RANK_NAME,
     inpatient.discharge_date AS dod,
     inpatient.Date_Of_Addmission as doa,
     inpatient.ad_no as adNo,
      inpatient.INPATIENT_ID as inpatient_id,
     patient.age as PATIENT_AGE,
mas_hospital.hospital_name,
mas_hospital.address
FROM  discharge_summary 
    left outer join patient on discharge_summary.hin_id=patient.hin_id
    left outer join  inpatient on inpatient.ad_no=discharge_summary.ad_no and inpatient.hospital_id = discharge_summary.hospital_id  
    left outer join discharge_items_category on discharge_items_category.id=discharge_summary.item_code
    left outer join  discharge_items on discharge_items.id=discharge_items_category.item_code
     LEFT OUTER JOIN mas_relation ON patient.relation_id = mas_relation.relation_id
     LEFT OUTER JOIN mas_rank ON patient.rank_id = mas_rank.rank_id
     LEFT OUTER JOIN mas_hospital ON discharge_summary.hospital_id = mas_hospital.hospital_id

     where discharge_summary.item_reply is not null and
     patient.service_no=$P{serviceNo} and
     discharge_summary.ad_no=$P{adNo}
     and discharge_summary.CATEGORY_NAME = $P{casetype}
     and discharge_items.item_code not in ('BWHT','ANTH','OFC','WGHT','HGHT')    
 order by discharge_summary.item_code desc]]></queryString>

