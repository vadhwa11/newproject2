	<queryString><![CDATA[SELECT Distinct
    concat(NVL(misc.icd_sub_category_name,''),concat('',NVL(concat(mas_icd.icd_name,mas_icd.icd_code),'')))AS mas_icd_icd_name
 FROM
    mas_icd RIGHT OUTER JOIN discharge_icd_code ON mas_icd.icd_id = discharge_icd_code.icd_id
  LEFT OUTER JOIN visit ON discharge_icd_code.visit_id = visit.visit_id
 LEFT OUTER JOIN patient ON discharge_icd_code.hin_id = patient.hin_id
 AND patient.hin_id = visit.hin_id
 LEFT OUTER JOIN opd_patient_details ON visit.visit_id = opd_patient_details.visit_id
 RIGHT OUTER JOIN mas_icd_sub_category misc ON mas_icd.icd_sub_category_id = misc.icd_sub_category_id
  WHERE visit.visit_id = $P{visitNo} And patient.service_no = $P{serviceNo} And patient.hin_no = $P{hinNo}]]></queryString>

