	<queryString><![CDATA[SELECT mas_hospital.hospital_name,mas_hospital.address,
  (SELECT DISTINCT (
    CASE
      WHEN opd_patient_history.present_complain=''
      THEN ''
      ELSE concat('COMPLAINTS :  ',concat(REPLACE(REPLACE(opd_patient_history.present_complain , '\n', ' '), '\r', ','),''))
    END)AS history_present_complain
  FROM visit
  LEFT OUTER JOIN patient
  ON visit.hin_id = patient.hin_id
  INNER JOIN opd_patient_details
  ON visit.visit_id = opd_patient_details.visit_id
  INNER JOIN opd_patient_history
  ON opd_patient_details.id = opd_patient_history.opd_patient_details
  WHERE visit.visit_id      = $P{visitNo}
  AND patient.service_no    = $P{serviceNo}
  AND patient.hin_no        = $P{hinNo}
  )AS present_complain,
  (SELECT DISTINCT (
    CASE
      WHEN opd_patient_history.present_illness=''
      THEN ''
      ELSE concat('H/O PRESENT ILLNESS :  ',concat(REPLACE(REPLACE(opd_patient_history.present_illness , '\n', ' '), '\r', ','),''))
    END)AS history_present_illness
  FROM visit
  LEFT OUTER JOIN patient
  ON visit.hin_id = patient.hin_id
  INNER JOIN opd_patient_details
  ON visit.visit_id = opd_patient_details.visit_id
  INNER JOIN opd_patient_history
  ON opd_patient_details.id = opd_patient_history.opd_patient_details
  WHERE visit.visit_id      = $P{visitNo}
  AND patient.service_no    = $P{serviceNo}
  AND patient.hin_no        = $P{hinNo}
  ) AS present_illness,
  (SELECT DISTINCT (
    CASE
      WHEN opd_patient_history.personal_past_history=''
      THEN ''
      ELSE concat('PAST HISTORY: ',concat(REPLACE(REPLACE(opd_patient_history.personal_past_history , '\n', ' '), '\r', ','),''))
    END)AS personal_past_history
  FROM visit
  LEFT OUTER JOIN patient
  ON visit.hin_id = patient.hin_id
  INNER JOIN opd_patient_details
  ON visit.visit_id = opd_patient_details.visit_id
  INNER JOIN opd_patient_history
  ON opd_patient_details.id = opd_patient_history.opd_patient_details
  WHERE visit.visit_id      = $P{visitNo}
  AND patient.service_no    = $P{serviceNo}
  AND patient.hin_no        = $P{hinNo}
  )AS personal_past_history,
  (SELECT DISTINCT (
    CASE
      WHEN opd_patient_history.family_present_history=''
      THEN ''
      ELSE concat('FAMILY HISTORY: ',concat(REPLACE(REPLACE(opd_patient_history.family_present_history, '\n', ' '), '\r', ','),''))
    END)AS family_present_history
  FROM visit
  LEFT OUTER JOIN patient
  ON visit.hin_id = patient.hin_id
  INNER JOIN opd_patient_details
  ON visit.visit_id = opd_patient_details.visit_id
  INNER JOIN opd_patient_history
  ON opd_patient_details.id = opd_patient_history.opd_patient_details
  WHERE visit.visit_id      = $P{visitNo}
  AND patient.service_no    = $P{serviceNo}
  AND patient.hin_no        = $P{hinNo}
  ) AS family_present_history,
  (SELECT DISTINCT (
    CASE
      WHEN opd_patient_history.personal_other_details =''
      THEN ''
      ELSE concat('OTHER : ',concat(REPLACE(REPLACE(opd_patient_history.personal_other_details, '\n', ' '), '\r', ','),''))
    END)AS patient_history_other
  FROM visit
  LEFT OUTER JOIN patient
  ON visit.hin_id = patient.hin_id
  INNER JOIN opd_patient_details
  ON visit.visit_id = opd_patient_details.visit_id
  INNER JOIN opd_patient_history
  ON opd_patient_details.id = opd_patient_history.opd_patient_details
  WHERE visit.visit_id      = $P{visitNo}
  AND patient.service_no    = $P{serviceNo}
  AND patient.hin_no        = $P{hinNo}
  ) AS OTHERS,
  (SELECT DISTINCT (
    CASE
      WHEN TO_CHAR(opd_patient_details.AFMS_desc)=''
      THEN ''
      ELSE concat('ON EXAMINATION :  ',concat(REPLACE(REPLACE(TO_CHAR(opd_patient_details.AFMS_desc) , '\n', ' '), '\r', ','),''))
    END)AS details_on_examination
  FROM visit visit
  LEFT OUTER JOIN patient patient
  ON visit.hin_id = patient.hin_id
  INNER JOIN opd_patient_details opd_patient_details
  ON visit.visit_id = opd_patient_details.visit_id
  INNER JOIN opd_patient_history
  ON opd_patient_details.id = opd_patient_history.opd_patient_details
  WHERE visit.visit_id      = $P{visitNo}
  AND patient.service_no    = $P{serviceNo}
  AND patient.hin_no        = $P{hinNo}
  )AS examination,
  (SELECT DISTINCT (
    CASE
      WHEN opd_patient_history.present_advice=''
      THEN ''
      ELSE concat('ADVICE :  ',concat(REPLACE(REPLACE(opd_patient_history.present_advice , '\n', ' '), '\r', ','),''))
    END)AS history_present_advice
  FROM visit
  LEFT OUTER JOIN patient
  ON visit.hin_id = patient.hin_id
  INNER JOIN opd_patient_details
  ON visit.visit_id = opd_patient_details.visit_id
  INNER JOIN opd_patient_history
  ON opd_patient_details.id = opd_patient_history.opd_patient_details
  WHERE visit.visit_id      = $P{visitNo}
  AND patient.service_no    = $P{serviceNo}
  AND patient.hin_no        = $P{hinNo}
  ) AS present_advice,
  (SELECT DISTINCT concat((
    CASE
      WHEN visit.ed_dispose=''
      THEN ''
      ELSE concat('DISPOSAL: ',visit.ed_dispose)
    END),concat(' X ',(
    CASE
      WHEN visit.ed_days=''
      THEN ''
      ELSE concat(visit.ed_days,' Days')
    END)))AS disposal
  FROM visit
  LEFT OUTER JOIN patient
  ON visit.hin_id = patient.hin_id
    --INNER JOIN opd_patient_details ON visit.visit_id = opd_patient_details.visit_id
  WHERE visit.visit_id   = $P{visitNo}
  AND patient.service_no = $P{serviceNo}
  AND patient.hin_no     = $P{hinNo}
  ) AS disposal,
  (
  CASE
    WHEN opd_patient_details.initial_diagnosis=''
    THEN ''
    ELSE concat('WORKING DIAGNOSIS : ',opd_patient_details.initial_diagnosis)
  END) AS prov_diag,
  concat((
  CASE
    WHEN opd_patient_details.height=''
    THEN ''
    ELSE concat('Ht:-',concat(opd_patient_details.height,concat('Cms','/')))
  END),(
  CASE
    WHEN opd_patient_details.weight=''
    THEN ''
    ELSE concat('Wt:-',concat(opd_patient_details.weight,'Kg'))
  END)) AS HtWt,
  concat((
  CASE
    WHEN opd_patient_details.temperature=''
    THEN ''
    ELSE concat('Temp :-',concat(opd_patient_details.temperature,' F   '))
  END),concat( (
  CASE
    WHEN opd_patient_details.pulse=''
    THEN ''
    ELSE concat('Pulse :-',concat(opd_patient_details.pulse,' /Min  '))
  END),concat( (
  CASE
    WHEN opd_patient_details.resp=''
    THEN ''
    ELSE concat('Resp :-',concat(opd_patient_details.resp,' /Min   '))
  END), (
  CASE
    WHEN opd_patient_details.bp=''
    THEN ''
    ELSE concat('BP :-',concat(opd_patient_details.bp,' mm of Hg'))
  END))))                                                                                                AS clinicalInfo,
  visit.visit_date                                                                                       AS visit_visit_date,
  visit.visit_no                                                                                         AS visit_visit_no,
  patient.service_no                                                                                     AS patient_service_no,
  patient.hin_no                                                                                         AS patient_hin_no,
  concat(patient.service_years,patient.total_service_period)                                             AS serviceYears,
  mas_icd.icd_name                                                                                       AS mas_icd_icd_name,
  concat(mas_department.department_name,concat('/',concat(visit.visit_no,concat('/',visit.visit_date)))) AS deptName,
  --(case when mas_department1.department_name='' then '' else concat('REFERED TO : ',concat(mas_department1.department_name,' for specialist opinion and further management please.'))end) AS department_name1,
  mas_hospital.hospital_name                                                                              AS mas_hospital_hospital_name,
  concat(patient.p_first_name,concat(' ',concat(patient.p_middle_name ,concat(' ',patient.p_last_name)))) AS patient_name,
  concat(patient.s_first_name,concat(' ',concat(patient.s_middle_name ,concat(' ',patient.s_last_name)))) AS s_name,
  mas_relation.relation_name                                                                              AS relation_relation_name,
  mas_rank.rank_name                                                                                      AS mas_rank_rank_name,
  mas_administrative_sex.administrative_sex_name                                                          AS sexName,
  mas_unit.unit_name                                                                                      AS mas_unit_unit_name,
  mas_service_type.service_type_name                                                                      AS service_type_name,
  mas_trade.trade_name                                                                                    AS mas_trade_trade_name,
  opd_patient_history.ip_op_pac_status                                                                    AS ip_op_pac_status,
  patient_prescription_header.prescription_id                                                             AS header_prescription_id,
  patient_investigation_header.investigation_header_id                                                    AS investigation_header_id,
  opd_patient_details.referred_dept                                                                       AS details_referred_dept,
  opd_patient_details.refered_doctars                                                                     AS details_refered_doctars,
  opd_patient_details.initial_diagnosis                                                                   AS details_initial_diagnosis,
  (
  CASE
    WHEN opd_patient_details.next_visit_date=NULL
    THEN ''
    ELSE concat('FOLLOW UP ON :',opd_patient_details.next_visit_date)
  END) AS details_next_visit_date,
  (
  CASE
    WHEN opd_patient_details.plan=''
    THEN ''
    ELSE concat('',opd_patient_details.plan)
  END) AS details_plan,
  (
  CASE
    WHEN opd_patient_details.outside_investigation=''
    THEN ''
    ELSE concat('OTHER/OUTSIDE INVESTIGATION :',concat(REPLACE(REPLACE(opd_patient_details.outside_investigation , '\n', ' '), '\r', ','),''))
  END)                                                                                                             AS history_outside_investigation,
  concat(mas_employee.first_name,concat(' ',concat(mas_employee.middle_name ,concat(' ',mas_employee.last_name)))) AS doc_name,
  rank.rank_name                                                                                                   AS doc_rank,
  (
  CASE
    WHEN $P{iKeyLogin}='yes'
    THEN
      (SELECT concat(concat(concat(concat(concat(concat(concat(concat(concat('** THIS DOCUMENT IS DIGITALLY SIGNED BY',' '), mr.rank_name),' '),me.first_name),' '),me.middle_name),' '),me.last_name),' **')
      FROM mas_rank mr
      LEFT JOIN mas_employee me
      ON mr.rank_id       =me.rank_id
      WHERE me.employee_id=$P{userId}
      )
    ELSE ' '
  END ) AS iKeyLogin,
             (Case 
     when (patient.date_of_birth is not null )
     then
     ( Case 
          when 
              (trunc(months_between(sysdate,patient.date_of_birth)/12)) is not null
              then
              (Case
                     when 
                      (trunc(months_between(sysdate,patient.date_of_birth)/12))!=0
                    then 
                      concat((trunc(months_between(sysdate,patient.date_of_birth)/12)), ' Year(s)')
                  else
                      (case
                           when 
                              (trunc(mod(months_between(sysdate,patient.date_of_birth),12)))!=0
                            then
                              concat(trunc(mod(months_between(sysdate,patient.date_of_birth),12)),' Month(s)')
                            else
                               
                            (case    
                            when (trunc(sysdate-add_months(patient.date_of_birth,trunc(months_between(sysdate,patient.date_of_birth)/12)*12+trunc(mod(months_between(sysdate,patient.date_of_birth),12)))))!=0
                            then
                               concat(trunc(sysdate-add_months(patient.date_of_birth,trunc(months_between(sysdate,patient.date_of_birth)/12)*12+trunc(mod(months_between(sysdate,patient.date_of_birth),12)))),' Day(s)')
                               else
                               '1 Day(s)'
              
             end) end)
            
    end) end )else
    patient.age
    end)                                                                                                                                       AS patient_age
  
  
  
FROM visit
LEFT OUTER JOIN patient
ON visit.hin_id=patient.hin_id
LEFT OUTER JOIN patient_prescription_header
ON patient_prescription_header.visit_id = visit.visit_id
AND patient_prescription_header.hin_id  = visit.hin_id
LEFT OUTER JOIN mas_department
ON visit.department_id = mas_department.department_id
LEFT OUTER JOIN mas_hospital
ON visit.hospital_id = mas_hospital.hospital_id
LEFT OUTER JOIN mas_relation
ON patient.relation_id = mas_relation.relation_id
LEFT OUTER JOIN mas_rank
ON patient.rank_id = mas_rank.rank_id
LEFT OUTER JOIN mas_administrative_sex
ON patient.sex_id = mas_administrative_sex.administrative_sex_id
LEFT OUTER JOIN mas_unit
ON patient.unit_id = mas_unit.unit_id
LEFT OUTER JOIN mas_service_type
ON patient.service_type_id = mas_service_type.service_type_id
LEFT OUTER JOIN mas_trade
ON patient.trade_id = mas_trade.trade_id
LEFT OUTER JOIN patient_investigation_header
ON patient.hin_id                         = patient_investigation_header.hin_id
AND patient_investigation_header.visit_id = visit.visit_id
INNER JOIN opd_patient_details
ON visit.visit_id = opd_patient_details.visit_id
  -- LEFT OUTER JOIN mas_employee ON opd_patient_details.refered_doctars = mas_employee.employee_id
  --LEFT OUTER JOIN mas_department mas_department1 ON opd_patient_details.referred_dept = mas_department1.department_id
INNER JOIN opd_patient_history
ON opd_patient_details.id = opd_patient_history.opd_patient_details
LEFT OUTER JOIN discharge_icd_code
ON discharge_icd_code.visit_id=opd_patient_details.visit_id
LEFT OUTER JOIN mas_icd
ON discharge_icd_code.icd_id = mas_icd.icd_id
LEFT OUTER JOIN mas_employee
ON mas_employee.employee_id = visit.doctor_id
LEFT OUTER JOIN mas_rank rank
ON rank.rank_id        = MAS_EMPLOYEE.rank_id
WHERE visit.visit_id   = $P{visitNo}
AND patient.service_no = $P{serviceNo}
AND patient.hin_no     = $P{hinNo}
AND ROWNUM             <2]]></queryString>

