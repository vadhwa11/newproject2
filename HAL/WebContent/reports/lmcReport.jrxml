	<queryString><![CDATA[SELECT distinct
     ame_lmc.`disability_name` AS ame_lmc_disability_name,
     ame_lmc.`duration` AS ame_lmc_duration,
     mas_medical_category.`medical_category_name` AS mas_medical_category_medical_category_name,
     ame_lmc.`perm_temp` AS ame_lmc_perm_temp,
     ame_lmc.`date_of_nrv` AS ame_lmc_date_of_nrv,
     ame_lmc.`emp_restriction` AS ame_lmc_emp_restriction,
     emp_afmsf_det.`service_no` AS emp_afmsf_det_service_no,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
    mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `ame_lmc_header` ame_lmc_header RIGHT OUTER JOIN `ame_lmc` ame_lmc ON ame_lmc_header.`id` = ame_lmc.`lmc_header_id`
     LEFT OUTER JOIN `mas_medical_category` mas_medical_category ON ame_lmc.`category` = mas_medical_category.`medical_category_id`
     LEFT OUTER JOIN `annual_medical_examination` annual_medical_examination ON ame_lmc_header.`ame_id` = annual_medical_examination.`ame_id`
     LEFT OUTER JOIN `emp_afmsf_det` emp_afmsf_det ON annual_medical_examination.`emp_afmsf_det_id` = emp_afmsf_det.`empafmsfdet_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON emp_afmsf_det.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON emp_afmsf_det.`unit_id` = mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON emp_afmsf_det.`hospital_id` = mas_hospital.`hospital_id`

$P!{QUERY}]]></queryString>

