	<queryString><![CDATA[SELECT
     inpatient.`mlc` AS inpatient_mlc,
     patient.`service_no` AS patient_service_no,
     concat(patient.`s_first_name`,' ',patient.`s_middle_name`,' ',patient.`s_last_name`,',',inpatient.`age`)as PNameAdd,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_relation.`relation_name` AS mas_relation_relation_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     concat(discharge.`time_of_discharge`,' ',discharge.`date_of_discharge`)as DTime,
     mas_icd.`icd_name` AS mas_icd_icd_name,
     sil_dil_status.`condition_status` AS sil_dil_status_condition_status
FROM
     `inpatient` inpatient INNER JOIN `discharge` discharge ON inpatient.`ad_no` = discharge.`ad_no`
     INNER JOIN `patient` patient ON inpatient.`hin_id` = patient.`hin_id`
     left outer JOIN `sil_dil_status` sil_dil_status ON inpatient.`inpatient_id` = sil_dil_status.`inpatient_id`
     left outer JOIN `mas_icd` mas_icd ON sil_dil_status.`icd_id` = mas_icd.`icd_id`
     INNER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`
     INNER JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`
     INNER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
WHERE
     discharge.date_of_discharge = $P{Date} and discharge.discharge_status_id = '3' 
     and discharge.disposed_to_id  = '16' and discharge.disposal_id = '8'
     and discharge.ward_id = $P{Dept_ID}]]></queryString>

