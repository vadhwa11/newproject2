	<queryString><![CDATA[select 
a.type_of_vehical, a.date_of_accident, a.cause_of_accident, a.other_cause_of_accident,
p.age,
u.unit_name,
h.hospital_name, a.death_case,
(select count(a.accident_id) from accidental_details)as count
from accidental_details a
left outer join patient p on a.driver_hin_id=p.hin_id
left outer join mas_unit u on p.unit_id =u.unit_id
left outer join mas_hospital h on a.hospital_id = h.hospital_id
where a.date_of_accident between $P{fromDate} and $P{toDate}
and h.hospital_id=$P{hospitalId} and h.command_id=$P{commandId}]]></queryString>

