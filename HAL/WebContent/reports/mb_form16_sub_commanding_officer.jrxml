	<queryString><![CDATA[select datetheindividual,
case when low_med_cat='y' then 'Yes' else 'No' end as lowmedcat,
case when duty_excused ='y' then 'Yes' else 'No' end as duty_excused,
nature_duties,
case when severe_excep_stress ='y' then 'Yes' else 'No' end as severe_excep_stress,
nvl(sincewhen,'') as sincewhen ,
nvl(since_on,'') as since_on,
case when family_living ='y' then 'Yes' else 'No' end as family_living,
sincewhen1,
case when since_in ='govt' then 'Govt. Accomodation' else 'Own Arrangement' end as since_in,
case when living_unit_lines ='y' then 'Yes' else 'No' end as living_unit_lines,
datesoflastleave,
case spent when 'town' then 'Town' when 'village' then 'Village' when 'state' then 'State' end as spent,
case when infection_disability='y' then 'Yes' else 'No' end as infection_disability,
case when  disability_other_case ='y' then 'Yes' else 'No' end as  disability_other_case ,
disease_surrounding_areas ,
case when  prevent_measure ='y' then 'Yes' else 'No' end as  prevent_measure,
case when  tranmitted_disease ='y' then 'Yes' else 'No' end as  tranmitted_disease,
tranmitted_date,place,hospital_std_center,
case when  followup_treatment  ='y' then 'Yes' else 'No' end as  followup_treatment ,
flc_date,responsiblity_factor,
nvl(comd_officer_rank,''),CommandingOfficer,
case when comd_officer_unit='null' then '' else nvl(comd_officer_unit,'')end as comd_officer_unit,
signed_by ,
(nvl(comd_officer_unit,'')||' '||nvl(signed_by,''))as main

from mas_medical_examination_report
where visit_id=$P{visitId} and medical_examination_id=$P{medExamId}]]></queryString>

