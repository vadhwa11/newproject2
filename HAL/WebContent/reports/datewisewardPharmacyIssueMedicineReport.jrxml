	<queryString><![CDATA[select sibs.batch_no,msi.nomenclature,msi.pvms_no,it.qty_issued,qty_request,
(me.FIRST_NAME + ' ' + ISNULL(me.MIDDLE_NAME,'') + ' ' + ISNULL(me.LAST_NAME,'')) as issued_by,
department_name
from store_issue_m im
inner join store_issue_t it on im.id = it.issue_m_id
left outer join store_item_batch_stock sibs on sibs.stock_id=it.batch_stock_id
left outer join mas_store_item msi on it.item_id=msi.item_id
left outer join mas_employee me on im.issued_by=me.employee_id
left outer join mas_department md on im.to_store=md.department_id
where $P!{query} im.issue_date between $P{fromDate} and $P{toDate}
order by im.issue_date 
]]></queryString>

