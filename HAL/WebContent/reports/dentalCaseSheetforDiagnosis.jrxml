		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
     
 opd_patient_details.initial_diagnosis as workingdiagnosis ,
sdiagnosis.system_diagnosis_name as sdiag,
visit.visit_id as visit_id
 
from   opd_patient_details opd_patient_details 
 left outer join visit visit on opd_patient_details.visit_id = visit.visit_id
 left outer join patient patient on visit.hin_id=patient.hin_id 
     left outer join mas_hospital mas_hospital on patient.hospital_id = mas_hospital.hospital_id
     left outer join mas_medical_examination_report mas_medical_examination_report on visit.visit_id = mas_medical_examination_report.visit_id
     left outer join category category on mas_medical_examination_report.pastmedicalcategory = category.categoryid
     and category.categoryid =mas_medical_examination_report.presentmedicalhistory
           left  join mas_anesthesia  aneshthesia on opd_patient_details.anesthesia_id = aneshthesia.anesthesia_id
 left  join mas_system_diagnosis  sdiagnosis on opd_patient_details.system_diagnosis_id = sdiagnosis.system_diagnosis_id
left join opd_patient_history opd_patient_history on visit.visit_id = opd_patient_history.visit_inpatient_id
 
WHERE
     visit.visit_id=$P{visit_id}]]></queryString>

