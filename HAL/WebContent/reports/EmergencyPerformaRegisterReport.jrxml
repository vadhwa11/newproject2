	<queryString><![CDATA[select
     (mas_rank.rank_name ||' ' || mas_employee.first_name ||' '|| mas_employee.last_name ||' '|| mas_employee.middle_name )as employee_name,
     emergency_performa.call_rcd_date as call_rcd_date,
     emergency_performa.call_rcd_time as call_rcd_time,
     emergency_performa.source_from as source_from,
     emergency_performa.remarks as remarks,
     emergency_performa.reported_by as reported_by,
     mas_hospital.hospital_name as hospital_name,
     emergency_performa.location as location_name,
     emergency_performa.attended_time as attended_time,
     emergency_performa.emergency_type as emergency_type, 
emergency_performa.casualties as casualties,
emergency_performa.action_taken as action_taken     

from
    emergency_performa emergency_performa 
     left outer join mas_hospital mas_hospital on emergency_performa.hospital_id = mas_hospital.hospital_id
     left outer join mas_employee mas_employee on emergency_performa.medical_officer_id = mas_employee.employee_id
left join mas_rank mas_rank on mas_employee.rank_id = mas_rank.rank_id
 
where
trunc(emergency_performa.call_rcd_date) between $P{fromDate} and $P{toDate}
and emergency_performa.hospital_id=$P{hospitalId} $P!{qry}]]></queryString>

