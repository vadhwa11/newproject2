	<queryString><![CDATA[select ledger.ledger_date,
nvl2(ledger.vendor_id,(case when (ledger.unit_id is not null) then (ledger.particulars||' [From: '||mas_store_air_force_depot.air_force_depot_name||']') else ledger.particulars end),(ledger.particulars||' [From: '||mas_store_supplier.supplier_name||']')) as particulars,
ledger.qty, ledger.closing_stock,
mas_store_item.nomenclature,mas_store_item.pvms_no,
mas_store_unit.unit_name, mas_department.department_name, mas_store_supplier.supplier_name,
(select closing_stock from ledger where item_id =  $P{itemId} and dept_id = $P{deptId} and hospital_id = $P{hospitalId} and ledger_date = (select max(ledger_date)from ledger where item_id =  $P{itemId} and dept_id = $P{deptId}) and hospital_id = $P{hospitalId}  and rownum<=1) as final_stock
,hospital_name
from ledger ledger
left outer join mas_store_item mas_store_item on mas_store_item.item_id = ledger.item_id
left outer join mas_store_item_conversion mas_store_item_conversion on mas_store_item_conversion.item_conversion_id = mas_store_item.item_conversion_id
left outer join mas_store_unit mas_store_unit on mas_store_item_conversion.purchase_unit_id  = mas_store_unit.unit_id
left outer join mas_department mas_department on mas_department.department_id  = ledger.dept_id
left outer join mas_store_supplier mas_store_supplier on mas_store_supplier.supplier_id = ledger.vendor_id
left outer join mas_store_air_force_depot mas_store_air_force_depot on mas_store_air_force_depot.air_force_depot_id = ledger.unit_id
left outer join mas_hospital hospital on ledger.hospital_id=hospital.hospital_id
where ledger.item_id =  $P{itemId} and ledger.dept_id = $P{deptId}  and ledger.hospital_id = $P{hospitalId} 
order by ledger.ledger_date]]></queryString>

