	<queryString><![CDATA[select 

     PATIENT.AGE AS PATIENT_AGE,
    INPATIENT.AD_NO as ADNO,
     MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_NAME AS ADMINISTRATIVE_SEX_NAME,
     MAS_RELATION.RELATION_NAME AS A_RELATION_NAME,
     (PATIENT.P_FIRST_NAME + ' ' + ISNULL(PATIENT.P_MIDDLE_NAME,'') + ' ' + ISNULL(PATIENT.P_LAST_NAME,'')) AS Patient_NAME,
     PATIENT.SERVICE_NO AS PATIENT_SERVICE_NO,
     (doctor_inpatient.First_NAME + ' ' + ISNULL(doctor_inpatient.MIDDLE_NAME,'') + ' ' + ISNULL(doctor_inpatient.LAST_NAME,'')) AS admitted_by,
     (doctor.First_NAME + ' ' + ISNULL(doctor.MIDDLE_NAME,'') + ' ' + ISNULL(doctor.LAST_NAME,'')) AS doctor_name,
	osh.requisition_date,surgery_date,
dep.department_name as emp_dept

FROM


ot_post_anaesthesia_procedure  opre 
left outer join ot_pre_anaesthesia_proc_notes_main op on opre.ot_booking_id=op.booking_id
left outer join ot_booking ot_booking on ot_booking.booking_id=op.booking_id
left outer join ot_booking_dt ot_booking_dt on ot_booking_dt.ot_booking_hd=ot_booking.booking_id

left outer join ot_pre_anesthesia_detail ot_pre_anesthesia_detail on ot_pre_anesthesia_detail.id=ot_booking_dt.ot_pre_anesthesia_detail
left outer join ot_pre_anesthesia_hd opah on ot_pre_anesthesia_detail.anesthesia_hd_id= opah.id
left outer join opd_surgery_detail osd    on  ot_pre_anesthesia_detail.opd_surgery_detail =osd.id
left outer join opd_surgery_header  osh on   osd.opd_surgery_id=osh.opd_surgery_id


left outer JOIN INPATIENT INPATIENT ON ot_booking.inpatient_id = inpatient.inpatient_id
left outer JOIN patient patient ON ot_booking.hin_id = patient.hin_id
LEFT OUTER JOIN MAS_RELATION MAS_RELATION ON patient.RELATION_ID = MAS_RELATION.RELATION_ID
LEFT outer JOIN MAS_ADMINISTRATIVE_SEX MAS_ADMINISTRATIVE_SEX  ON MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_ID = PATIENT.SEX_ID
left outer join mas_employee_department ed on ed.employee_id=patient.employee_id
left outer join mas_employee emp_surgery on emp_surgery.employee_id= osh.employee_id
left outer join mas_employee doctor_inpatient on doctor_inpatient.employee_id= inpatient.doctor_id
left outer JOIN visit visit ON visit.visit_id = inpatient.visit_id
left outer join mas_employee doctor on doctor.employee_id= visit.doctor_id
left outer join mas_department dep on ed.department_id=dep.department_id

where opre.ot_post_anaesthesia_procedure_id=$P{mainId}]]></queryString>

