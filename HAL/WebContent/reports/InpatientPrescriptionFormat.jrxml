	</parameter>
	<parameter name="SUBREPORT_DIR" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["C:\\Documents and Settings\\User1\\Desktop\\opdreports\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
       (patient.p_first_name|| ' ' ||patient.p_middle_name|| ' ' ||patient.p_last_name)AS patient_p_first_name,
     ip.ad_no AS ad_no,
    ip.inpatient_id as inpatientId,
     patient.service_no AS patient_service_no,
(patient.s_first_name|| ' ' ||patient.s_middle_name|| ' ' ||patient.s_last_name)AS s_name,
     patient.age AS patient_age,
     mas_department.department_name AS mas_department_department_name,
     mas_department.department_code AS dept_code,
     mas_rank.rank_name AS rank_name,
     mas_relation.relation_name AS relation_name,
     mas_hospital.hospital_name AS hospital_name,
( mas_rank.rank_name|| ' ' ||mas_employee.first_name|| ' ' ||mas_employee.middle_name|| ' ' ||mas_employee.last_name)AS doctor,
     mas_unit.unit_name as unit,
    mas_trade.trade_name as trade,    
     patient_prescription_header.prescription_id AS prescriptionId,
     ip.date_of_addmission AS admission_date,
     ip.time_of_addmission AS admission_time,
     mas_icd.icd_name AS mas_icd_icd_name,administrative_sex_name,c.categories
FROM
     patient patient LEFT OUTER JOIN inpatient ip ON patient.hin_id = ip.hin_id
     LEFT OUTER JOIN mas_department mas_department ON ip.department_id = mas_department.department_id
     RIGHT OUTER JOIN patient_prescription_header patient_prescription_header ON ip.inpatient_id = patient_prescription_header.inpatient_id
     AND patient_prescription_header.hin_id = ip.hin_id
     LEFT OUTER JOIN mas_employee mas_employee ON ip.doctor_id = mas_employee.employee_id
     LEFT OUTER JOIN discharge_icd_code discharge_icd_code ON ip.inpatient_id = discharge_icd_code.inpatient_id
     AND patient.hin_id = discharge_icd_code.hin_id
     LEFT OUTER JOIN mas_icd mas_icd ON discharge_icd_code.icd_id = mas_icd.icd_id
     LEFT OUTER JOIN mas_rank mas_rank ON patient.rank_id = mas_rank.rank_id
     LEFT OUTER JOIN mas_relation mas_relation ON patient.relation_id = mas_relation.relation_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON patient.hospital_id = mas_hospital.hospital_id   
     left outer join mas_unit mas_unit on patient.unit_id = mas_unit.unit_id
     left outer join mas_trade mas_trade on patient.trade_id = mas_trade.trade_id
   left outer join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
left outer join category c on patient.category_id=c.categoryid
WHERE
     ip.ad_no = $P{adNo}
      And mas_hospital.hospital_id=$P{hospitalId}]]></queryString>

