	<queryString><![CDATA[select  sibs.item_id,mas_store_item.nomenclature,mic.item_category_name,
mas_store_item.pvms_no,sibs.batch_no,
sibs.expiry_date,
(sum(nvl(sibs.opening_balance_qty,0)) 
+ sum(nvl(sibs.received_qty,0)) 
- sum(nvl(sibs.issue_qty,0))
) as Stock_qty,
sibs.cost_price,sibs.mrp
from mas_store_item mas_store_item
left outer join store_item_batch_stock  sibs on mas_store_item.item_id=sibs.item_id
left outer join mas_item_category  mic on mas_store_item.item_category_id=mic.item_category_id
where sibs.department_id = 24
and  mas_store_item.Status='y'
and mas_store_item.item_id =$P{itemId}
group by  sibs.item_id,mas_store_item.nomenclature,mic.item_category_name,
mas_store_item.pvms_no,sibs.batch_no,
sibs.expiry_date,sibs.cost_price,sibs.mrp
Order By mas_store_item.pvms_no,mas_store_item.nomenclature
]]></queryString>

