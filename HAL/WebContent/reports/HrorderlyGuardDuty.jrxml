	<queryString><![CDATA[SELECT
     hrorderly_guard_duty_entry.`duty_date` AS duty_date,
     concat( ifnull(mas_employee.`first_name`,''),' ', ifnull(mas_employee.`middle_name`,''),' ', ifnull(mas_employee.`last_name`,'')) as first_name ,
     mas_employee.`service_no` AS service_no,
     mas_rank.`rank_name` AS rank_name,
     concat(hr_duty_time_master.`duty_from_time`," - ",hr_duty_time_master.`duty_to_time`) AS duty_time
FROM
     `hrorderly_guard_duty_entry` hrorderly_guard_duty_entry
     left JOIN mas_employee `mas_employee` ON hrorderly_guard_duty_entry.`emp_id`=mas_employee.`employee_id`
    LEFT JOIN `mas_rank` mas_rank ON mas_employee.`rank_id` = mas_rank.`rank_id`
    LEFT JOIN `hr_duty_time_master` hr_duty_time_master ON hrorderly_guard_duty_entry.`duty_time_id` = hr_duty_time_master.`id`
where hrorderly_guard_duty_entry.duty_date between $P{fromDate} and $P{toDate}]]></queryString>

