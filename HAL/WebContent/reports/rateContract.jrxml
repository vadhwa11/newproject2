	<queryString><![CDATA[SELECT
    
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
t.rc_qty as s_rc_qty,
d.department_id,
d.department_name
FROM
     patient_prescription_header m 
LEFT OUTER JOIN patient_prescription_details t ON m.prescription_id = t.prescription_id
     LEFT OUTER JOIN mas_store_item mas_store_item ON t.item_id = mas_store_item.item_id
     LEFT OUTER JOIN mas_department d ON d.department_id = m.department_id

where   m.inpatient_id is not null and m.prescription_date  between $P{fromDate} and  $P{toDate} 
and t.rc_qty>0
$P!{query1}
group by    mas_store_item.pvms_no ,     mas_store_item.nomenclature ,d.department_id,
d.department_name, t.rc_qty
order by d.department_name, mas_store_item.nomenclature
]]></queryString>

