	<queryString><![CDATA[SELECT
     Concat(patient.`p_first_name`,patient.`p_middle_name`,patient.`p_last_name`)AS patient_name,
     dg_sample_collection_details.`collected_by` AS dg_sample_collection_details_collected_by,
     dg_collection_center.`collection_center_name` AS dg_collection_center_collection_center_name,
     patient.`service_no` AS patient_service_no,
     concat(patient.`p_first_name`,patient.`p_middle_name`,patient.`p_last_name`)AS patient_name,
     mas_relation.`relation_name` AS mas_relation_relation_name,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_charge_code.`charge_code_name` AS mas_charge_code_charge_code_name,
     dg_mas_collection.`collection_name` AS dg_mas_collection_collection_name,
     dg_mas_investigation.`quantity` AS dg_mas_investigation_quantity,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `dg_sample_collection_header` dg_sample_collection_header LEFT OUTER JOIN `dg_sample_collection_details` dg_sample_collection_details ON dg_sample_collection_header.`sample_collection_header_id` = dg_sample_collection_details.`sample_collection_header_id`
     LEFT OUTER JOIN `dg_collection_center` dg_collection_center ON dg_sample_collection_details.`collection_center_id` = dg_collection_center.`collection_center_id`
     LEFT OUTER JOIN `mas_charge_code` mas_charge_code ON dg_sample_collection_details.`charge_code_id` = mas_charge_code.`charge_code_id`
     LEFT OUTER JOIN `dg_mas_collection` dg_mas_collection ON dg_sample_collection_details.`container` = dg_mas_collection.`collection_id`
     INNER JOIN `dg_mas_investigation` dg_mas_investigation ON dg_sample_collection_details.`investigation_id` = dg_mas_investigation.`investigation_id`
     AND dg_mas_collection.`collection_id` = dg_mas_investigation.`collection_id`
     AND mas_charge_code.`charge_code_id` = dg_mas_investigation.`charge_code_id`
     LEFT OUTER JOIN `patient` patient ON dg_sample_collection_header.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON dg_sample_collection_header.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `mas_relation` mas_relation ON patient.`relation_id` = mas_relation.`relation_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON patient.`rank_id` = mas_rank.`rank_id`


$P!{qry}]]></queryString>

