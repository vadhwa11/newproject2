	<queryString><![CDATA[SELECT
     mas_manufacturer.manufacturer_code AS mas_manufacturer_manufacturer_code,
     mas_manufacturer.manufacturer_name AS mas_manufacturer_manufacturer_name,
     mas_manufacturer.address1+mas_manufacturer.address2 AS mas_manufacturer_address,
     mas_manufacturer.phoneno AS mas_manufacturer_phoneno,
     mas_manufacturer.mobileno AS mas_manufacturer_mobileno,
     mas_manufacturer.email_id AS mas_manufacturer_email_id,
     mas_manufacturer.fax_number AS mas_manufacturer_fax_number,
     mas_manufacturer.pin_code AS mas_manufacturer_pin_code,
     mas_manufacturer.licence_no AS mas_manufacturer_licence_no,
     mas_manufacturer.sales_tax_no AS mas_manufacturer_sales_tax_no,
     mas_state.state_name AS mas_state_state_name,
     mas_district.district_name AS mas_district_district_name
FROM
 mas_manufacturer mas_manufacturer 
   left outer JOIN   mas_district mas_district  ON mas_district.district_id = mas_manufacturer.city_id
     left outer  JOIN mas_state mas_state ON mas_manufacturer.state_id = mas_state.state_id

$P!{qry}
ORDER BY
     mas_manufacturer.manufacturer_name ASC]]></queryString>

