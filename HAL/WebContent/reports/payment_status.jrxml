	<queryString><![CDATA[select 
   b.avi_ca34_id,
   b.licence_no,
(b.first_name|| ' ' ||b.last_name)as name,
b.dd_no, 
b.dd_no_date,
b.amount,
b.file_no,
bank.bank_name,
hosp.hospital_name
FROM
avi_ca34 b
left outer join mas_bank_master bank on b.bank_id=bank.bank_id

left outer join mas_hospital hosp ON b.hospital_id =hosp.hospital_id
where b.last_chg_date between $P{fromDate} and $P{toDate}
and b.hospital_id=$P{hospitalId} $P!{qry}
order by b.dd_no_date]]></queryString>

