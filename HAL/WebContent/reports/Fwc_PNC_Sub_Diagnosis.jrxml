	<queryString><![CDATA[select
Initial_Diagnosis, system_diagnosis_name, icd_name
from opd_patient_details opd
left outer join visit v on opd.visit_id=v.visit_id
left outer join patient p on v.hin_id=p.hin_id
left outer join mas_system_diagnosis msd on opd.system_diagnosis_id=msd.system_diagnosis_id
left outer join discharge_icd_code dic on opd.id=dic.opd_patient_details_id
left outer join mas_icd mi on dic.icd_id=mi.icd_id

WHERE
    v.visit_id = $P{visitId} and p.service_no=$P{serviceNo}]]></queryString>

