     `annual_medical_examination` annual_medical_examination RIGHT OUTER JOIN `ame_lmc_header` ame_lmc_header ON annual_medical_examination.`ame_id` = ame_lmc_header.`ame_id`
     LEFT OUTER JOIN `emp_afmsf_det` emp_afmsf_det ON annual_medical_examination.`emp_afmsf_det_id` = emp_afmsf_det.`empafmsfdet_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON emp_afmsf_det.`unit_id` = mas_unit.`unit_id`
     LEFT OUTER JOIN `mas_rank` mas_rank ON emp_afmsf_det.`rank_id` = mas_rank.`rank_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON emp_afmsf_det.`hospital_id` = mas_hospital.`hospital_id`
