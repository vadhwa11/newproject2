	<queryString><![CDATA[SELECT
     opd_gastro_enterology_colonoscopy.`report_date` AS opd_gastro_enterology_colonoscopy_report_date,
     opd_gastro_enterology_colonoscopy.`anal_canal` AS opd_gastro_enterology_colonoscopy_anal_canal,
     opd_gastro_enterology_colonoscopy.`rectum` AS opd_gastro_enterology_colonoscopy_rectum,
     opd_gastro_enterology_colonoscopy.`sigmoid` AS opd_gastro_enterology_colonoscopy_sigmoid,
     opd_gastro_enterology_colonoscopy.`descending_colon` AS opd_gastro_enterology_colonoscopy_descending_colon,
     opd_gastro_enterology_colonoscopy.`transverse_colon` AS opd_gastro_enterology_colonoscopy_transverse_colon,
     opd_gastro_enterology_colonoscopy.`cecum` AS opd_gastro_enterology_colonoscopy_cecum,
     opd_gastro_enterology_colonoscopy.`biopsy` AS opd_gastro_enterology_colonoscopy_biopsy,
     opd_gastro_enterology_colonoscopy.`reportNo` AS opd_gastro_enterology_colonoscopy_reportNo,
     visit.`age` AS visit_age,
     patient.`service_no` AS patient_service_no,
     mas_rank.`rank_name` AS mas_rank_rank_name,
     mas_diagnosis_conclusion.`diagnosis_conclusion_name` AS mas_diagnosis_conclusion_diagnosis_conclusion_name,
     concat(mas_employee.`first_name`,mas_employee.`last_name`,mas_employee.`middle_name`)As mas_employee_name,
     concat(patient.`p_first_name`,patient.`p_middle_name`,patient.`p_last_name`)AS patient_name,
     mas_unit.`unit_name` AS mas_unit_unit_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name
FROM
     `visit` visit RIGHT OUTER JOIN `opd_gastro_enterology_colonoscopy` opd_gastro_enterology_colonoscopy ON visit.`visit_id` = opd_gastro_enterology_colonoscopy.`visit_id`
     LEFT OUTER JOIN `patient` patient ON visit.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `mas_diagnosis_conclusion` mas_diagnosis_conclusion ON visit.`diagnosis_id` = mas_diagnosis_conclusion.`diagnosis_conclusion_id`
     LEFT OUTER JOIN `mas_unit` mas_unit ON patient.`unit_id` = mas_unit.`unit_id`
     INNER JOIN `mas_hospital` mas_hospital ON patient.`hospital_id` = mas_hospital.`hospital_id`
     AND mas_hospital.`hospital_id` = opd_gastro_enterology_colonoscopy.`hospital_id`
     INNER JOIN `mas_employee` mas_employee ON mas_hospital.`hospital_id` = mas_employee.`hospital_id`
     AND opd_gastro_enterology_colonoscopy.`referred_by` = mas_employee.`employee_id`
     INNER JOIN `mas_rank` mas_rank ON mas_employee.`rank_id` = mas_rank.`rank_id`

WHERE
     visit.`visit_no` = $P{visitNo}
     And patient.`service_no` = $P{serviceNo}
     And patient.`hin_no` = $P{hinNo}]]></queryString>

