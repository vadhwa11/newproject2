	<queryString><![CDATA[select mas_unit.`unit_name` as unit_name,
mas_department.`department_name` as department_name ,
mas_employee.`tel_no_office` as tel_on_office ,
mas_employee.`service_no` as service_no ,
mas_rank.`rank_name` as rank_name ,
mas_trade.`trade_name` as trade_name ,
hrorderly_leave_application.`leave_total_days` as leave_total_days ,
hr_leave_type_master.`details` as leave_type ,
hr_leave_type_master.`days` as days ,
hrorderly_leave_application.`leave_from_date` as from_date ,
hrorderly_leave_application.`date_of_reporting` as to_date ,
hrorderly_leave_application.`addl_leave_total_days` as addl_leave_total_days ,
hrorderly_leave_application.`addl_leave_from_date` as addl_leave_from_date ,
hrorderly_leave_application.`addl_leave_to_date` as addl_leave_to_date ,
hrorderly_leave_application.`full_name` as full_name ,
hrorderly_leave_application.`house_name` as house_name ,
hrorderly_leave_application.`village` as village ,
hrorderly_leave_application.`po` as po ,
hrorderly_leave_application.`telegraph_office` as telegraph_office ,
hrorderly_leave_application.`police_station` as police_station ,
hrorderly_leave_application.`pin` as pin ,
hrorderly_leave_application.`telephone_no` as telephone_no ,
mas_state.`state_name` as state_name ,
mas_district2.`district_name` as RS_upto ,
hrorderly_leave_application.`other_trains_upto` as other_trains_upto ,
case when hrorderly_leave_application.`drawing_transport_allowance`='y' then "YES"
     when hrorderly_leave_application.`drawing_transport_allowance`='n' then "NO"
     else "YES/NO" end  as drawing_transport ,
case when hrorderly_leave_application.`availing_ltc`='y' then "YES"
     when hrorderly_leave_application.`availing_ltc`='n' then "NO"
     else "YES/NO" end  as availing_ltc ,
mas_district1.`district_name` as city_to ,
mas_district.`district_name` as district_name ,
case when hrorderly_leave_application.`type`="f" then "FRW"
     when hrorderly_leave_application.`type`="c" then "CV"
     else "FRW/CV" end as application_type ,
(select leave_availed from hrorderly_leave_account where leave_type_id = hrorderly_leave_application.`leave_id` and employee_id=hrorderly_leave_application.`employee_id`) as leave_availed ,

concat(ifnull(mas_title.`title_name`,"")," ",ifnull(mas_employee.`first_name`,"")," ",ifnull(mas_employee.`middle_name`,"")," ",ifnull(mas_employee.`last_name`,"")) as employee_name ,
ifnull( (select hla.`leave_availed` from hrorderly_leave_account hla left outer join hr_leave_type_master hltm on hla.`leave_type_id`=hltm.`id`
 where hla.`employee_id`=mas_employee.`employee_id` and hltm.`leave_type`='AL')," ") as al_leave ,
ifnull( (select hla.`leave_availed` from hrorderly_leave_account hla left outer join hr_leave_type_master hltm on hla.`leave_type_id`=hltm.`id`
 where hla.`employee_id`=mas_employee.`employee_id` and hltm.`leave_type`='SL')," ") as sl_leave ,
ifnull( (select hla.`leave_availed` from hrorderly_leave_account hla left outer join hr_leave_type_master hltm on hla.`leave_type_id`=hltm.`id`
 where hla.`employee_id`=mas_employee.`employee_id` and hltm.`leave_type`='CL')," ") as cl_leave ,
mas_district3.`district_name` as required_from
from hrorderly_leave_application `hrorderly_leave_application` left outer join
mas_employee `mas_employee` on hrorderly_leave_application.`employee_id`=mas_employee.`employee_id`
left outer join mas_department `mas_department` on mas_employee.`department_id`=mas_department.`department_id`
left outer join mas_rank `mas_rank` on mas_employee.`rank_id`=mas_rank.`rank_id`
left outer join mas_unit `mas_unit` on mas_unit.`unit_id`=mas_employee.`unit_id`
left outer join mas_trade `mas_trade` on mas_employee.`trade_id`=mas_trade.`trade_id`
left outer join hr_leave_type_master `hr_leave_type_master` on hrorderly_leave_application.`leave_id`=hr_leave_type_master.`id`
left outer join mas_district `mas_district` on mas_district.`district_id` = hrorderly_leave_application.`district_name`
left outer join mas_state `mas_state` on mas_state.`state_id` = hrorderly_leave_application.`state_id`
left outer join mas_title `mas_title` on mas_title.`title_id`=mas_employee.`title_id`
left outer join mas_district `mas_district1` on hrorderly_leave_application.`city_to`=mas_district1.`district_id`
left outer join mas_district `mas_district2` on hrorderly_leave_application.`rajdhani_upto`=mas_district2.`district_id`
left outer join mas_district `mas_district3` on hrorderly_leave_application.`required_from`=mas_district3.`district_id`
where hrorderly_leave_application.`employee_id`=$P{employee_id}
order by hrorderly_leave_application.`leave_application_id` desc limit 0,1]]></queryString>

