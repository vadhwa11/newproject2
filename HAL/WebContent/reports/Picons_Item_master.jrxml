	<queryString><![CDATA[SELECT
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
     mas_store_item_conversion.item_unit_name AS conversion_item_unit_name,
     mas_store_item.self_life AS self_life,
     mas_store_section.section_name AS section_section_name,
     mas_store_item.cost_price AS mas_store_item_cost_price,
     mas_store_item.remarks as remarks,
     mas_store_item.source_of_supply as source_of_supply,
     mas_store_item.specification as specification,
    mas_store_unit.unit_name as unit_name,
     mas_store_item.min_stock AS mas_store_item_min_stock,
   mas_item_type.item_type_name AS mas_item_type_item_type_name
    
FROM
     mas_store_item_conversion mas_store_item_conversion RIGHT OUTER JOIN mas_store_item mas_store_item ON mas_store_item_conversion.item_conversion_id = mas_store_item.item_conversion_id
     RIGHT OUTER JOIN mas_store_unit mas_store_unit ON mas_store_item_conversion.purchase_unit_id = mas_store_unit.unit_id
     INNER JOIN mas_store_section mas_store_section ON mas_store_item.section_id = mas_store_section.section_id
   LEFT OUTER JOIN mas_item_type mas_item_type ON mas_store_item.item_type_id = mas_item_type.item_type_id
 
where 
 mas_store_item.status='y'
and mas_item_type.item_type_name='PVMS']]></queryString>

