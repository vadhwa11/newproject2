	<queryString><![CDATA[select
     t.reason_for_sentence, t.qty, t.part_no,
     m.condemnation_no, i.nomenclature, i.pvms_no,
     wm.work_order_date, wm.work_order_no, t.serial_no,
     m.condemnation_date, h.hospital_name,
     d.department_name
from
     store_condemnation_m m 
     
     left outer join store_condemnation_t t on m.id = t.condem_m_id
     left outer join mas_store_item i on t.item_id = i.item_id
     left outer join mas_department d on m.department_id = d.department_id
     left outer join store_work_order_m wm on m.work_order_id = wm.id
     left outer join mas_hospital h on m.hospital_id = h.hospital_id

where m.condemnation_no = $P{Condemnation_No} 
and  m.department_id   = $P{Dept_Id}]]></queryString>

