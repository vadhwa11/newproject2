	<queryString><![CDATA[SELECT
     dg_mas_investigation.`investigation_name` AS dg_mas_investigation_investigation_name,
     dg_sub_mas_investigation.`sub_investigation_name` AS dg_sub_mas_investigation_sub_investigation_name,
     mas_main_chargecode.`main_chargecode_name` AS mas_main_chargecode_main_chargecode_name,
     mas_sub_chargecode.`sub_chargecode_name` AS mas_sub_chargecode_sub_chargecode_name,
     CONCAT(dg_result_entry_detail.`result`,"   ",dg_uom.`uom_name`)AS result,
     CONCAT(dg_normal_value.`min_normal_value`," - ",dg_normal_value.`max_normal_value`) AS ranges,
     dg_normal_value.`normalValue` AS dg_normal_value_normalValue,
     dg_fixed_value.`fixed_value` AS dg_fixed_value_fixed_value
FROM
     `dg_orderhd` dg_orderhd LEFT OUTER JOIN `dg_sample_collection_header` dg_sample_collection_header ON dg_orderhd.`orderhd_id` = dg_sample_collection_header.`order_id`
     LEFT OUTER JOIN `dg_sample_collection_details` dg_sample_collection_details ON dg_sample_collection_header.`sample_collection_header_id` = dg_sample_collection_details.`sample_collection_header_id`
     LEFT OUTER JOIN `dg_result_entry_header` dg_result_entry_header ON dg_sample_collection_header.`sample_collection_header_id` = dg_result_entry_header.`sample_collection_header_id`
     LEFT OUTER JOIN `dg_result_entry_detail` dg_result_entry_detail ON dg_result_entry_header.`result_entry_id` = dg_result_entry_detail.`result_entry_id`
     LEFT OUTER JOIN `dg_normal_value` dg_normal_value ON dg_result_entry_detail.`normal_id` = dg_normal_value.`normal_id`
     LEFT OUTER JOIN `dg_mas_investigation` dg_mas_investigation ON dg_sample_collection_details.`investigation_id` = dg_mas_investigation.`investigation_id`
     LEFT OUTER JOIN `dg_sub_mas_investigation` dg_sub_mas_investigation ON dg_mas_investigation.`investigation_id` = dg_sub_mas_investigation.`investigation_id`
     LEFT OUTER JOIN `mas_main_chargecode` mas_main_chargecode ON dg_sub_mas_investigation.`main_chargecode_id` = mas_main_chargecode.`main_chargecode_id`
     LEFT OUTER JOIN `mas_sub_chargecode` mas_sub_chargecode ON dg_sub_mas_investigation.`sub_chargecode_id` = mas_sub_chargecode.`sub_chargecode_id`
     LEFT OUTER JOIN `dg_uom` dg_uom ON dg_sub_mas_investigation.`uom_id` = dg_uom.`uom_id`
     LEFT OUTER JOIN `dg_fixed_value` dg_fixed_value ON dg_sub_mas_investigation.`sub_investigation_id` = dg_fixed_value.`sub_investigation_id`
WHERE
     dg_orderhd.`order_no` = $P{QUERY}
     and `dg_result_entry_header`.`result_type` IS null
     and `dg_result_entry_detail`.`result_type` = 'm'
     and `dg_result_entry_detail`.`result_detail_status` = 'a' ]]></queryString>

