	<queryString><![CDATA[select ueg.emp_group_id,ueg.user_emp_id,eg.emp_group_code,us.user_name,us.login_name,eg.emp_group_name,concat(mah.hospital_name,' ',mah.address) as hospital_name FROM user_emp_group as ueg
left outer join emp_groups as eg
on ueg.emp_group_id = eg.id
left outer join users as us
on ueg.user_id = us.user_id
