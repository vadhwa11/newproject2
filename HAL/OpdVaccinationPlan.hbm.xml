<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OpdVaccinationPlan"
		table="opd_vaccination_plan"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="opd_vaccination_plan"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="VisitId"
			column="visit_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="HinId"
			column="hin_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="DepartmentId"
			column="department_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="VacinationType"
			column="vacination_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Dob"
			column="dob"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="VaccinDate"
			column="vaccin_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="VaccinCompleteDate"
			column="vaccin_complete_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="VaccinePlace"
			column="vaccine_place"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="VaccinToDate"
			column="vaccin_to_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Vaccin"
			column="vaccin_id"
			class="OpdVaccinMst"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="SubCentre"
			column="sub_centre_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>